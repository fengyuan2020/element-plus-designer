{
  "__id__": "color-picker",
  "name": "颜色选择",
  "tag": "el-color-picker",
  "tagIcon": "color",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.gitee.io/zh-CN/component/color-picker.html",
  "attributes": {
    "vModel": "modelValue",
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "size": {
      "label": "尺寸",
      "title": "尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default"
    },
    "show-alpha": {
      "label": "透明度选择",
      "title": "是否支持透明度选择",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "color-format": {
      "label": "颜色的格式",
      "title": "写入 v-model 的颜色的格式",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "hsl",
              "value": "hsl"
            },
            {
              "label": "hsv",
              "value": "hsv"
            },
            {
              "label": "hex",
              "value": "hex"
            },
            {
              "label": "rgb",
              "value": "rgb"
            }
          ]
        }
      ],
      "default": "hex"
    },
    "popper-class": {
      "label": "下拉框的类名",
      "title": "el-color-picker 下拉框的类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "predefine": {
      "label": "预设颜色",
      "title": "预设颜色",
      "types": [
        {
          "name": "array",
          "tag": "array-editor",
          "props": {
            "tag": "el-color-picker"
          }
        }
      ],
      "default": []
    },
    "validate-event": {
      "label": "表单校验",
      "title": "输入时是否触发表单的校验",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "tabindex": {
      "label": "",
      "title": "ColorPicker 的 tabindex",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ],
      "default": 0
    },
    "label": {
      "title": "ColorPicker 的 aria-label",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true
    },
    "id": {
      "label": "",
      "title": "ColorPicker 的 id",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    }
  },
  "events": [
    {
      "name": "change",
      "title": "当绑定值变化时触发",
      "type": "Function",
      "fnDes": "(value: string) => void"
    },
    {
      "name": "active-change",
      "title": "面板中当前显示的颜色发生改变时触发",
      "type": "Function",
      "fnDes": "(value: string) => void"
    },
    {
      "name": "focus",
      "title": "当获得焦点时触发",
      "type": "Function",
      "version": "2.4.0",
      "fnDes": "(event: FocusEvent) => void"
    },
    {
      "name": "blur",
      "title": "当失去焦点时触发",
      "type": "Function",
      "version": "2.4.0",
      "fnDes": "(event: FocusEvent) => void"
    }
  ],
  "exposes": [
    {
      "name": "color",
      "title": "当前色彩对象",
      "type": "^[object]`Color`"
    },
    {
      "name": "show",
      "title": "手动显示颜色选择器",
      "type": "Function",
      "version": "2.3.3",
      "fnDes": "() => void"
    },
    {
      "name": "hide",
      "title": "手动隐藏颜色选择器",
      "type": "Function",
      "version": "2.3.3",
      "fnDes": "() => void"
    },
    {
      "name": "focus",
      "title": "使 picker 获得焦点",
      "type": "Function",
      "version": "2.3.13",
      "fnDes": "() => void"
    },
    {
      "name": "blur",
      "title": "使 picker 失去焦点",
      "type": "Function",
      "version": "2.3.13",
      "fnDes": "() => void"
    }
  ],
  "slots": []
}