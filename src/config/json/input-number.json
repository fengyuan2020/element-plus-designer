{
  "__id__": "input-number",
  "name": "计数器",
  "tag": "el-input-number",
  "tagIcon": "number",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.gitee.io/zh-CN/component/input-number.html",
  "attributes": {
    "vModel": "modelValue",
    "placeholder": "请输入",
    "min": {
      "label": "最小值",
      "title": "设置计数器允许的最小值",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": "-Infinity"
    },
    "max": {
      "label": "最大值",
      "title": "设置计数器允许的最大值",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": "Infinity"
    },
    "step": {
      "label": "步长",
      "title": "计数器步长",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 1
    },
    "step-strictly": {
      "label": "严格步数",
      "title": "是否只能输入 step 的倍数",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "precision": {
      "label": "精度",
      "title": "数值精度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number",
          "props": {
            "min": 0
          }
        }
      ]
    },
    "size": {
      "label": "精度",
      "title": "计数器尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default"
    },
    "readonly": {
      "label": "只读",
      "title": "原生  readonly 属性，是否只读",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "version": "2.2.16"
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用状态",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "controls": {
      "label": "使用控制按钮",
      "title": "是否使用控制按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "controls-position": {
      "label": "按钮位置",
      "title": "控制按钮位置",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "默认",
              "value": ""
            },
            {
              "label": "右边",
              "value": "right"
            }
          ]
        }
      ]
    },
    "name": {
      "label": "",
      "title": "等价于原生 input name 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "hide": true
    },
    "label": {
      "label": "",
      "title": "输入框关联的 label 文字",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "hide": true
    },
    "id": {
      "label": "",
      "title": "等价于原生 input `id` 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "value-on-clear": {
      "label": "清空时显示的值",
      "title": "当输入框被清空时显示的值 (> 2.2.0)",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "未设置",
              "value": null
            },
            {
              "label": "最小值",
              "value": "min"
            },
            {
              "label": "最大值",
              "value": "max"
            }
          ]
        },
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "version": "2.2.0"
    },
    "validate-event": {
      "label": "触发表单校验",
      "title": "输入时是否触发表单的校验",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    }
  },
  "events": [
    {
      "name": "change",
      "title": "绑定值被改变时触发",
      "type": "Function",
      "fnDes": "(currentValue: number | undefined, oldValue: number | undefined) => void"
    },
    {
      "name": "blur",
      "title": "在组件 Input 失去焦点时触发",
      "type": "Function",
      "fnDes": "(event: FocusEvent) => void"
    },
    {
      "name": "focus",
      "title": "在组件 Input 获得焦点时触发",
      "type": "Function",
      "fnDes": "(event: FocusEvent) => void"
    }
  ],
  "exposes": [
    {
      "name": "focus",
      "title": "使 input 组件获得焦点",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "blur",
      "title": "使 input 组件失去焦点",
      "type": "Function",
      "fnDes": "() => void"
    }
  ],
  "slots": []
}