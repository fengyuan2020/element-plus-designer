{
  "__id__": "date-time-picker",
  "name": "日期时间选择",
  "tag": "el-date-picker",
  "tagIcon": "date",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.gitee.io/zh-CN/component/date-picker.html",
  "attributes": {
    "readonly": {
      "label": "",
      "title": "只读",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "disabled": {
      "label": "",
      "title": "禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "editable": {
      "label": "",
      "title": "文本框可输入",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "clearable": {
      "label": "",
      "title": "是否显示清除按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "size": {
      "label": "",
      "title": "输入框尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default"
    },
    "start-placeholder": {
      "label": "",
      "title": "范围选择时开始日期的占位内容",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "end-placeholder": {
      "label": "",
      "title": "范围选择时结束日期的占位内容",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "arrow-control": {
      "label": "",
      "title": "whether to pick time using arrow buttons",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "type": {
      "label": "",
      "title": "显示类型",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "year",
              "value": "year"
            },
            {
              "label": "month",
              "value": "month"
            },
            {
              "label": "date",
              "value": "date"
            },
            {
              "label": "datetime",
              "value": "datetime"
            },
            {
              "label": "week",
              "value": "week"
            },
            {
              "label": "datetimerange",
              "value": "datetimerange"
            },
            {
              "label": "daterange",
              "value": "daterange"
            }
          ]
        }
      ],
      "default": "date"
    },
    "format": {
      "label": "",
      "title": "显示在输入框中的格式",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "see [date formats](",
              "value": "see [date formats]("
            },
            {
              "label": "en-US",
              "value": "en-US"
            },
            {
              "label": "component",
              "value": "component"
            },
            {
              "label": "date-picker#date-formats)",
              "value": "date-picker#date-formats)"
            }
          ]
        }
      ],
      "default": "YYYY-MM-DD HH:mm:ss"
    },
    "popper-class": {
      "label": "",
      "title": "DateTimePicker 下拉框的类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "range-separator": {
      "label": "",
      "title": "选择范围时的分隔符",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "-"
    },
    "default-value": {
      "label": "",
      "title": "可选，选择器打开时默认显示的时间",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "",
              "value": ""
            }
          ]
        },
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "",
              "value": ""
            }
          ]
        }
      ]
    },
    "default-time": {
      "label": "",
      "title": "选择日期后的默认时间值。 如未指定则默认时间值为 `00:00:00`",
      "types": [
        {
          "name": "date",
          "tag": ""
        },
        {
          "name": "[Date, Date]",
          "tag": ""
        }
      ]
    },
    "value-format": {
      "label": "",
      "title": "可选，绑定值的格式。 不指定则绑定值为 Date 对象",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "查看 [日期格式](https:",
              "value": "查看 [日期格式](https:"
            },
            {
              "label": "",
              "value": ""
            },
            {
              "label": "day.js.org",
              "value": "day.js.org"
            },
            {
              "label": "docs",
              "value": "docs"
            },
            {
              "label": "en",
              "value": "en"
            },
            {
              "label": "display",
              "value": "display"
            },
            {
              "label": "format)",
              "value": "format)"
            }
          ]
        }
      ]
    },
    "date-format": {
      "title": "可选，时间选择器下拉列表中显示的日期格式",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "参见 [date formats](https:",
              "value": "参见 [date formats](https:"
            },
            {
              "label": "",
              "value": ""
            },
            {
              "label": "day.js.org",
              "value": "day.js.org"
            },
            {
              "label": "docs",
              "value": "docs"
            },
            {
              "label": "en",
              "value": "en"
            },
            {
              "label": "display",
              "value": "display"
            },
            {
              "label": "format)",
              "value": "format)"
            }
          ]
        }
      ],
      "version": "2.4.0"
    },
    "time-format": {
      "title": "可选，时间选择器下拉列表中显示的时间格式",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "参见 [date formats](https:",
              "value": "参见 [date formats](https:"
            },
            {
              "label": "",
              "value": ""
            },
            {
              "label": "day.js.org",
              "value": "day.js.org"
            },
            {
              "label": "docs",
              "value": "docs"
            },
            {
              "label": "en",
              "value": "en"
            },
            {
              "label": "display",
              "value": "display"
            },
            {
              "label": "format)",
              "value": "format)"
            }
          ]
        }
      ],
      "version": "2.4.0"
    },
    "id": {
      "label": "",
      "title": "等价于原生 input `id` 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "[string, string]",
          "tag": ""
        }
      ]
    },
    "name": {
      "label": "",
      "title": "等价于原生 input `name` 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "unlink-panels": {
      "label": "",
      "title": "在范围选择器里取消两个日期面板之间的联动",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "prefix-icon": {
      "label": "",
      "title": "自定义前缀图标",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": "Date"
    },
    "clear-icon": {
      "label": "",
      "title": "自定义清除图标",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": "CircleClose"
    },
    "shortcuts": {
      "label": "",
      "title": "设置快捷选项，需要传入数组对象",
      "types": [
        {
          "name": "object[{ text: string, value: date",
          "tag": ""
        },
        {
          "name": "function }]",
          "tag": ""
        }
      ]
    },
    "disabled-date": {
      "label": "",
      "title": "一个用来判断该日期是否被禁用的函数，接受一个 Date 对象作为参数。 应该返回一个 Boolean 值。",
      "types": [
        {
          "name": "Function(Date)",
          "tag": ""
        }
      ]
    },
    "cell-class-name": {
      "label": "",
      "title": "设置自定义类名",
      "types": [
        {
          "name": "Function(Date)",
          "tag": ""
        }
      ]
    },
    "teleported": {
      "label": "",
      "title": "是否将 datetime-picker 的下拉列表插入至 body 元素",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    }
  },
  "events": [
    {
      "name": "change",
      "title": "用户确认选定的值时触发",
      "params": "value"
    },
    {
      "name": "blur",
      "title": "在组件 Input 失去焦点时触发",
      "params": "(e: FocusEvent)"
    },
    {
      "name": "focus",
      "title": "在组件 Input 获得焦点时触发",
      "params": "(e: FocusEvent)"
    },
    {
      "name": "calendar-change",
      "title": "如果用户没有选择日期，那默认展示当前日的月份。 选中日历日期后会执行的回调，只有当 `datetimerange` 才生效",
      "params": "[Date, Date]"
    },
    {
      "name": "visible-change",
      "title": "当 DateTimePicker 的下拉列表出现/消失时触发",
      "params": "出现时为true，隐藏时为false"
    }
  ],
  "slots": [
    {
      "name": "default",
      "title": "自定义单元格内容"
    },
    {
      "name": "range-separator",
      "title": "自定义范围分割符内容"
    }
  ],
  "exposes": [
    {
      "name": "focus",
      "title": "使 input 获取焦点",
      "params": "—"
    }
  ],
  "desc": "在同一个选择器里选择日期和时间"
}