{
  "__id__": "tour",
  "name": "漫游式引导",
  "tag": "el-tour",
  "tagIcon": "tour",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.gitee.io/zh-CN/component/tour.html",
  "childTag": [
    "el-tour-step"
  ],
  "attributes": {
    "vModel": "modelValue",
    "show-arrow": {
      "label": "显示箭头",
      "title": "是否显示箭头",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "placement": {
      "label": "引导卡片位置",
      "title": "引导卡片相对于目标元素的位置",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "top",
              "value": "top"
            },
            {
              "label": "top-start",
              "value": "top-start"
            },
            {
              "label": "top-end",
              "value": "top-end"
            },
            {
              "label": "bottom",
              "value": "bottom"
            },
            {
              "label": "bottom-start",
              "value": "bottom-start"
            },
            {
              "label": "bottom-end",
              "value": "bottom-end"
            },
            {
              "label": "left",
              "value": "left"
            },
            {
              "label": "left-start",
              "value": "left-start"
            },
            {
              "label": "left-end",
              "value": "left-end"
            },
            {
              "label": "right",
              "value": "right"
            },
            {
              "label": "right-start",
              "value": "right-start"
            },
            {
              "label": "right-end",
              "value": "right-end"
            }
          ]
        }
      ],
      "default": "bottom"
    },
    "content-style": {
      "label": "内容样式",
      "title": "为content自定义样式",
      "types": [
        {
          "name": "object",
          "tag": "style-editor"
        }
      ],
      "default": {}
    },
    "mask": {
      "label": "启用蒙层",
      "title": "是否启用蒙层，也可传入配置改变蒙层样式和填充色",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "type": {
      "label": "类型",
      "title": "类型，影响底色与文字颜色",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "primary",
              "value": "primary"
            }
          ]
        }
      ],
      "default": "default"
    },
    "current": {
      "label": "",
      "title": "当前值",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "scroll-into-view-options": {
      "label": "支持当前元素滚动到视窗内",
      "title": "是否支持当前元素滚动到视窗内，也可传入配置指定滚动视窗的相关参数",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        },
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {
        "block": "center"
      }
    },
    "z-index": {
      "label": "",
      "title": "Tour 的层级",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 2001
    },
    "show-close": {
      "label": "显示关闭按钮",
      "title": "是否显示关闭按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "close-icon": {
      "label": "关闭图标",
      "title": "自定义关闭图标，默认Close",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": "Close"
    },
    "close-on-press-escape": {
      "label": "ESC 关闭引导",
      "title": "是否可以通过按下 ESC 关闭引导",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "target-area-clickable": {
      "label": "",
      "title": "启用蒙层时，target 元素区域是否可以点击。",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "tourStep 组件列表"
    },
    {
      "name": "indicators",
      "title": "自定义指示器, scope 参数是 `{ current, total }`"
    }
  ],
  "events": [
    {
      "name": "close",
      "title": "关闭引导时的回调函数",
      "type": "Function",
      "fnDes": "(current: number) => void"
    },
    {
      "name": "finish",
      "title": "引导完成时的回调",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "change",
      "title": "步骤改变时的回调",
      "type": "Function",
      "fnDes": "(current: number) => void"
    }
  ],
  "desc": "用于分步引导用户了解产品功能的气泡组件。 用来引导用户并介绍产品"
}