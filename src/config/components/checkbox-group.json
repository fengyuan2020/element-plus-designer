{
  "__id__": "checkbox-group",
  "name": "多选框组",
  "desc": "在一组备选项中进行多选。",
  "tag": "el-checkbox-group",
  "tagIcon": "checkbox",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/checkbox.html",
  "attributes": {
    "vModel": "modelValue",
    "size": {
      "label": "尺寸",
      "title": "多选框组尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ]
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "min": {
      "label": "勾选最小数量",
      "title": "可被勾选的 checkbox 的最小数量",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "max": {
      "label": "勾选最大数量",
      "title": "可被勾选的 checkbox 的最大数量",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "aria-label": {
      "label": "原生 aria-label属性",
      "title": "原生 <code>aria-label</code>属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true,
      "version": "2.7.2"
    },
    "text-color": {
      "label": "按钮活跃颜色",
      "title": "当按钮为活跃状态时的字体颜色",
      "types": [
        {
          "name": "string",
          "tag": "el-color-picker"
        }
      ],
      "default": "#ffffff"
    },
    "fill": {
      "label": "按钮活跃边框背景颜色",
      "title": "当按钮为活跃状态时的边框和背景颜色",
      "types": [
        {
          "name": "string",
          "tag": "el-color-picker"
        }
      ],
      "default": "#409eff"
    },
    "tag": {
      "label": "复选框组元素标签",
      "title": "复选框组元素标签",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "div"
    },
    "validate-event": {
      "label": "表单校验",
      "title": "是否触发表单验证",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "label": {
      "label": "原生 aria-label属性",
      "title": "原生 <code>aria-label</code>属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "a11y": true,
      "deprecated": true
    },
    "options": {
      "label": "选项的数据源",
      "title": "选项的数据源， <code>value</code> 的 key 和 <code>label</code> 和 <code> disabled</code>可以通过 <code>props</code>自定义.",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "version": "2.11.2"
    },
    "props": {
      "label": "options 的配置",
      "title": "options 的配置",
      "types": [
        {
          "name": "object",
          "tag": "fixed-keys-object-editor",
          "props": {
            "keys": [
              {
                "name": "label",
                "title": "标签",
                "tag": "el-input"
              },
              {
                "name": "value",
                "title": "值",
                "tag": "el-input"
              },
              {
                "name": "disabled",
                "title": "禁用",
                "tag": "el-input"
              }
            ]
          }
        }
      ],
      "default": {
        "value": "value",
        "label": "label",
        "disabled": "disabled"
      },
      "version": "2.11.2"
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义默认内容",
      "subtags": [
        "checkbox",
        "checkbox-button"
      ]
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "当绑定值变化时触发的事件",
      "type": "function",
      "details": "(value: string[] | number[]) => void"
    }
  ],
  "data": {
    "name": "options",
    "label": "选项",
    "source": "static",
    "static": {
      "ref": true,
      "type": {
        "name": "array",
        "tag": "object-array-editor",
        "props": {
          "keys": [
            {
              "key": "value",
              "label": "值",
              "tag": "string-number-input"
            },
            {
              "key": "label",
              "label": "标签",
              "tag": "string-number-input"
            }
          ]
        }
      },
      "default": [
        {
          "label": "选项一",
          "value": "1"
        },
        {
          "label": "选项二",
          "value": "2"
        }
      ]
    },
    "dynamic": {
      "url": "array.json",
      "method": "get",
      "dataKey": "list"
    }
  }
}