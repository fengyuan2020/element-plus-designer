{
  "__id__": "countdown",
  "name": "倒计时",
  "desc": "显示统计数据。",
  "tag": "el-countdown",
  "tagIcon": "countdown",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/statistic.html#%E5%80%92%E8%AE%A1%E6%97%B6",
  "attributes": {
    "value": {
      "label": "目标时间",
      "title": "目标时间",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "string",
          "tag": "el-date-picker",
          "props": {
            "type": "datetime",
            "value-format": "YYYY-MM-DD HH:mm:ss"
          }
        }
      ]
    },
    "format": {
      "label": "格式化倒计时",
      "title": "格式化倒计时",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "HH:mm:ss"
    },
    "prefix": {
      "label": "前缀",
      "title": "设置倒计时前缀",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "suffix": {
      "label": "后缀",
      "title": "设置倒计时的后缀",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "title": {
      "label": "标题",
      "title": "倒计时标题",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "value-style": {
      "label": "数字样式",
      "title": "倒计时值的样式",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "object",
          "tag": "style-editor"
        }
      ]
    }
  },
  "slots": [
    {
      "name": "prefix",
      "title": "倒计时值前缀"
    },
    {
      "name": "suffix",
      "title": "倒计时后缀"
    },
    {
      "name": "title",
      "title": "倒计时标题"
    }
  ],
  "events": [
    {
      "title": "时间差改变事件",
      "type": "function",
      "details": "(value: number) => void"
    },
    {
      "title": "倒计时结束事件",
      "type": "function",
      "details": "() => void"
    }
  ],
  "exposes": [
    {
      "name": "displayValue",
      "title": "当前显示值",
      "type": "object",
      "details": "Ref<string>"
    }
  ]
}