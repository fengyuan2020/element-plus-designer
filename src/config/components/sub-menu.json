{
  "__id__": "sub-menu",
  "name": "子菜单",
  "desc": "为网站提供导航功能的菜单。",
  "tag": "el-sub-menu",
  "tagIcon": "sub-menu",
  "layouts": [
    "containerItem"
  ],
  "parentTag": [
    "el-menu",
    "el-sub-menu",
    "el-menu-item-group"
  ],
  "document": "https://element-plus.org/zh-CN/component/menu.html#submenu-attributes",
  "attributes": {
    "index": {
      "label": "唯一标志",
      "title": "唯一标志",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "required": true
    },
    "popper-class": {
      "label": "popper类名",
      "title": "为 popper 添加类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "show-timeout": {
      "label": "展开延时",
      "title": "子菜单出现之前的延迟，(继承 menu 的 <code>show-timeout</code> 配置)",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "hide-timeout": {
      "label": "收起延时",
      "title": "子菜单消失之前的延迟，(继承 menu 的 <code>hide-timeout</code> 配置)",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "teleported": {
      "label": "将弹出菜单挂载到 body 上",
      "title": "是否将弹出菜单挂载到 body 上，第一级SubMenu默认值为 true，其他SubMenus 的值为 false",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ]
    },
    "popper-offset": {
      "label": "弹出窗口偏移",
      "title": "弹出窗口的偏移量 (覆盖 <code>popper</code>的菜单)",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "expand-close-icon": {
      "label": "父展开子关闭图标",
      "title": "父菜单展开且子菜单关闭时的图标， <code>expand-close-icon</code> 和 <code>expand-open-icon</code> 需要一起配置才能生效",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "expand-open-icon": {
      "label": "父展开子打开图标",
      "title": "父菜单展开且子菜单打开时的图标， <code>expand-open-icon</code> 和 <code>expand-close-icon</code> 需要一起配置才能生效",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "collapse-close-icon": {
      "label": "父收起子关闭图标",
      "title": "父菜单收起且子菜单关闭时的图标， <code>collapse-close-icon</code> 和 <code>collapse-open-icon</code> 需要一起配置才能生效",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "collapse-open-icon": {
      "label": "父收起子打开图标",
      "title": "父菜单收起且子菜单打开时的图标， <code>collapse-open-icon</code> 和 <code>collapse-close-icon</code> 需要一起配置才能生效",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义默认内容",
      "subtags": [
        "sub-menu",
        "menu-item",
        "menu-item-group"
      ]
    },
    {
      "name": "title",
      "title": "自定义标题内容"
    }
  ]
}