{
  "__id__": "table-column",
  "name": "表格子项",
  "tag": "el-table-column",
  "tagIcon": "table-column",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/table.html",
  "parentTag": [
    "el-table"
  ],
  "attributes": {
    "type": {
      "label": "列类型",
      "title": "对应列的类型。 如果设置了selection则显示多选框； 如果设置了 index 则显示该行的索引（从 1 开始计算）； 如果设置了 expand 则显示为一个可展开的按钮",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "默认"
            },
            {
              "label": "selection",
              "value": "selection"
            },
            {
              "label": "index",
              "value": "index"
            },
            {
              "label": "expand",
              "value": "expand"
            }
          ]
        }
      ]
    },
    "index": {
      "label": "自定义索引",
      "title": "如果设置了 type=index，可以通过传递 index 属性来自定义索引",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "hide": true
    },
    "label": {
      "label": "列标题",
      "title": "显示的标题",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "column-key": {
      "label": "列Key",
      "title": "column 的 key， column 的 key， 如果需要使用 filter-change 事件，则需要此属性标识是哪个 column 的筛选条件",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "prop": {
      "label": "字段名",
      "title": "字段名称 对应列内容的字段名， 也可以使用 property属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "width": {
      "label": "宽度",
      "title": "对应列的宽度",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "min-width": {
      "label": "最小宽度",
      "title": "对应列的最小宽度， 对应列的最小宽度， 与 width 的区别是 width 是固定的，min-width 会把剩余宽度按比例分配给设置了 min-width 的列",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "fixed": {
      "label": "列固定位置",
      "title": "列是否固定在左侧或者右侧。 true 表示固定在左侧",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "左侧",
              "value": true
            },
            {
              "label": "左侧",
              "value": "left"
            },
            {
              "label": "右侧",
              "value": "right"
            }
          ]
        }
      ]
    },
    "render-header": {
      "label": "",
      "title": "列标题 Label 区域渲染使用的 Function",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "{ column, $index }"
        }
      ],
      "hide": true
    },
    "sortable": {
      "label": "排序",
      "title": "对应列是否可以排序， 如果设置为 'custom'，则代表用户希望远程排序，需要监听 Table 的 sort-change 事件",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        },
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "不排序",
              "value": false
            },
            {
              "label": "custom",
              "value": "custom"
            }
          ]
        }
      ],
      "default": false
    },
    "sort-method": {
      "label": "",
      "title": "指定数据按照哪个属性进行排序，仅当sortable设置为true的时候有效。 应该如同 Array.sort 那样返回一个 Number",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "a, b"
        }
      ],
      "hide": true
    },
    "sort-by": {
      "label": "",
      "title": "指定数据按照哪个属性进行排序，仅当 sortable 设置为 true 且没有设置 sort-method 的时候有效。 如果 sort-by 为数组，则先按照第 1 个属性排序，如果第 1 个相等，再按照第 2 个排序，以此类推",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "row, index"
        },
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "hide": true
    },
    "sort-orders": {
      "label": "",
      "title": "数据在排序时所使用排序策略的轮转顺序，仅当 sortable 为 true 时有效。 需传入一个数组，随着用户点击表头，该列依次按照数组中元素的顺序进行排序",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "数组中的元素需为以下三者之一：ascending 表示升序，descending 表示降序，null 表示还原为原始顺序",
              "value": "数组中的元素需为以下三者之一：ascending 表示升序，descending 表示降序，null 表示还原为原始顺序"
            }
          ]
        }
      ],
      "default": [
        "ascending",
        "descending",
        null
      ],
      "hide": true
    },
    "resizable": {
      "label": "改变宽度",
      "title": "对应列是否可以通过拖动改变宽度（需要在 el-table 上设置 border 属性为真）",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "formatter": {
      "label": "",
      "title": "用来格式化内容",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "row, column, cellValue, index"
        }
      ],
      "hide": true
    },
    "show-overflow-tooltip": {
      "label": "内容过长显示tooltip",
      "title": "当内容过长被隐藏时显示 tooltip",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "align": {
      "label": "对齐方式",
      "title": "对齐方式",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "left",
              "value": "left"
            },
            {
              "label": "center",
              "value": "center"
            },
            {
              "label": "right",
              "value": "right"
            }
          ]
        }
      ],
      "default": "left"
    },
    "header-align": {
      "label": "表头对齐方式",
      "title": "表头对齐方式， 若不设置该项，则使用表格的对齐方式",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "left",
              "value": "left"
            },
            {
              "label": "center",
              "value": "center"
            },
            {
              "label": "right",
              "value": "right"
            }
          ]
        }
      ]
    },
    "class-name": {
      "label": "列类名",
      "title": "列的 className",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "label-class-name": {
      "label": "列标题类名",
      "title": "当前列标题的自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "selectable": {
      "label": "",
      "title": "仅对 type=selection 的列有效，类型为 Function，Function 的返回值用来决定这一行的 CheckBox 是否可以勾选",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "row, index"
        }
      ],
      "hide": true
    },
    "reserve-selection": {
      "label": "反选",
      "title": "仅对  type=selection 的列有效， 请注意， 需指定 row-key 来让这个功能生效。",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "filters": {
      "label": "",
      "title": "数据过滤的选项， 数组格式，数组中的元素需要有 text 和 value 属性。 数组中的每个元素都需要有 text 和 value 属性。",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "hide": true
    },
    "filter-placement": {
      "label": "",
      "title": "过滤弹出框的定位",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "与 Tooltip 的 placement 属性相同",
              "value": "与 Tooltip 的 placement 属性相同"
            }
          ]
        }
      ],
      "hide": true
    },
    "filter-class-name": {
      "title": "过滤弹出框的 className",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "version": "2.5.0"
    },
    "filter-multiple": {
      "label": "",
      "title": "数据过滤的选项是否多选",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "hide": true
    },
    "filter-method": {
      "label": "",
      "title": "数据过滤使用的方法， 如果是多选的筛选项，对每一条数据会执行多次，任意一次返回 true 就会显示。",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "value, row, column"
        }
      ],
      "hide": true
    },
    "filtered-value": {
      "label": "",
      "title": "选中的数据过滤项，如果需要自定义表头过滤的渲染方式，可能会需要此属性。",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "hide": true
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义列的内容",
      "type": "^[object]`{ row: any, column: any, $index: number }`"
    },
    {
      "name": "header",
      "title": "自定义表头的内容，",
      "type": "^[object]`{ column: any, $index: number }`"
    },
    {
      "name": "filter-icon",
      "title": "自定义 filter 图标",
      "type": "^[object]`{ filterOpened: boolean }`",
      "version": "2.7.8"
    }
  ],
  "desc": "用于展示多条结构类似的数据， 可对数据进行排序、筛选、对比或其他自定义操作。"
}