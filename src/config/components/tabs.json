{
  "__id__": "tabs",
  "name": "标签页",
  "desc": "分隔内容上有关联但属于不同类别的数据集合。",
  "tag": "el-tabs",
  "tagIcon": "tabs",
  "layouts": [
    "containerItem"
  ],
  "childTag": [
    "el-tab-pane"
  ],
  "document": "https://element-plus.org/zh-CN/component/tabs.html",
  "attributes": {
    "vModel": "modelValue",
    "type": {
      "label": "风格类型",
      "title": "风格类型",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "card",
              "value": "card"
            },
            {
              "label": "border-card",
              "value": "border-card"
            }
          ]
        }
      ]
    },
    "closable": {
      "label": "标签可关闭",
      "title": "标签是否可关闭",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "addable": {
      "label": "标签可增加",
      "title": "标签是否可增加",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "editable": {
      "label": "标签可增加和关闭",
      "title": "标签是否同时可增加和关闭",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "tab-position": {
      "label": "选项卡位置",
      "title": "选项卡所在位置",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "top",
              "value": "top"
            },
            {
              "label": "right",
              "value": "right"
            },
            {
              "label": "bottom",
              "value": "bottom"
            },
            {
              "label": "left",
              "value": "left"
            }
          ]
        }
      ],
      "default": "top"
    },
    "stretch": {
      "label": "标签宽度自撑开",
      "title": "标签的宽度是否自撑开",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "before-leave": {
      "label": "切换标签之前的钩子函数",
      "title": "切换标签之前的钩子函数， 若返回 <code>false </code> 或者返回被 reject 的 <code>Promise</code>，则阻止切换。",
      "types": [
        {
          "name": "function",
          "details": "(activeName: TabPaneName, oldActiveName: TabPaneName) => Awaitable<void | boolean>"
        }
      ],
      "default": "() => true"
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "默认插槽",
      "subtags": [
        "tab-pane"
      ]
    },
    {
      "name": "add-icon",
      "title": "自定义添加按钮图标",
      "version": "2.5.4"
    },
    {
      "name": "addIcon",
      "title": "自定义添加按钮图标",
      "deprecated": true,
      "version": "2.4.0"
    }
  ],
  "events": [
    {
      "name": "tab-click",
      "title": "tab 被选中时触发",
      "type": "function",
      "details": "(pane: TabsPaneContext, ev: Event) => void"
    },
    {
      "name": "tab-change",
      "title": "<code>activeName</code> 改变时触发",
      "type": "function",
      "details": "(name: TabPaneName) => void"
    },
    {
      "name": "tab-remove",
      "title": "点击 tab 移除按钮时触发",
      "type": "function",
      "details": "(name: TabPaneName) => void"
    },
    {
      "name": "tab-add",
      "title": "点击 tab 新增按钮时触发",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "edit",
      "title": "点击 tab 的新增或移除按钮后触发",
      "type": "function",
      "details": "(paneName: TabPaneName | undefined, action: 'remove' | 'add') => void"
    }
  ],
  "exposes": [
    {
      "name": "currentName",
      "title": "当前活动的面板名称",
      "type": "object",
      "details": "Ref<TabPaneName>"
    },
    {
      "name": "tabNavRef",
      "title": "tab-nav 组件实例",
      "type": "object",
      "details": "Ref<TabNavInstance | undefined>",
      "version": "2.9.10"
    }
  ]
}