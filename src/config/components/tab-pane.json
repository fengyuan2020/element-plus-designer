{
  "__id__": "tab-pane",
  "name": "标签页",
  "desc": "分隔内容上有关联但属于不同类别的数据集合。",
  "tag": "el-tab-pane",
  "tagIcon": "tab-pane",
  "layouts": [
    "containerItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/tabs.html",
  "parentTag": [
    "el-tabs"
  ],
  "attributes": {
    "label": {
      "label": "选项卡标题",
      "title": "选项卡标题",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "name": {
      "label": "标识符",
      "title": "与选项卡绑定值 value 对应的标识符，表示选项卡别名。默认值是tab面板的序列号，如第一个 tab 是 0",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "closable": {
      "label": "可关闭",
      "title": "标签是否可关闭",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "lazy": {
      "label": "延迟渲染",
      "title": "标签是否延迟渲染",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "Tab-pane 的内容"
    },
    {
      "name": "label",
      "title": "Tab-pane 的标题内容"
    }
  ]
}