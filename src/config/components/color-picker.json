{
  "__id__": "color-picker",
  "name": "颜色选择",
  "desc": "用于颜色选择，支持多种格式。",
  "tag": "el-color-picker",
  "tagIcon": "color",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/color-picker.html",
  "attributes": {
    "vModel": "modelValue",
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "size": {
      "label": "尺寸",
      "title": "尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default"
    },
    "show-alpha": {
      "label": "透明度选择",
      "title": "是否支持透明度选择",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "color-format": {
      "label": "颜色的格式",
      "title": "写入 v-model 的颜色的格式",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "hsl",
              "value": "hsl"
            },
            {
              "label": "hsv",
              "value": "hsv"
            },
            {
              "label": "hex",
              "value": "hex"
            },
            {
              "label": "rgb",
              "value": "rgb"
            }
          ]
        }
      ],
      "default": "hex"
    },
    "popper-class": {
      "label": "下拉框的类名",
      "title": "ColorPicker 下拉框的类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "predefine": {
      "label": "预设颜色",
      "title": "预定义颜色",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": []
    },
    "validate-event": {
      "label": "表单校验",
      "title": "输入时是否触发表单的校验",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "tabindex": {
      "label": "ColorPicker 的 tabindex",
      "title": "ColorPicker 的 tabindex",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ],
      "default": 0
    },
    "aria-label": {
      "label": "ColorPicker 的 aria-label",
      "title": "ColorPicker 的 aria-label",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true,
      "version": "2.7.2"
    },
    "empty-values": {
      "label": "组件的空值配置",
      "title": "组件的空值配置 <a href=\"/en-US/component/config-provider#empty-values-configurations\">参考config-provider</a>",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "version": "2.10.3"
    },
    "value-on-clear": {
      "label": "清空选项的值",
      "title": "清空选项的值 <a href=\"/en-US/component/config-provider#empty-values-configurations\">参考 config-provider</a>",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        },
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "boolean",
          "tag": "el-switch"
        },
        {
          "name": "function",
          "details": ""
        }
      ],
      "version": "2.10.3"
    },
    "id": {
      "label": "ColorPicker 的 id",
      "title": "ColorPicker 的 id",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "teleported": {
      "label": "将 popover 的下拉列表渲染至 body 下",
      "title": "是否将 popover 的下拉列表渲染至 body 下",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "version": "2.7.2"
    },
    "label": {
      "label": "ColorPicker 的 aria-label",
      "title": "ColorPicker 的 aria-label",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true,
      "deprecated": true
    },
    "persistent": {
      "label": "当颜色选择器未激活且 persistent 为 false 时",
      "title": "当颜色选择器未激活且 persistent 为 false 时，颜色面板将被销毁。",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "version": "2.10.5"
    },
    "append-to": {
      "label": "挂载到哪个 DOM 元素",
      "title": "挂载到哪个 DOM 元素",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "version": "2.10.5"
    }
  },
  "slots": [],
  "events": [
    {
      "name": "change",
      "title": "当绑定值变化时触发",
      "type": "function",
      "details": "(value: string) => void"
    },
    {
      "name": "active-change",
      "title": "面板中当前显示的颜色发生改变时触发",
      "type": "function",
      "details": "(value: string) => void"
    },
    {
      "name": "focus",
      "title": "当获得焦点时触发",
      "type": "function",
      "details": "(event: FocusEvent) => void",
      "version": "2.4.0"
    },
    {
      "name": "blur",
      "title": "当失去焦点时触发",
      "type": "function",
      "details": "(event: FocusEvent) => void",
      "version": "2.4.0"
    }
  ],
  "exposes": [
    {
      "name": "color",
      "title": "当前色彩对象",
      "type": "object",
      "details": "Color"
    },
    {
      "name": "show",
      "title": "手动显示颜色选择器",
      "type": "function",
      "details": "() => void",
      "version": "2.3.3"
    },
    {
      "name": "hide",
      "title": "手动隐藏颜色选择器",
      "type": "function",
      "details": "() => void",
      "version": "2.3.3"
    },
    {
      "name": "focus",
      "title": "使 picker 获得焦点",
      "type": "function",
      "details": "() => void",
      "version": "2.3.13"
    },
    {
      "name": "blur",
      "title": "使 picker 失去焦点",
      "type": "function",
      "details": "() => void",
      "version": "2.3.13"
    }
  ]
}