{
  "__id__": "input",
  "name": "单行文本",
  "tag": "el-input",
  "tagIcon": "input",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/input.html",
  "attributes": {
    "vModel": "modelValue",
    "placeholder": "请输入",
    "type": {
      "label": "",
      "title": "类型",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "hide": true,
      "default": "text"
    },
    "maxlength": {
      "label": "最大输入长度",
      "title": "最大输入长度",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "minlength": {
      "label": "最小输入长度",
      "title": "原生属性，最小输入长度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "hide": true
    },
    "show-word-limit": {
      "label": "统计字数",
      "title": "是否显示统计字数, 只在 type 为 'text' 或 'textarea' 的时候生效",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "switch": [
        "show-password",
        false
      ]
    },
    "clearable": {
      "label": "可清空",
      "title": "是否可清空",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "formatter": {
      "label": "输入值的格式",
      "title": "指定输入值的格式。(只有当 type 是\"text\"时才能工作)",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "(value: string | number) => string"
        }
      ],
      "hide": true,
      "switch": [
        "type",
        "text"
      ]
    },
    "parser": {
      "label": "提取的值",
      "title": "指定从格式化器输入中提取的值。(仅当 type 是\"text\"时才起作用)",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "(value: string) => string"
        }
      ],
      "hide": true,
      "switch": [
        "type",
        "text"
      ]
    },
    "show-password": {
      "label": "显示切换密码图标",
      "title": "是否显示切换密码图标",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "hide": true,
      "default": false
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "size": {
      "label": "尺寸",
      "title": "输入框尺寸，只在 type 不为 'textarea' 时有效",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default",
      "switch": [
        "type",
        "text"
      ]
    },
    "prefix-icon": {
      "label": "前缀图标",
      "title": "自定义前缀图标",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "switch": [
        "type",
        "text"
      ]
    },
    "suffix-icon": {
      "label": "后缀图标",
      "title": "自定义后缀图标",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "switch": [
        "type",
        "text"
      ]
    },
    "rows": {
      "label": "输入框行数",
      "title": "输入框行数，仅 `type` 为 'textarea' 时有效",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "switch": [
        "type",
        "textarea"
      ]
    },
    "autosize": {
      "label": "高度自适应",
      "title": "textarea 高度是否自适应，仅 `type` 为 'textarea' 时生效。 可以接受一个对象，比如: `{ minRows: 2, maxRows: 6 }`",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        },
        {
          "name": "object",
          "tag": "fixed-keys-object-editor",
          "props": {
            "keys": [
              {
                "name": "minRows",
                "tag": "el-input-number"
              },
              {
                "name": "maxRows",
                "tag": "el-input-number"
              }
            ]
          }
        }
      ],
      "default": false,
      "switch": [
        "type",
        "textarea"
      ]
    },
    "autocomplete": {
      "label": "autocomplete",
      "title": "原生 autocomplete 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "off",
      "hide": true
    },
    "name": {
      "label": "name",
      "title": "等价于原生 input name 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "hide": true
    },
    "readonly": {
      "label": "只读",
      "title": "原生  readonly 属性，是否只读",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "max": {
      "label": "max",
      "title": "原生 max 属性，设置最大值",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "hide": true
    },
    "min": {
      "label": "min",
      "title": "原生属性，设置最小值",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "hide": true
    },
    "step": {
      "label": "step",
      "title": "原生属性，设置输入字段的合法数字间隔",
      "types": [
        {
          "name": "—",
          "tag": ""
        }
      ],
      "hide": true
    },
    "resize": {
      "label": "缩放",
      "title": "控制是否能被用户缩放",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "none",
              "value": "none"
            },
            {
              "label": "both",
              "value": "both"
            },
            {
              "label": "horizontal",
              "value": "horizontal"
            },
            {
              "label": "vertical",
              "value": "vertical"
            }
          ]
        }
      ],
      "hide": true
    },
    "autofocus": {
      "label": "自动获取焦点",
      "title": "原生属性，自动获取焦点",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "form": {
      "label": "form",
      "title": "原生属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "hide": true
    },
    "label": {
      "label": "标签文本",
      "title": "标签文本",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "hide": true,
      "a11y": true,
      "deprecated": true
    },
    "aria-label": {
      "title": "等价于原生 input `aria-label` 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true,
      "version": "2.7.2"
    },
    "tabindex": {
      "label": "tabindex",
      "title": "输入框的 tabindex",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ],
      "hide": true
    },
    "validate-event": {
      "label": "表单校验",
      "title": "输入时是否触发表单的校验",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "input-style": {
      "label": "style",
      "title": "input 元素或 textarea 元素的 style",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "object",
          "tag": "style-editor"
        }
      ],
      "default": {}
    }
  },
  "events": [
    {
      "name": "blur",
      "title": "当选择器的输入框失去焦点时触发",
      "type": "Function",
      "fnDes": "(event: FocusEvent) => void"
    },
    {
      "name": "focus",
      "title": "当选择器的输入框获得焦点时触发",
      "type": "Function",
      "fnDes": "(event: FocusEvent) => void"
    },
    {
      "name": "change",
      "title": "仅当 modelValue 改变时，当输入框失去焦点或用户按Enter时触发",
      "type": "Function",
      "fnDes": "(value: string | number) => void"
    },
    {
      "name": "input",
      "title": "在 Input 值改变时触发",
      "type": "Function",
      "fnDes": "(value: string | number) => void"
    },
    {
      "name": "clear",
      "title": "在点击由 `clearable` 属性生成的清空按钮时触发",
      "type": "Function",
      "fnDes": "() => void"
    }
  ],
  "slots": [
    {
      "name": "prefix",
      "title": "输入框头部内容，只对非 `type=\"textarea\"` 有效"
    },
    {
      "name": "suffix",
      "title": "输入框尾部内容，只对非 `type=\"textarea\"` 有效"
    },
    {
      "name": "prepend",
      "title": "输入框前置内容，只对非 `type=\"textarea\"` 有效"
    },
    {
      "name": "append",
      "title": "输入框后置内容，只对非 `type=\"textarea\"` 有效"
    }
  ],
  "exposes": [
    {
      "name": "blur",
      "title": "使 input 失去焦点",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "clear",
      "title": "清除 input 值",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "focus",
      "title": "使 input 获取焦点",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "input",
      "title": "Input HTML 元素",
      "type": "^[object]`Ref<HTMLInputElement>`"
    },
    {
      "name": "ref",
      "title": "HTML元素 input 或 textarea",
      "type": "^[object]`Ref<HTMLInputElement | HTMLTextAreaElement>`"
    },
    {
      "name": "resizeTextarea",
      "title": "改变 textarea 大小",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "select",
      "title": "选中 input 中的文字",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "textarea",
      "title": "HTML textarea 元素",
      "type": "^[object]`Ref<HTMLTextAreaElement>`"
    },
    {
      "name": "textareaStyle",
      "title": "textarea 的样式",
      "type": "^[object]`Ref<StyleValue>`"
    }
  ],
  "desc": "通过鼠标或键盘输入字符"
}