{
  "__id__": "date-picker",
  "name": "日期选择",
  "desc": "用于选择或输入日期",
  "tag": "el-date-picker",
  "tagIcon": "date",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/date-picker.html",
  "attributes": {
    "vModel": "modelValue",
    "placeholder": "请选择",
    "readonly": {
      "label": "只读",
      "title": "只读",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "disabled": {
      "label": "禁用",
      "title": "禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "size": {
      "label": "尺寸",
      "title": "输入框尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default"
    },
    "editable": {
      "label": "文本框可输入",
      "title": "文本框可输入",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "clearable": {
      "label": "显示清除按钮",
      "title": "是否显示清除按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "type": {
      "label": "类型",
      "title": "显示类型",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "日(date)",
              "value": "date"
            },
            {
              "label": "多日(dates)",
              "value": "dates"
            },
            {
              "label": "周(week)",
              "value": "week"
            },
            {
              "label": "月(month)",
              "value": "month"
            },
            {
              "label": "年(year)",
              "value": "year"
            },
            {
              "label": "日期时间(datetime)",
              "value": "datetime"
            },
            {
              "label": "日期范围(daterange)",
              "value": "daterange"
            },
            {
              "label": "月范围(monthrange)",
              "value": "monthrange"
            },
            {
              "label": "日期时间范围(datetimerange)",
              "value": "datetimerange"
            }
          ]
        }
      ],
      "default": "date",
      "keep": true
    },
    "format": {
      "label": "显示格式",
      "title": "显示在输入框中的格式",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": "YYYY-MM-DD",
      "keep": true
    },
    "popper-class": {
      "label": "下拉框的类名",
      "title": "DatePicker 下拉框的类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "popper-style": {
      "label": "弹出内容的自定义样式",
      "title": "弹出内容的自定义样式",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "object",
          "tag": "style-editor"
        }
      ]
    },
    "popper-options": {
      "label": "自定义 popper 选项",
      "title": "自定义 popper 选项，更多请参考 <a href=\"https://popper.js.org/docs/v2/\">popper.js</a>",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "range-separator": {
      "label": "分隔符",
      "title": "选择范围时的分隔符",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "-"
    },
    "default-value": {
      "label": "默认显示时间",
      "title": "可选，选择器打开时默认显示的时间",
      "types": [
        {
          "name": "date",
          "tag": "el-date-picker"
        },
        {
          "name": "array",
          "tag": "array-editor",
          "props": {
            "tag": "el-date-picker",
            "fixedNumber": 2
          }
        }
      ],
      "keep": true
    },
    "default-time": {
      "label": "默认时间",
      "title": "范围选择时选中日期所使用的当日内具体时刻",
      "types": [
        {
          "name": "date",
          "tag": "el-time-picker"
        },
        {
          "name": "array",
          "tag": "array-editor",
          "props": {
            "tag": "el-time-picker",
            "fixedNumber": 2
          }
        }
      ],
      "keep": true
    },
    "value-format": {
      "label": "值格式",
      "title": "可选，绑定值的格式。 不指定则绑定值为 Date 对象",
      "types": [
      ],
      "hide": true,
      "keep": true
    },
    "id": {
      "label": "等价于原生 input id 属性",
      "title": "等价于原生 input <code>id</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "name": {
      "label": "等价于原生 input name 属性",
      "title": "等价于原生 input <code>name</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "hide": true,
      "keep": true
    },
    "unlink-panels": {
      "label": "取消两个日期面板联动",
      "title": "在范围选择器里取消两个日期面板之间的联动",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "prefix-icon": {
      "label": "前缀图标",
      "title": "自定义前缀图标 如果 <code>type</code>的值是<code>TimeLikeType</code>，那么就是 <code>Clock</code>，不然就是 <code>Calendar</code>",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "clear-icon": {
      "label": "清除图标",
      "title": "自定义清除图标",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": "CircleClose"
    },
    "validate-event": {
      "label": "触发表单校验",
      "title": "是否触发表单验证",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "disabled-date": {
      "label": "一个用来判断该日期是否被禁用的函数",
      "title": "一个用来判断该日期是否被禁用的函数，接受一个 Date 对象作为参数。 应该返回一个 Boolean 值。",
      "types": [
        {
          "name": "function",
          "details": "(data: Date) => boolean"
        }
      ],
      "hide": true
    },
    "shortcuts": {
      "label": "快捷选项",
      "title": "设置快捷选项，需要传入数组对象",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "hide": true,
      "keep": true
    },
    "cell-class-name": {
      "label": "自定义类名",
      "title": "设置自定义类名",
      "types": [
        {
          "name": "function",
          "details": "(data: Date) => string"
        }
      ]
    },
    "teleported": {
      "label": "下拉列表插入至 body 元素",
      "title": "是否将 date-picker 的下拉列表插入至 body 元素",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "empty-values": {
      "label": "组件的空值配置",
      "title": "组件的空值配置 <a href=\"/en-US/component/config-provider.html#empty-values-configurations\"></a>",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "version": "2.7.0"
    },
    "value-on-clear": {
      "label": "清空选项的值",
      "title": "清空选项的值 <a href=\"/en-US/component/config-provider.html#empty-values-configurations\"></a>",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        },
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "boolean",
          "tag": "el-switch"
        },
        {
          "name": "function",
          "details": "^[string] / ^[number] / ^[boolean] / ^[Function"
        }
      ],
      "version": "2.7.0"
    },
    "fallback-placements": {
      "label": "Tooltip 可用的 positions 请查看popper.js 文档",
      "title": "Tooltip 可用的 positions 请查看<a href=\"https://popper.js.org/docs/v2/modifiers/flip/#fallbackplacements\">popper.js 文档</a>",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "version": "2.8.4"
    },
    "placement": {
      "label": "下拉框出现的位置",
      "title": "下拉框出现的位置",
      "types": [
        {
          "name": "Placement"
        }
      ],
      "default": "bottom",
      "version": "2.8.4"
    },
    "show-footer": {
      "label": "显示 footer",
      "title": "whether to show footer where the date picker is one [object Object]",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "version": "2.10.5"
    },
    "show-confirm": {
      "label": "显示确定按钮",
      "title": "是否显示确定按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "version": "2.11.0"
    },
    "show-week-number": {
      "label": "显示周数",
      "title": "显示周数(除周外)",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "version": "2.10.3"
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义单元格内容"
    },
    {
      "name": "range-separator",
      "title": "自定义范围分割符内容"
    },
    {
      "name": "prev-month",
      "title": "上个月的图标",
      "version": "2.8.0"
    },
    {
      "name": "next-month",
      "title": "下个月的图标",
      "version": "2.8.0"
    },
    {
      "name": "prev-year",
      "title": "上一年图标",
      "version": "2.8.0"
    },
    {
      "name": "next-year",
      "title": "下一年图标",
      "version": "2.8.0"
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "当用户确认值或点击外部时触发",
      "type": "function",
      "details": "(val: typeof v-model) => void"
    },
    {
      "name": "blur",
      "title": "在组件 Input 失去焦点时触发",
      "type": "function",
      "details": "(e: FocusEvent) => void"
    },
    {
      "name": "focus",
      "title": "在组件 Input 获得焦点时触发",
      "type": "function",
      "details": "(e: FocusEvent) => void"
    },
    {
      "name": "clear",
      "title": "可清空的模式下用户点击清空按钮时触发",
      "type": "function",
      "details": "() => void",
      "version": "2.7.7"
    },
    {
      "name": "calendar-change",
      "title": "在日历所选日期更改时触发 仅用于 <code>range</code>",
      "type": "function",
      "details": "(val: [Date, null | Date]) => void"
    },
    {
      "name": "panel-change",
      "title": "当日期面板改变时触发。",
      "type": "function",
      "details": "(date: Date | [Date, Date], mode: 'month' | 'year', view?: string) => void"
    },
    {
      "name": "visible-change",
      "title": "当 DatePicker 的下拉列表出现/消失时触发",
      "type": "function",
      "details": "(visibility: boolean) => void"
    }
  ],
  "exposes": [
    {
      "name": "focus",
      "title": "使组件获取焦点",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "blur",
      "title": "使组件失去焦点",
      "type": "function",
      "details": "() => void",
      "version": "2.8.7"
    },
    {
      "name": "handleOpen",
      "title": "打开日期选择器弹窗",
      "type": "function",
      "details": "() => void",
      "version": "2.2.16"
    },
    {
      "name": "handleClose",
      "title": "关闭日期选择器弹窗",
      "type": "function",
      "details": "() => void",
      "version": "2.2.16"
    }
  ]
}