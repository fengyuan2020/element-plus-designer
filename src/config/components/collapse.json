{
  "__id__": "collapse",
  "name": "折叠面板",
  "desc": "通过折叠面板收纳内容区域",
  "tag": "el-collapse",
  "tagIcon": "collapse",
  "layouts": [
    "containerItem"
  ],
  "childTag": [
    "el-collapse-item"
  ],
  "document": "https://element-plus.org/zh-CN/component/collapse.html",
  "attributes": {
    "vModel": "modelValue",
    "accordion": {
      "label": "手风琴模式",
      "title": "是否手风琴模式",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "expand-icon-position": {
      "label": "展开图标位置",
      "title": "设置展开图标位置",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "left",
              "value": "left"
            },
            {
              "label": "right",
              "value": "right"
            }
          ]
        }
      ],
      "default": "right",
      "version": "2.9.10"
    },
    "before-collapse": {
      "label": "折叠状态更改之前的折叠钩子。 返回 false 或者返回 Promise 且被 reject 则停止切换",
      "title": "折叠状态更改之前的折叠钩子。 返回 <code>false</code> 或者返回 <code>Promise</code> 且被 reject 则停止切换",
      "types": [
        {
          "name": "function",
          "details": "() => Promise<boolean> | boolen"
        }
      ],
      "version": "2.9.11"
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义默认内容",
      "subtags": [
        "collapse-item"
      ]
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "切换当前活动面板，在手风琴模式下其类型是<code>string</code>，在其他模式下是<code>array</code>",
      "type": "function",
      "details": "(activeNames: array | string) => void"
    }
  ],
  "exposes": [
    {
      "name": "activeNames",
      "title": "当前活动的面板名称",
      "type": "object",
      "details": "ComputedRef<(string | number)[]>"
    },
    {
      "name": "setActiveNames",
      "title": "设置活动面板名称",
      "type": "function",
      "details": "(activeNames: (string | number)[]) => void"
    }
  ]
}