{
  "__id__": "collapse",
  "name": "折叠面板",
  "tag": "el-collapse",
  "tagIcon": "collapse",
  "layouts": [
    "containerItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/collapse.html",
  "childTag": [
    "el-collapse-item"
  ],
  "attributes": {
    "vModel": "modelValue",
    "accordion": {
      "label": "手风琴模式",
      "title": "是否手风琴模式",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "name": {
      "title": "唯一标志符",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "title": {
      "title": "面板标题",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "icon": {
      "title": "折叠项目的图标",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": "ArrowRight",
      "version": "2.8.3"
    },
    "disabled": {
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    }
  },
  "events": [
    {
      "name": "change",
      "title": "切换当前活动面板，在手风琴模式下其类型是`string`，在其他模式下是`array`",
      "type": "Function",
      "fnDes": "(activeNames: array | string) => void"
    }
  ],
  "slots": [
    {
      "name": "default",
      "title": "自定义默认内容",
      "subtags": [
        "collapse-item"
      ]
    }
  ],
  "desc": "通过折叠面板收纳内容区域"
}