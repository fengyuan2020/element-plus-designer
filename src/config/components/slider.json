{
  "__id__": "slider",
  "name": "滑块",
  "desc": "通过拖动滑块在一个固定区间内进行选择",
  "tag": "el-slider",
  "tagIcon": "slider",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/slider.html",
  "attributes": {
    "vModel": "modelValue",
    "min": {
      "label": "最小值",
      "title": "最小值",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    },
    "max": {
      "label": "最大值",
      "title": "最大值",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 100
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "step": {
      "label": "步长",
      "title": "步长",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 1
    },
    "show-input": {
      "label": "输入框",
      "title": "是否显示输入框，仅在非范围选择时有效",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "switch": [
        "range",
        false
      ]
    },
    "show-input-controls": {
      "label": "输入框控制按钮",
      "title": "在显示输入框的情况下，是否显示输入框的控制按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "switch": [
        "show-input",
        true
      ]
    },
    "size": {
      "label": "大小",
      "title": "slider 包装器的大小，垂直模式下该属性不可用",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default",
      "switch": [
        "vertical",
        false
      ]
    },
    "input-size": {
      "label": "输入框的大小",
      "title": "输入框的大小，如果设置了 <code>size</code> 属性，默认值自动取 <code>size</code>",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default",
      "switch": [
        "show-input",
        true
      ]
    },
    "show-stops": {
      "label": "间断点",
      "title": "是否显示间断点",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "show-tooltip": {
      "label": "显示提示信息",
      "title": "是否显示提示信息",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "format-tooltip": {
      "label": "格式化提示信息",
      "title": "格式化提示信息",
      "types": [
        {
          "name": "function",
          "details": "(value: number) => number | string"
        }
      ]
    },
    "range": {
      "label": "选择范围",
      "title": "是否开启选择范围",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "vertical": {
      "label": "垂直模式",
      "title": "垂直模式",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "height": {
      "label": "高度",
      "title": "滑块高度，垂直模式必填",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "switch": [
        "vertical",
        true
      ]
    },
    "aria-label": {
      "label": "原生 aria-label属性",
      "title": "原生 <code>aria-label</code>属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true,
      "version": "2.7.2"
    },
    "range-start-label": {
      "label": "标签开始标记",
      "title": "当 <code>range</code> 为true时，屏幕阅读器标签开始的标记",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "switch": [
        "vertical",
        true
      ]
    },
    "range-end-label": {
      "label": "标签结尾标记",
      "title": "当 <code>range</code> 为true时，屏幕阅读器标签结尾的标记",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "switch": [
        "vertical",
        true
      ]
    },
    "format-value-text": {
      "label": "显示屏幕阅读器的 aria-valuenow 属性的格式",
      "title": "显示屏幕阅读器的 <code>aria-valuenow</code> 属性的格式",
      "types": [
        {
          "name": "function",
          "details": "(value: number) => string"
        }
      ]
    },
    "debounce": {
      "label": "去抖延迟",
      "title": "输入时的去抖延迟，毫秒，仅在 <code>show-input</code> 等于 true 时有效",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 300
    },
    "tooltip-class": {
      "label": "tooltip类名",
      "title": "tooltip 的自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "placement": {
      "label": "Tooltip出现位置",
      "title": "Tooltip 出现的位置",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "top",
              "value": "top"
            },
            {
              "label": "top-start",
              "value": "top-start"
            },
            {
              "label": "top-end",
              "value": "top-end"
            },
            {
              "label": "bottom",
              "value": "bottom"
            },
            {
              "label": "bottom-start",
              "value": "bottom-start"
            },
            {
              "label": "bottom-end",
              "value": "bottom-end"
            },
            {
              "label": "left",
              "value": "left"
            },
            {
              "label": "left-start",
              "value": "left-start"
            },
            {
              "label": "left-end",
              "value": "left-end"
            },
            {
              "label": "right",
              "value": "right"
            },
            {
              "label": "right-start",
              "value": "right-start"
            },
            {
              "label": "right-end",
              "value": "right-end"
            }
          ]
        }
      ],
      "default": "top"
    },
    "marks": {
      "label": "标记",
      "title": "标记， key 的类型必须为 <code>number</code> 且取值在闭区间 <code>[min, max]</code> 内，每个标记可以单独设置样式",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "validate-event": {
      "label": "表单校验",
      "title": "输入时是否触发表单的校验",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "persistent": {
      "label": "当 slider 的 tooltip 处于非活动状态且 persistent 为 false 时",
      "title": "当 slider 的 tooltip 处于非活动状态且 <code>persistent</code> 为 <code>false</code> 时，Popconfirm 将被销毁。 当 <code>show-tooltip</code> 为 <code>false</code> 时，<code>persistent</code> 将始终为 <code>false</code>。",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "version": "2.9.5"
    },
    "label": {
      "label": "屏幕阅读器标签",
      "title": "原生 <code>aria-label</code>属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "a11y": true,
      "deprecated": true
    }
  },
  "slots": [],
  "events": [
    {
      "name": "change",
      "title": "值改变时触发（使用鼠标拖曳时，只在松开鼠标后触发）",
      "type": "function",
      "details": "(value: Arrayable<number>) => boolean"
    },
    {
      "name": "input",
      "title": "数据改变时触发（使用鼠标拖曳时，活动过程实时触发）",
      "type": "function",
      "details": "(value: Arrayable<number>) => boolean"
    }
  ]
}