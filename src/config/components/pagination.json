{
  "__id__": "pagination",
  "name": "分页",
  "desc": "当数据量过多时，使用分页分解数据。",
  "tag": "el-pagination",
  "tagIcon": "pagination",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/pagination.html",
  "attributes": {
    "size": {
      "label": "大小",
      "title": "分页大小",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default",
      "version": "2.7.6"
    },
    "background": {
      "label": "添加背景色",
      "title": "是否为分页按钮添加背景色",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "default-page-size": {
      "label": "每页个数初始值",
      "title": "每页默认的条目个数，不设置时默认为10",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "total": {
      "label": "总数",
      "title": "总条目数",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "page-count": {
      "label": "总页数",
      "title": "总页数， <code>total</code> 和 <code>page-count</code> 设置任意一个就可以达到显示页码的功能；如果要支持 <code>page-sizes</code> 的更改，则需要使用 <code>total</code> 属性",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "pager-count": {
      "label": "最大页码按钮数",
      "title": "设置最大页码按钮数。 页码按钮的数量，当总页数超过该值时会折叠",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 7
    },
    "default-current-page": {
      "label": "初始页",
      "title": "当前页数的默认初始值，不设置时默认为 1",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "layout": {
      "label": "组件布局",
      "title": "组件布局，子组件名用逗号分隔",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "prev, pager, next, jumper, ->, total"
    },
    "page-sizes": {
      "label": "每页显示个数",
      "title": "每页显示个数选择器的选项设置",
      "types": [
        {
          "name": "array",
          "tag": "array-editor",
          "props": {
            "tag": "el-input-number",
            "tagProps": {
              "step": 10,
              "value-on-clear": 10
            }
          }
        }
      ],
      "default": [
        10,
        20,
        30,
        40,
        50,
        100
      ],
      "keep": true
    },
    "append-size-to": {
      "label": "下拉框挂载到哪个 DOM 元素",
      "title": "下拉框挂载到哪个 DOM 元素",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "version": "2.8.4"
    },
    "popper-class": {
      "label": "下拉框类名",
      "title": "每页显示个数选择器的下拉框类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "prev-text": {
      "label": "上一页文字",
      "title": "替代图标显示的上一页文字",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "prev-icon": {
      "label": "上一页图标",
      "title": "上一页的图标， 比 <code>prev-text</code> 优先级更高",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": "ArrowLeft"
    },
    "next-text": {
      "label": "下一页文字",
      "title": "替代图标显示的下一页文字",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "next-icon": {
      "label": "下一页图标",
      "title": "下一页的图标， 比 <code>next-text</code> 优先级更低",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": "ArrowRight"
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用分页",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "teleported": {
      "label": "将下拉菜单teleport至 body",
      "title": "是否将下拉菜单teleport至 body",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "version": "2.3.13"
    },
    "hide-on-single-page": {
      "label": "只有一页时隐藏",
      "title": "只有一页时是否隐藏",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "small": {
      "label": "小型分页",
      "title": "是否使用小型分页样式",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "deprecated": true
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义内容 设置文案，需要在 <code>layout</code> 中列出 <code>slot</code>"
    }
  ],
  "events": [
    {
      "name": "size-change",
      "title": "<code>page-size</code> 改变时触发",
      "type": "function",
      "details": "(value: number) => void"
    },
    {
      "name": "current-change",
      "title": "<code>current-page</code> 改变时触发",
      "type": "function",
      "details": "(value: number) => void"
    },
    {
      "name": "change",
      "title": "<code>current-page</code> 或 <code>page-size</code> 更改时触发",
      "type": "function",
      "details": "(currentPage: number, pageSize: number) => void",
      "version": "2.4.4"
    },
    {
      "name": "prev-click",
      "title": "用户点击上一页按钮改变当前页时触发",
      "type": "function",
      "details": "(value: number) => void"
    },
    {
      "name": "next-click",
      "title": "用户点击下一页按钮改变当前页时触发",
      "type": "function",
      "details": "(value: number) => void"
    }
  ]
}