{
  "__id__": "input-tag",
  "name": "标签输入框",
  "desc": "InputTag 组件允许用户添加内容作为标签",
  "tag": "el-input-tag",
  "tagIcon": "input-tag",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/tag-tag.html",
  "attributes": {
    "vModel": "modelValue",
    "placeholder": "请输入",
    "max": {
      "label": "最大数量",
      "title": "可添加标签的最大数量",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "tag-type": {
      "label": "标签类型",
      "title": "标签类型",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "success",
              "value": "success"
            },
            {
              "label": "info",
              "value": "info"
            },
            {
              "label": "warning",
              "value": "warning"
            },
            {
              "label": "danger",
              "value": "danger"
            }
          ]
        }
      ],
      "default": "info"
    },
    "tag-effect": {
      "label": "标签效果",
      "title": "标签效果",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "light",
              "value": "light"
            },
            {
              "label": "dark",
              "value": "dark"
            },
            {
              "label": "plain",
              "value": "plain"
            }
          ]
        }
      ],
      "default": "light"
    },
    "trigger": {
      "label": "触发输入标签的按键",
      "title": "触发输入标签的按键",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "Enter",
              "value": "Enter"
            },
            {
              "label": "Space",
              "value": "Space"
            }
          ]
        }
      ],
      "default": "Enter"
    },
    "draggable": {
      "label": "可以拖动标签",
      "title": "是否可以拖动标签",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "delimiter": {
      "label": "在匹配分隔符时添加标签",
      "title": "在匹配分隔符时添加标签",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        },
        {
          "name": "regex"
        }
      ],
      "version": "2.9.9"
    },
    "size": {
      "label": "输入框尺寸",
      "title": "输入框尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ]
    },
    "save-on-blur": {
      "label": "当输入失去焦点时是否保存输入值",
      "title": "当输入失去焦点时是否保存输入值",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "version": "2.9.7"
    },
    "clearable": {
      "label": "清除按钮",
      "title": "是否显示清除按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "validate-event": {
      "label": "触发表单验证",
      "title": "是否触发表单验证",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "readonly": {
      "label": "readonly",
      "title": "等价于原生 <code>readonly</code> 属性",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "autofocus": {
      "label": "autofocus",
      "title": "等价于原生 <code> autofocus </code> 属性",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "id": {
      "label": "inputid",
      "title": "等价于原生 input <code>id</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "tabindex": {
      "label": "tabindex",
      "title": "等价于原生 <code> tabindex </code> 属性",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "maxlength": {
      "label": "maxlength",
      "title": "等价于原生 <code> maxlength </code> 属性",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "minlength": {
      "label": "minlength",
      "title": "等价于原生 <code> minlength </code> 属性",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "autocomplete": {
      "label": "autocomplete",
      "title": "等价于原生 <code> autocomplete </code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": "off"
    },
    "aria-label": {
      "label": "aria-label",
      "title": "等价于原生 <code> aria-label </code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true
    }
  },
  "slots": [
    {
      "name": "tag",
      "title": "作为tag的内容",
      "type": "object",
      "details": "{ value: string, index: number }"
    },
    {
      "name": "prefix",
      "title": "InputTag 头部内容"
    },
    {
      "name": "suffix",
      "title": "InputTag 尾部内容"
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "绑定值变化时触发的事件",
      "type": "function",
      "details": "(value: string[]) => void"
    },
    {
      "name": "input",
      "title": "在 Input 值改变时触发",
      "type": "function",
      "details": "(value: string) => void"
    },
    {
      "name": "add-tag",
      "title": "tag 被添加时触发",
      "type": "function",
      "details": "(value: string \\| string []) => void"
    },
    {
      "name": "remove-tag",
      "title": "tag 被移除时触发",
      "type": "function",
      "details": "(value: string) => void"
    },
    {
      "name": "focus",
      "title": "在 Input 获得焦点时触发",
      "type": "function",
      "details": "(event: FocusEvent) => void"
    },
    {
      "name": "blur",
      "title": "在 Input 失去焦点时触发",
      "type": "function",
      "details": "(event: FocusEvent) => void"
    },
    {
      "name": "clear",
      "title": "点击清除图标时触发",
      "type": "function",
      "details": "() => void"
    }
  ],
  "exposes": [
    {
      "name": "focus",
      "title": "使 input 获取焦点",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "blur",
      "title": "使 input 失去焦点",
      "type": "function",
      "details": "() => void"
    }
  ]
}