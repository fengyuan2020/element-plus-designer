{
  "__id__": "dropdown",
  "name": "下拉菜单",
  "desc": "将动作或菜单折叠到下拉菜单中。",
  "tag": "el-dropdown",
  "tagIcon": "dropdown",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/dropdown.html",
  "attributes": {
    "type": {
      "label": "菜单按钮类型",
      "title": "菜单按钮类型，同 <code>Button</code> 组件一样，仅在 <code>split-button</code> 为 true 的情况下有效。",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "primary",
              "value": "primary"
            },
            {
              "label": "success",
              "value": "success"
            },
            {
              "label": "warning",
              "value": "warning"
            },
            {
              "label": "info",
              "value": "info"
            },
            {
              "label": "danger",
              "value": "danger"
            },
            {
              "label": "text (deprecated)",
              "value": "text"
            }
          ]
        }
      ],
      "keep": true
    },
    "size": {
      "label": "菜单尺寸",
      "title": "菜单尺寸，在 split-button 为 true 的情况下也对触发按钮生效。",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ]
    },
    "button-props": {
      "label": "按钮组件的 props",
      "title": "按钮组件的 props，参考 <a href=\"./button.html#button-attributes\"></a>",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "max-height": {
      "label": "最大高度",
      "title": "菜单最大高度",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "split-button": {
      "label": "下拉触发元素呈现为按钮组",
      "title": "下拉触发元素呈现为按钮组",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "placement": {
      "label": "菜单弹出位置",
      "title": "菜单弹出位置",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "top",
              "value": "top"
            },
            {
              "label": "top-start",
              "value": "top-start"
            },
            {
              "label": "top-end",
              "value": "top-end"
            },
            {
              "label": "bottom",
              "value": "bottom"
            },
            {
              "label": "bottom-start",
              "value": "bottom-start"
            },
            {
              "label": "bottom-end",
              "value": "bottom-end"
            }
          ]
        }
      ],
      "default": "bottom"
    },
    "trigger": {
      "label": "触发下拉的行为",
      "title": "触发下拉的行为",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "hover",
              "value": "hover"
            },
            {
              "label": "click",
              "value": "click"
            },
            {
              "label": "contextmenu",
              "value": "contextmenu"
            }
          ]
        }
      ],
      "default": "hover"
    },
    "trigger-keys": {
      "label": "指定键盘上哪些按键可以触发操作",
      "title": "指定键盘上哪些按键可以触发操作",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "default": "['Enter', 'Space', 'ArrowDown', 'NumpadEnter']",
      "version": "2.9.1"
    },
    "hide-on-click": {
      "label": "在点击菜单项后隐藏菜单",
      "title": "是否在点击菜单项后隐藏菜单",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "show-timeout": {
      "label": "展开下拉菜单的延时",
      "title": "展开下拉菜单的延时，仅在 trigger 为 hover 时有效",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 150
    },
    "hide-timeout": {
      "label": "收起下拉菜单的延时",
      "title": "收起下拉菜单的延时（仅在 trigger 为 hover 时有效）",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 150
    },
    "role": {
      "label": "ARIA 属性",
      "title": "下拉菜单的 ARIA 属性。 根据具体场景，您可能想要将此更改为“navigation”",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "dialog",
              "value": "dialog"
            },
            {
              "label": "grid",
              "value": "grid"
            },
            {
              "label": "group",
              "value": "group"
            },
            {
              "label": "listbox",
              "value": "listbox"
            },
            {
              "label": "menu",
              "value": "menu"
            },
            {
              "label": "navigation",
              "value": "navigation"
            },
            {
              "label": "tooltip",
              "value": "tooltip"
            },
            {
              "label": "tree",
              "value": "tree"
            }
          ]
        }
      ],
      "default": "menu"
    },
    "tabindex": {
      "label": "Dropdown 组件的 [object Object]",
      "title": "Dropdown 组件的 <a href=\"https://developer.mozilla.org/en-US/docs/Web/HTML/Global_attributes/tabindex\">tabindex</a>",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ],
      "default": 0
    },
    "popper-class": {
      "label": "自定义浮层类名",
      "title": "自定义浮层类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "popper-options": {
      "label": "popper.js 参数",
      "title": "<a href=\"https://popper.js.org/docs/v2/\">popper.js</a> 参数",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": "{modifiers: [{name: 'computeStyles',options: {gpuAcceleration: false}}]}"
    },
    "teleported": {
      "label": "将下拉列表插入至 body 元素",
      "title": "是否将下拉列表插入至 body 元素",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "version": "2.2.20"
    },
    "persistent": {
      "label": "当下拉菜单处于非活动状态且 persistent 为 false 时",
      "title": "当下拉菜单处于非活动状态且 <code>persistent</code> 为 <code>false</code> 时，下拉菜单将被销毁",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "version": "2.9.5"
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "下拉菜单的内容。 注意：必须是有效的 html DOM 元素（例如 <code><span>、<button></code> 等）或 <code>el-component</code>，以附加监听触发器"
    },
    {
      "name": "dropdown",
      "title": "下拉列表，通常是 <code><el-dropdown-menu></code> 组件",
      "subtags": [
        "dropdown-menu"
      ]
    }
  ],
  "events": [
    {
      "name": "click",
      "title": "<code>split-button</code> 为 true 时，点击左侧按钮的回调",
      "type": "function",
      "details": "(e: MouseEvent) => void"
    },
    {
      "name": "command",
      "title": "当下拉项被点击时触发，参数是从下拉菜单中发送的命令",
      "type": "function",
      "details": "(...args: any[]) => void"
    },
    {
      "name": "visible-change",
      "title": "当下拉菜单出现/消失时触发器, 当它出现时, 参数将是 <code>true</code>, 否则将是 <code>false</code>",
      "type": "function",
      "details": "(val: boolean) => void"
    }
  ],
  "exposes": [
    {
      "name": "handleOpen",
      "title": "打开下拉菜单",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "handleClose",
      "title": "关闭下拉菜单",
      "type": "function",
      "details": "() => void"
    }
  ]
}