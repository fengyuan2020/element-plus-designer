{
  "__id__": "menu-item",
  "name": "菜单项",
  "desc": "为网站提供导航功能的菜单。",
  "tag": "el-menu-item",
  "tagIcon": "menu-item",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/menu.html#menu-item-attributes",
  "parentTag": [
    "el-menu",
    "el-sub-menu",
    "el-menu-item-group"
  ],
  "attributes": {
    "index": {
      "label": "唯一标志",
      "title": "唯一标志",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "required": true
    },
    "route": {
      "label": "Vue Router",
      "title": "Vue Route 路由位置参数",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        },
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义默认内容"
    },
    {
      "name": "title",
      "title": "自定义标题内容"
    }
  ],
  "events": [
    {
      "name": "click",
      "title": "点击菜单项时回调函数, 参数为菜单项实例",
      "type": "function",
      "details": "(item: MenuItemRegistered) => void"
    }
  ]
}