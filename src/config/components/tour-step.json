{
  "__id__": "tour-step",
  "name": "漫游式引导步骤",
  "desc": "用于分步引导用户了解产品功能的气泡组件。 用来引导用户并介绍产品",
  "tag": "el-tour-step",
  "tagIcon": "tour-step",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/step.html",
  "parentTag": [
    "el-tour"
  ],
  "attributes": {
    "target": {
      "label": "指向元素",
      "title": "获取引导卡片指向的元素， 为空时居中于屏幕。 自[object Object]以来支持字符串和函数类型。 字符串类型是文档.querySelector的选择器。",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "show-arrow": {
      "label": "显示箭头",
      "title": "是否显示箭头",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "title": {
      "label": "标题部分",
      "title": "标题部分",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "description": {
      "label": "主要描述部分",
      "title": "主要描述部分",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "placement": {
      "label": "引导卡片位置",
      "title": "引导卡片相对于目标元素的位置",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "top",
              "value": "top"
            },
            {
              "label": "top-start",
              "value": "top-start"
            },
            {
              "label": "top-end",
              "value": "top-end"
            },
            {
              "label": "bottom",
              "value": "bottom"
            },
            {
              "label": "bottom-start",
              "value": "bottom-start"
            },
            {
              "label": "bottom-end",
              "value": "bottom-end"
            },
            {
              "label": "left",
              "value": "left"
            },
            {
              "label": "left-start",
              "value": "left-start"
            },
            {
              "label": "left-end",
              "value": "left-end"
            },
            {
              "label": "right",
              "value": "right"
            },
            {
              "label": "right-start",
              "value": "right-start"
            },
            {
              "label": "right-end",
              "value": "right-end"
            }
          ]
        }
      ],
      "default": "bottom"
    },
    "content-style": {
      "label": "内容样式",
      "title": "为content自定义样式",
      "types": [
        {
          "name": "object",
          "tag": "style-editor"
        }
      ],
      "default": {}
    },
    "mask": {
      "label": "启用蒙层",
      "title": "是否启用蒙层，也可传入配置改变蒙层样式和填充色",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        },
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": true
    },
    "type": {
      "label": "类型",
      "title": "类型，影响底色与文字颜色",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "primary",
              "value": "primary"
            }
          ]
        }
      ],
      "default": "default"
    },
    "next-button-props": {
      "label": "“下一步”按钮的属性",
      "title": "“下一步”按钮的属性",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "prev-button-props": {
      "label": "“上一步”按钮的属性",
      "title": "“上一步”按钮的属性",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "scroll-into-view-options": {
      "label": "支持当前元素滚动到视窗内",
      "title": "是否支持当前元素滚动到视窗内，也可传入配置指定滚动视窗的相关参数，默认跟随 Tour 的 <code>scrollIntoViewOptions</code> 属性",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        },
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {
        "block": "center"
      }
    },
    "show-close": {
      "label": "显示关闭按钮",
      "title": "是否显示关闭按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "close-icon": {
      "label": "关闭图标",
      "title": "自定义关闭图标，默认Close",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": "Close"
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "主要描述部分"
    },
    {
      "name": "header",
      "title": "头部"
    }
  ],
  "events": [
    {
      "name": "close",
      "title": "关闭引导时的回调函数",
      "params": [
        {
          "type": "Function",
          "details": "() => void"
        }
      ]
    }
  ]
}