{
  "__id__": "radio",
  "name": "单选框",
  "desc": "在一组备选项中进行单选",
  "tag": "el-radio",
  "tagIcon": "radio",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/radio.html",
  "attributes": {
    "vModel": "modelValue",
    "value": {
      "label": "值",
      "title": "单选框的值",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        },
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "version": "2.6.0"
    },
    "label": {
      "label": "的值",
      "title": "单选框的 label 如果<code>value</code>没有值， <code>label</code>则作为<code>value</code>使用",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ]
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用单选框",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "border": {
      "label": "显示边框",
      "title": "是否显示边框",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "size": {
      "label": "尺寸",
      "title": "单选框的尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default"
    },
    "name": {
      "label": "原始 name 属性",
      "title": "原始 <code>name</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义默认内容"
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "绑定值变化时触发的事件",
      "type": "function",
      "details": "(value: string | number | boolean) => void"
    }
  ]
}