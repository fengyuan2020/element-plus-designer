{
  "__id__": "radio",
  "name": "单选框",
  "tag": "el-radio",
  "tagIcon": "radio",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/radio.html",
  "attributes": {
    "vModel": "modelValue",
    "value": {
      "title": "单选框的值",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        },
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "version": "2.6.0"
    },
    "label": {
      "label": "单选框的值",
      "title": "单选框的值",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ]
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用单选框",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "border": {
      "label": "显示边框",
      "title": "是否显示边框",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "size": {
      "label": "尺寸",
      "title": "单选框的尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default"
    },
    "name": {
      "label": "",
      "title": "原始 `name` 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "text-color": {
      "title": "按钮形式的 Radio 激活时的文本颜色",
      "types": [
        {
          "name": "string",
          "tag": "el-color-picker"
        }
      ],
      "default": "#ffffff"
    },
    "fill": {
      "title": "按钮形式的 Radio 激活时的填充色和边框色",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": "#409eff"
    },
    "validate-event": {
      "title": "输入时是否触发表单的校验",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "aria-label": {
      "title": "与 RadioGroup 中的 `aria-label` 属性相同",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true,
      "version": "2.7.2"
    },
    "id": {
      "title": "原生 `id` 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    }
  },
  "events": [
    {
      "name": "change",
      "title": "绑定值变化时触发的事件",
      "type": "Function",
      "fnDes": "(value: string | number | boolean) => void"
    }
  ],
  "slots": [
    {
      "name": "default",
      "title": "自定义默认内容"
    }
  ],
  "desc": "在一组备选项中进行单选"
}