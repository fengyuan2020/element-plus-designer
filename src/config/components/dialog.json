{
  "__id__": "dialog",
  "name": "对话框",
  "desc": "在保留当前页面状态的情况下，告知用户并承载相关操作。",
  "tag": "el-dialog",
  "tagIcon": "dialog",
  "layouts": [
    "containerItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/dialog.html",
  "attributes": {
    "vModel": "modelValue",
    "title": {
      "label": "标题",
      "title": "Dialog 对话框 Dialog 的标题， 也可通过具名 slot （见下表）传入",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "width": {
      "label": "宽度",
      "title": "对话框的宽度，默认值为 50%",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "fullscreen": {
      "label": "全屏",
      "title": "是否为全屏 Dialog",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "top": {
      "label": "margin-top",
      "title": "dialog CSS 中的 margin-top 值，默认为 15vh",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "15vh"
    },
    "modal": {
      "label": "遮罩层",
      "title": "是否需要遮罩层",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "modal-penetrable": {
      "label": "允许穿透遮罩层",
      "title": "是否允许穿透遮罩层。 modal 属性必须为 <code>false</code>。",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "version": "2.10.5"
    },
    "modal-class": {
      "label": "遮罩类名",
      "title": "遮罩的自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "header-class": {
      "label": "header 部分的自定义 class 名",
      "title": "header 部分的自定义 class 名",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "version": "2.9.3"
    },
    "body-class": {
      "label": "body 部分的自定义 class 名",
      "title": "body 部分的自定义 class 名",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "version": "2.9.3"
    },
    "footer-class": {
      "label": "footer 部分的自定义 class 名",
      "title": "footer 部分的自定义 class 名",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "version": "2.9.3"
    },
    "append-to-body": {
      "label": "插入至 body 元素上",
      "title": "Dialog 自身是否插入至 body 元素上。 嵌套的 Dialog 必须指定该属性并赋值为 <code>true</code>",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "append-to": {
      "label": "Dialog 挂载到哪个 DOM 元素 将覆盖 append-to-body",
      "title": "Dialog 挂载到哪个 DOM 元素 将覆盖 <code>append-to-body</code>",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": "body",
      "version": "2.4.3"
    },
    "lock-scroll": {
      "label": "Dialog 出现 body 滚动锁定",
      "title": "是否在 Dialog 出现时将 body 滚动锁定",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "open-delay": {
      "label": "打开延时",
      "title": "dialog 打开的延时时间，单位毫秒",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    },
    "close-delay": {
      "label": "关闭延时",
      "title": "dialog 关闭的延时时间，单位毫秒",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    },
    "close-on-click-modal": {
      "label": "点击modal关闭Dialog",
      "title": "是否可以通过点击 modal 关闭 Dialog",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "close-on-press-escape": {
      "label": "ESC 关闭 Dialog",
      "title": "是否可以通过按下 ESC 关闭 Dialog",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "show-close": {
      "label": "显示关闭按钮",
      "title": "是否显示关闭按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "before-close": {
      "label": "关闭前回调",
      "title": "关闭前的回调，会暂停 Dialog 的关闭. 回调函数内执行 done 参数方法的时候才是真正关闭对话框的时候.",
      "types": [
        {
          "name": "function",
          "details": "(done: DoneFn) => void"
        }
      ]
    },
    "draggable": {
      "label": "可拖拽",
      "title": "为 Dialog 启用可拖拽功能",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "overflow": {
      "label": "拖动范围可以超出可视区",
      "title": "拖动范围可以超出可视区",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "version": "2.5.4"
    },
    "center": {
      "label": "header 和 footer 居中",
      "title": "是否让 Dialog 的 header 和 footer 部分居中排列",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "align-center": {
      "label": "水平垂直对齐",
      "title": "是否水平垂直对齐对话框",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "version": "2.2.16"
    },
    "destroy-on-close": {
      "label": "关闭销毁元素",
      "title": "当关闭 Dialog 时，销毁其中的元素",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "close-icon": {
      "label": "关闭图标",
      "title": "自定义关闭图标，默认 Close",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": "Close"
    },
    "z-index": {
      "label": "z-index",
      "title": "和原生的 CSS 的 z-index 相同，改变 z 轴的顺序",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "header-aria-level": {
      "label": "header 的 aria-level 属性",
      "title": "header 的 <code>aria-level</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": "2",
      "a11y": true
    },
    "transition": {
      "label": "动画的自定义过渡配置。 可以是一个字符串",
      "title": "对话框动画的自定义过渡配置。 可以是一个字符串（过渡名称），也可以是一个包含 Vue 过渡属性的对象。",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        },
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": "dialog-fade",
      "version": "2.10.5"
    },
    "custom-class": {
      "label": "自定义类名",
      "title": "Dialog 的自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "deprecated": true
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "对话框的默认内容"
    },
    {
      "name": "header",
      "title": "对话框标题的内容；会替换标题部分，但不会移除关闭按钮。"
    },
    {
      "name": "footer",
      "title": "Dialog 按钮操作区的内容"
    },
    {
      "name": "title",
      "title": "与 header 作用相同 请使用 header",
      "deprecated": true
    }
  ],
  "events": [
    {
      "name": "open",
      "title": "Dialog 打开的回调",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "opened",
      "title": "Dialog 打开动画结束时的回调",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "close",
      "title": "Dialog 关闭的回调",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "closed",
      "title": "Dialog 关闭动画结束时的回调",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "open-auto-focus",
      "title": "输入焦点聚焦在 Dialog 内容时的回调",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "close-auto-focus",
      "title": "输入焦点从 Dialog 内容失焦时的回调",
      "type": "function",
      "details": "() => void"
    }
  ],
  "exposes": [
    {
      "name": "resetPosition",
      "title": "重置位置",
      "type": "function",
      "details": "() => void",
      "version": "2.8.1"
    },
    {
      "name": "handleClose",
      "title": "关闭对话框",
      "type": "function",
      "details": "() => void",
      "version": "2.9.8"
    }
  ]
}