{
  "__id__": "image-viewer",
  "name": "图片",
  "desc": "图片容器，在保留所有原生 img 的特性下，支持懒加载，自定义占位、加载失败等",
  "tag": "el-image-viewer",
  "tagIcon": "image",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/image.html",
  "attributes": {
    "url-list": {
      "label": "用于预览的图片链接列表",
      "title": "用于预览的图片链接列表",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "z-index": {
      "label": "预览时遮罩层的 z-index",
      "title": "预览时遮罩层的 z-index",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "initial-index": {
      "label": "初始预览图像索引",
      "title": "初始预览图像索引，小于 <code>url-list</code> 的长度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    },
    "infinite": {
      "label": "可以无限循环预览",
      "title": "是否可以无限循环预览",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "hide-on-click-modal": {
      "label": "可以通过点击遮罩层关闭预览",
      "title": "是否可以通过点击遮罩层关闭预览",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "teleported": {
      "label": "image 自身是否插入至 body 元素上。 嵌套的父元素属性会发生修改时应该将此属性设置为 true",
      "title": "image 自身是否插入至 body 元素上。 嵌套的父元素属性会发生修改时应该将此属性设置为 <code>true</code>",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "zoom-rate": {
      "label": "图像查看器缩放事件的缩放速率。",
      "title": "图像查看器缩放事件的缩放速率。",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 1.2,
      "version": "2.2.27"
    },
    "min-scale": {
      "label": "图像查看器缩放事件的最小缩放比例",
      "title": "图像查看器缩放事件的最小缩放比例",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0.2,
      "version": "2.4.0"
    },
    "max-scale": {
      "label": "图像查看器缩放事件的最大缩放比例",
      "title": "图像查看器缩放事件的最大缩放比例",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 7,
      "version": "2.4.0"
    },
    "close-on-press-escape": {
      "label": "可以通过按下 ESC 关闭 Image Viewer",
      "title": "是否可以通过按下 ESC 关闭 Image Viewer",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "show-progress": {
      "label": "显示预览图片的进度条内容",
      "title": "是否显示预览图片的进度条内容",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "version": "2.9.4"
    }
  },
  "events": [
    {
      "name": "close",
      "title": "当点击 X 按钮或者在<code>hide-on-click-modal</code>为 true 时点击遮罩层时触发",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "switch",
      "title": "切换图像时触发。",
      "type": "function",
      "details": "(index: number) => void"
    },
    {
      "name": "rotate ",
      "version": "2.3.13",
      "title": "旋转图像时触发。",
      "type": "function",
      "details": "(deg: number) => void"
    }
  ],
  "exposes": [
    {
      "name": "setActiveItem",
      "title": "手动切换图片",
      "type": "function",
      "details": "(index: number) => void"
    }
  ]
}