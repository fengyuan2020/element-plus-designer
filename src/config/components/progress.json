{
  "__id__": "progress",
  "name": "进度条",
  "desc": "用于展示操作进度，告知用户当前状态和预期。",
  "tag": "el-progress",
  "tagIcon": "progress",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/progress.html",
  "attributes": {
    "percentage": {
      "label": "百分比",
      "title": "percentage",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0,
      "required": true
    },
    "type": {
      "label": "类型",
      "title": "进度条类型",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "line",
              "value": "line"
            },
            {
              "label": "circle",
              "value": "circle"
            },
            {
              "label": "dashboard",
              "value": "dashboard"
            }
          ]
        }
      ],
      "default": "line"
    },
    "stroke-width": {
      "label": "进宽度",
      "title": "进度条的宽度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 6
    },
    "text-inside": {
      "label": "文字内置",
      "title": "进度条显示文字内置在进度条内（仅 <code>type</code> 为 'line' 时可用）",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "status": {
      "label": "当前状态",
      "title": "进度条当前状态",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "success",
              "value": "success"
            },
            {
              "label": "exception",
              "value": "exception"
            },
            {
              "label": "warning",
              "value": "warning"
            }
          ]
        }
      ]
    },
    "indeterminate": {
      "label": "动画进度条",
      "title": "是否为动画进度条",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "duration": {
      "label": "速度",
      "title": "控制动画进度条速度和条纹进度条流动速度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 3
    },
    "color": {
      "label": "背景色",
      "title": "进度条背景色 （会覆盖 <code>status</code> 状态颜色）",
      "types": [
        {
          "name": "string",
          "tag": "el-color-picker"
        },
        {
          "name": "function",

          "details": "(percentage: number) => string",
          "desc": "(percentage: number) => string` / ^[Array]`{ color: string; percentage: number }[]"
        },
        {
          "name": "array",
          "tag": "object-array-editor",
          "props": {
            "keys": [
              {
                "key": "color",
                "label": "颜色",
                "tag": "el-color-picker"
              },
              {
                "key": "percentage",
                "label": "百分比",
                "tag": "el-input-number"
              }
            ]
          }
        }
      ]
    },
    "width": {
      "label": "环形画布宽度",
      "title": "环形进度条画布宽度（只在 type 为 circle 或 dashboard 时可用）",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 126
    },
    "show-text": {
      "label": "显示文字内容",
      "title": "是否显示进度条文字内容",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "stroke-linecap": {
      "label": "两端形状",
      "title": "circle/dashboard 类型路径两端的形状",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "butt",
              "value": "butt"
            },
            {
              "label": "round",
              "value": "round"
            },
            {
              "label": "square",
              "value": "square"
            }
          ]
        }
      ],
      "default": "round"
    },
    "format": {
      "label": "指定进度条文字内容",
      "title": "指定进度条文字内容",
      "types": [
        {
          "name": "function",
          "details": "(percentage: number) => string"
        }
      ]
    },
    "striped": {
      "label": "在进度条上增加条纹",
      "title": "在进度条上增加条纹",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "version": "2.3.4"
    },
    "striped-flow": {
      "label": "让进度条上的条纹流动起来",
      "title": "让进度条上的条纹流动起来",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "version": "2.3.4"
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义内容",
      "type": "object",
      "details": "{ percentage: number }"
    }
  ]
}