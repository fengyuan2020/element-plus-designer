{
  "__id__": "mention",
  "name": "提及",
  "desc": "用于在输入中提及某人或某事。",
  "tag": "el-mention",
  "tagIcon": "mention",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/mention.html",
  "attributes": {
    "vModel": "modelValue",
    "placeholder": "请输入",
    "options": {
      "label": "提及选项列表",
      "title": "提及选项列表",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "hide": true,
      "default": []
    },
    "props": {
      "label": "options 的配置",
      "title": "options 的配置",
      "types": [
        {
          "name": "object",
          "tag": "fixed-keys-object-editor",
          "props": {
            "keys": [
              {
                "name": "label",
                "title": "标签",
                "tag": "el-input"
              },
              {
                "name": "value",
                "title": "值",
                "tag": "el-input"
              },
              {
                "name": "disabled",
                "title": "禁用",
                "tag": "el-input"
              }
            ]
          }
        }
      ],
      "default": {
        "value": "value",
        "label": "label",
        "disabled": "disabled"
      },
      "version": "2.11.3",
      "keep": true
    },
    "prefix": {
      "label": "触发前缀",
      "title": "触发字段的前缀。 字符串长度必须且只能为 1",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        },
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "default": "@"
    },
    "split": {
      "label": "拆分字符",
      "title": "用于拆分提及的字符。 字符串长度必须且只能为 1",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": ""
    },
    "filter-option": {
      "label": "选项逻辑",
      "title": "定制筛选器选项逻辑",
      "types": [
        {
          "name": "false",
          "tag": ""
        },
        {
          "name": "function",
          "details": "(pattern: string, option: MentionOption) => boolean"
        }
      ]
    },
    "placement": {
      "label": "弹出位置",
      "title": "设置弹出位置",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": "bottom"
    },
    "show-arrow": {
      "label": "下拉菜单是否有箭头",
      "title": "下拉菜单的内容是否有箭头",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "offset": {
      "label": "下拉面板偏移量",
      "title": "下拉面板偏移量",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    },
    "whole": {
      "label": "整体删除",
      "title": "当退格键被按下做删除操作时，是否将提及部分作为整体删除",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "check-is-whole": {
      "label": "是否整体删除",
      "title": "当退格键被按下做删除操作时，检查是否将提及部分作为整体删除",
      "types": [
        {
          "name": "function",
          "details": "(pattern: string, prefix: string) => boolean"
        }
      ]
    },
    "loading": {
      "label": "加载状态",
      "title": "提及的下拉面板是否处于加载状态",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "popper-class": {
      "label": "浮层类名",
      "title": "自定义浮层类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "popper-options": {
      "label": "popper.js参数",
      "title": "<a href=\"https://popper.js.org/docs/v2/\">popper.js</a> 参数",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "keep": true
    },
    "type": {
      "label": "类型",
      "title": "类型",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "text",
      "keep": true
    },
    "maxlength": {
      "label": "最大输入长度",
      "title": "同原生 <code>maxlength</code> 属性",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "minlength": {
      "label": "最小输入长度",
      "title": "原生属性，最小输入长度",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "show-word-limit": {
      "label": "统计字数",
      "title": "是否显示统计字数, 只在 <code>type</code> 为 'text' 或 'textarea' 的时候生效",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "switch": [
        "show-password",
        false
      ]
    },
    "clearable": {
      "label": "可清空",
      "title": "是否显示清除按钮，只有当 <code>type</code> 不是 textarea时生效",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "clear-icon": {
      "label": "自定义清除图标",
      "title": "自定义清除图标",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": "CircleClose",
      "version": "2.11.0"
    },
    "formatter": {
      "label": "输入值的格式",
      "title": "指定输入值的格式。(只有当 <code>type</code> 是\"text\"时才能工作)",
      "types": [
        {
          "name": "function",
          "details": "(value: string | number) => string"
        }
      ],
      "switch": [
        "type",
        "text"
      ],
      "hide": true
    },
    "parser": {
      "label": "提取的值",
      "title": "指定从格式化器输入中提取的值。(仅当 <code>type</code> 是\"text\"时才起作用)",
      "types": [
        {
          "name": "function",
          "details": "(value: string) => string"
        }
      ],
      "switch": [
        "type",
        "text"
      ],
      "hide": true
    },
    "show-password": {
      "label": "显示切换密码图标",
      "title": "是否显示切换密码图标",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "hide": true
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "size": {
      "label": "尺寸",
      "title": "输入框尺寸，只在 <code>type</code> 不为 'textarea' 时有效",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "switch": [
        "type",
        "text"
      ],
      "default": "default"
    },
    "prefix-icon": {
      "label": "前缀图标",
      "title": "自定义前缀图标",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "switch": [
        "type",
        "text"
      ]
    },
    "suffix-icon": {
      "label": "后缀图标",
      "title": "自定义后缀图标",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "switch": [
        "type",
        "text"
      ]
    },
    "rows": {
      "label": "输入框行数",
      "title": "输入框行数，仅 <code>type</code> 为 'textarea' 时有效",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 2,
      "switch": [
        "type",
        "textarea"
      ]
    },
    "autosize": {
      "label": "高度自适应",
      "title": "textarea 高度是否自适应，仅 <code>type</code> 为 'textarea' 时生效。 可以接受一个对象，比如: <code>{ minRows: 2, maxRows: 6 }</code>",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        },
        {
          "name": "object",
          "tag": "fixed-keys-object-editor",
          "props": {
            "keys": [
              {
                "name": "minRows",
                "tag": "el-input-number"
              },
              {
                "name": "maxRows",
                "tag": "el-input-number"
              }
            ]
          }
        }
      ],
      "default": false,
      "switch": [
        "type",
        "textarea"
      ]
    },
    "autocomplete": {
      "label": "autocomplete",
      "title": "原生 <code>autocomplete</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "off",
      "hide": true
    },
    "name": {
      "label": "name",
      "title": "等价于原生 input <code>name</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "hide": true
    },
    "readonly": {
      "label": "只读",
      "title": "原生 <code>readonly</code> 属性，是否只读",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "max": {
      "label": "max",
      "title": "原生 max 属性，设置最大值",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "hide": true,
      "keep": true
    },
    "min": {
      "label": "min",
      "title": "原生属性，设置最小值",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "hide": true,
      "keep": true
    },
    "step": {
      "label": "step",
      "title": "原生属性，设置输入字段的合法数字间隔",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "hide": true,
      "keep": true
    },
    "resize": {
      "label": "缩放",
      "title": "控制是否能被用户缩放",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "none",
              "value": "none"
            },
            {
              "label": "both",
              "value": "both"
            },
            {
              "label": "horizontal",
              "value": "horizontal"
            },
            {
              "label": "vertical",
              "value": "vertical"
            }
          ]
        }
      ],
      "hide": true
    },
    "autofocus": {
      "label": "自动获取焦点",
      "title": "原生属性，自动获取焦点",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "form": {
      "label": "form",
      "title": "原生属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "aria-label": {
      "label": "aria-label 属性",
      "title": "等价于原生 input <code>aria-label</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true,
      "version": "2.7.2"
    },
    "tabindex": {
      "label": "tabindex",
      "title": "输入框的 tabindex",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ],
      "hide": true
    },
    "validate-event": {
      "label": "表单校验",
      "title": "输入时是否触发表单的校验",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "input-style": {
      "label": "style",
      "title": "input 元素或 textarea 元素的 style",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "object",
          "tag": "style-editor"
        }
      ],
      "default": {}
    },
    "label": {
      "label": "标签文本",
      "title": "等价于原生 input <code>aria-label</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "a11y": true,
      "deprecated": true
    },
    "inputmode": {
      "label": "inputmode 属性",
      "title": "等价于原生 input <code>inputmode</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "version": "2.10.3"
    }
  },
  "slots": [
    {
      "name": "label",
      "title": "自定义标签内容",
      "type": "object",
      "details": "{ item: MentionOption, index: number }"
    },
    {
      "name": "loading",
      "title": "自定义 loading内容"
    },
    {
      "name": "header",
      "title": "下拉列表顶部的内容"
    },
    {
      "name": "footer",
      "title": "下拉列表底部的内容"
    },
    {
      "name": "prefix",
      "title": "输入框头部内容，只对非 <code>type=\"textarea\"</code> 有效"
    },
    {
      "name": "suffix",
      "title": "输入框尾部内容，只对非 <code>type=\"textarea\"</code> 有效"
    },
    {
      "name": "prepend",
      "title": "输入框前置内容，只对非 <code>type=\"textarea\"</code> 有效"
    },
    {
      "name": "append",
      "title": "输入框后置内容，只对非 <code>type=\"textarea\"</code> 有效"
    }
  ],
  "events": [
    {
      "name": "search",
      "title": "按下触发字段时触发",
      "type": "function",
      "details": "(pattern: string, prefix: string) => void"
    },
    {
      "name": "select",
      "title": "当用户选择选项时触发",
      "type": "function",
      "details": "(option: MentionOption, prefix: string) => void"
    },
    {
      "name": "whole-remove",
      "title": "当整个 mention 被移除，且 <code>whole</code> 为 <code>true</code> 或 <code>check-is-whole</code> 为 <code>true</code> 时触发。",
      "type": "function",
      "details": "(pattern: string, prefix: string) => void",
      "version": "2.10.4"
    },
    {
      "name": "prefix",
      "title": "输入框头部内容，只对非 <code>type=\"textarea\"</code> 有效"
    },
    {
      "name": "suffix",
      "title": "输入框尾部内容，只对非 <code>type=\"textarea\"</code> 有效"
    },
    {
      "name": "prepend",
      "title": "输入框前置内容，只对非 <code>type=\"textarea\"</code> 有效"
    },
    {
      "name": "append",
      "title": "输入框后置内容，只对非 <code>type=\"textarea\"</code> 有效"
    }
  ],
  "exposes": [
    {
      "name": "input",
      "title": "el-input 组件实例",
      "type": "object",
      "details": "Ref<InputInstance \\| null>"
    },
    {
      "name": "tooltip",
      "title": "el-tooltip 组件实例",
      "type": "object",
      "details": "Ref<TooltipInstance \\| null>"
    },
    {
      "name": "dropdownVisible",
      "title": "tooltip 显示状态",
      "type": "object",
      "details": "ComputedRef<boolean>",
      "version": "2.8.5"
    }
  ],
  "data": {
    "name": "options",
    "label": "选项",
    "source": "static",
    "inProps": true,
    "static": {
      "ref": true,
      "type": {
        "name": "array",
        "tag": "object-array-editor",
        "props": {
          "keys": [
            {
              "key": "value",
              "label": "值",
              "tag": "string-number-input"
            },
            {
              "key": "label",
              "label": "标签",
              "tag": "string-number-input"
            }
          ]
        }
      },
      "default": [
        {
          "label": "提及1",
          "value": "提及1"
        },
        {
          "label": "提及2",
          "value": "提及2"
        }
      ]
    },
    "dynamic": {
      "url": "array.json",
      "method": "get",
      "dataKey": ""
    }
  }
}