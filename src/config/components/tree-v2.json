{
  "__id__": "tree-v2",
  "name": "虚拟树形",
  "desc": "不论你的数据量多大，虚拟树都能毫无压力地处理。",
  "tag": "el-tree-v2",
  "tagIcon": "tree",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/tree-v2.html",
  "attributes": {
    "data": {
      "label": "展示数据",
      "title": "展示数据",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "hide": true,
      "default": []
    },
    "empty-text": {
      "label": "内容为空的时候展示的文本",
      "title": "内容为空的时候展示的文本",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "props": {
      "label": "配置选项",
      "title": "配置选项",
      "types": [
        {
          "name": "object",
          "tag": "fixed-keys-object-editor",
          "props": {
            "keys": [
              {
                "name": "label",
                "title": "指定子树为节点对象的某个属性值",
                "tag": "el-input"
              },
              {
                "name": "value",
                "title": "单单树形控件用不上，先放着",
                "tag": "el-input"
              },
              {
                "name": "children",
                "title": "指定子树为节点对象的某个属性值",
                "tag": "el-input"
              },
              {
                "name": "disabled",
                "title": "指定节点选择框是否禁用为节点对象的某个属性值",
                "tag": "el-input"
              }
            ]
          }
        }
      ],
      "default": {
        "value": "id",
        "label": "label",
        "children": "children",
        "disabled": "disabled"
      },
      "keep": true
    },
    "highlight-current": {
      "label": "高亮选中",
      "title": "是否高亮当前选中节点",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "expand-on-click-node": {
      "label": "点击时展开或收缩",
      "title": "是否在点击节点的时候展开或者收缩节点， 默认值为 true，如果为 false，则只有点箭头图标的时候才会展开或者收缩节点",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "check-on-click-node": {
      "label": "点击选中",
      "title": "是否在点击节点的时候选中节点，默认值为 false，即只有在点击复选框时才会选中节点",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "check-on-click-leaf": {
      "label": "点击叶节点",
      "title": "点击叶节点(最后一个子节点)时是否检查或取消节点",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "version": "2.9.6"
    },
    "default-expanded-keys": {
      "label": "默认展开节点",
      "title": "默认展开的节点的 key 的数组",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "keep": true
    },
    "show-checkbox": {
      "label": "可被选择",
      "title": "节点是否可被选择",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "check-strictly": {
      "label": "遵循父子不互相关联",
      "title": "在显示复选框的情况下，是否严格的遵循父子不互相关联的做法，默认为 false",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "default-checked-keys": {
      "label": "默认勾选节点",
      "title": "默认勾选的节点的 key 的数组",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "keep": true
    },
    "current-node-key": {
      "label": "当前选中节点",
      "title": "当前选中的节点",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "filter-method": {
      "label": "对树节点进行筛选时执行的方法",
      "title": "对树节点进行筛选时执行的方法，返回 true 表示这个节点可以显示， 返回 <code>false</code> 则表示这个节点会被隐藏",
      "types": [
        {
          "name": "function",
          "details": "(query: string, data: TreeNodeData, node: TreeNode) => boolean"
        }
      ],
      "hide": true
    },
    "indent": {
      "label": "水平缩进",
      "title": "相邻级节点间的水平缩进，单位为像素",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 16
    },
    "icon": {
      "label": "节点图标",
      "title": "自定义树节点图标组件",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "item-size": {
      "label": "自定义树节点的高度",
      "title": "自定义树节点的高度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 26,
      "version": "2.2.33"
    },
    "scrollbar-always-on": {
      "label": "总是显示滚动条",
      "title": "总是显示滚动条",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "version": "2.10.4"
    },
    "height": {
      "label": "高度",
      "title": "tree 的高度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 200
    }
  },
  "slots": [
    {
      "name": "-",
      "title": "自定义树节点的内容。 作用域参数为 <code>{ node: TreeNode, data: TreeNodeData }</code>"
    },
    {
      "name": "empty",
      "title": "当数据为空时自定义的内容",
      "version": "2.9.0"
    }
  ],
  "events": [
    {
      "name": "node-click",
      "title": "当节点被点击的时候触发",
      "type": "(data: TreeNodeData, node: TreeNode, e: MouseEvent)"
    },
    {
      "name": "node-drop",
      "title": "拖放到节点时触发器",
      "type": "(data: TreeNodeData, node: TreeNode, e: DragEvent)",
      "version": "2.8.3"
    },
    {
      "name": "node-contextmenu",
      "title": "当某一节点被鼠标右键点击时会触发该事件",
      "type": "(e: Event, data: TreeNodeData, node: TreeNode)"
    },
    {
      "name": "check-change",
      "title": "当复选框被点击的时候触发",
      "type": "(data: TreeNodeData, checked: boolean)"
    },
    {
      "name": "check",
      "title": "点击节点复选框之后触发",
      "type": "(data: TreeNodeData, info: { checkedKeys: TreeKey[],checkedNodes: TreeData, halfCheckedKeys: TreeKey[], halfCheckedNodes: TreeData,})"
    },
    {
      "name": "current-change",
      "title": "当前选中节点变化时触发的事件",
      "type": "(data: TreeNodeData, node: TreeNode)"
    },
    {
      "name": "node-expand",
      "title": "节点被展开时触发的事件",
      "type": "(data: TreeNodeData, node: TreeNode)"
    },
    {
      "name": "node-collapse",
      "title": "节点被关闭时触发的事件",
      "type": "(data: TreeNodeData, node: TreeNode)"
    }
  ],
  "exposes": [
    {
      "title": "过滤所有树节点，过滤后的节点将被隐藏",
      "params": [
        "<code>(query: string)</code>"
      ]
    },
    {
      "title": "如果节点可以被选中，(<code>show-checkbox</code> 为 <code>true</code>), 本方法将返回当前选中节点的数组",
      "params": [
        "<code>(leafOnly: boolean)</code>"
      ]
    },
    {
      "title": "若节点可用被选中 (<code>show-checkbox</code> 为 <code>true</code>), 它将返回当前选中节点 key 的数组",
      "params": [
        "<code>(leafOnly: boolean)</code>"
      ]
    },
    {
      "title": "通过 keys 设置目前勾选的节点",
      "params": [
        "<code>(keys: TreeKey[])</code>"
      ]
    },
    {
      "title": "通过 key 设置某个节点的勾选状态",
      "params": [
        "<code>(key: TreeKey, checked: boolean)</code>"
      ]
    },
    {
      "title": "设置当前展开的节点",
      "params": [
        "<code>(keys: TreeKey[])</code>"
      ]
    },
    {
      "title": "如果节点可用被选中 (<code>show-checkbox</code> 为 <code>true</code>), 它将返回当前半选中的节点组成的数组",
      "params": ":::"
    },
    {
      "title": "若节点可被选中(<code>show-checkbox</code> 为 <code>true</code>)，则返回目前半选中的节点的 key 所组成的数组",
      "params": ":::"
    },
    {
      "title": "获取当前被选中节点的 key，若没有节点被选中则返回 <code>undefined</code>",
      "params": ":::"
    },
    {
      "title": "获取当前被选中节点的 data，若没有节点被选中则返回 <code>undefined</code>",
      "params": ":::"
    },
    {
      "title": "通过 key 设置某个节点的当前选中状态",
      "params": [
        "<code>(key: TreeKey)</code>"
      ]
    },
    {
      "title": "通过 key 或 data 获取节点",
      "params": [
        "<code>(data: TreeKey | TreeNodeData)</code>"
      ]
    },
    {
      "title": "展开指定节点",
      "params": [
        "<code>(node: TreeNode)</code>"
      ]
    },
    {
      "title": "折叠指定节点",
      "params": [
        "<code>(node: TreeNode)</code>"
      ]
    },
    {
      "title": "当数据量非常庞大的时候，总是使用响应式数据将导致性能表现不佳，所以我们提供一种显式设置的方式来避免此种情况",
      "params": [
        "<code>(data: TreeData)</code>"
      ]
    },
    {
      "title": "滚动到给定位置",
      "params": [
        "<code>(offset: number)</code>"
      ]
    },
    {
      "title": "使用给定的滚动策略滚动至指定位置",
      "params": [
        "<code>(key: TreeKey, strategy?: auto | smart | center | start | end)</code>"
      ]
    }
  ],
  "data": {
    "name": "data",
    "label": "展示数据",
    "source": "static",
    "static": {
      "ref": true,
      "type": {
        "name": "array",
        "tag": "tree-editor"
      },
      "default": [
        {
          "id": "1",
          "label": "选项1",
          "children": [
            {
              "id": "2",
              "label": "选项1-1"
            }
          ]
        }
      ]
    },
    "dynamic": {
      "url": "https://www.fastmock.site/mock/c9e1a1c3cefb84da4306dcf9fadde652/ele-plus-desinger/tree-v2-data",
      "method": "get",
      "dataKey": ""
    }
  }
}