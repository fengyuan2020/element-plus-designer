{
  "__id__": "descriptions",
  "name": "描述列表",
  "tag": "el-descriptions",
  "tagIcon": "menu",
  "layouts": [
    "rawItem"
  ],
  "childTag": [
    "el-descriptions-item"
  ],
  "document": "https://element-plus.org/zh-CN/component/descriptions.html",
  "attributes": {
    "border": {
      "label": "边框",
      "title": "是否带有边框",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "column": {
      "label": "每行数量",
      "title": "一行 Descriptions Item 的数量",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 3
    },
    "direction": {
      "label": "排列的方向",
      "title": "排列的方向",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "vertical",
              "value": "vertical"
            },
            {
              "label": "horizontal",
              "value": "horizontal"
            }
          ]
        }
      ],
      "default": "horizontal"
    },
    "size": {
      "label": "尺寸",
      "title": "列表的尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default"
    },
    "title": {
      "label": "标题文本",
      "title": "标题文本，显示在左上方",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "extra": {
      "label": "操作区文本",
      "title": "操作区文本，显示在右上方",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "label-width": {
      "label": "标签宽度",
      "title": "每一列的标签宽度",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ],
      "version": "2.8.8"
    },
    "label": {
      "title": "标签文本",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "span": {
      "title": "列的数量",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 1
    },
    "rowspan": {
      "title": "单元格应该跨越的行数",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 1,
      "version": "2.8.1"
    },
    "width": {
      "title": "列的宽度，不同行相同列的宽度按最大值设定（如无 `border` ，宽度包含标签与内容）",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "min-width": {
      "title": "列的最小宽度，与 `width` 的区别是 `width` 是固定的，`min-width` 会把剩余宽度按比例分配给设置了 `min-width` 的列（如无 `border`，宽度包含标签与内容）",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "align": {
      "title": "列的内容对齐方式（如无 `border`，对标签和内容均生效）",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "left",
              "value": "left"
            },
            {
              "label": "center",
              "value": "center"
            },
            {
              "label": "right",
              "value": "right"
            }
          ]
        }
      ],
      "default": "left"
    },
    "label-align": {
      "title": "列的标签对齐方式，若不设置该项，则使用内容的对齐方式（如无 `border`，请使用 `align` 参数）",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "left",
              "value": "left"
            },
            {
              "label": "center",
              "value": "center"
            },
            {
              "label": "right",
              "value": "right"
            }
          ]
        }
      ]
    },
    "class-name": {
      "title": "列的内容自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "label-class-name": {
      "title": "column label custom class name",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义默认内容",
      "subtags": [
        "descriptions-item"
      ]
    },
    {
      "name": "title",
      "title": "自定义标题，显示在左上方"
    },
    {
      "name": "extra",
      "title": "自定义操作区，显示在右上方"
    }
  ],
  "desc": "列表形式展示多个字段。"
}