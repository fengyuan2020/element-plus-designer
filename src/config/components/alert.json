{
  "__id__": "alert",
  "name": "提示",
  "desc": "用于页面中展示重要的提示信息。",
  "tag": "el-alert",
  "tagIcon": "alert",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/result.html",
  "attributes": {
    "title": {
      "label": "标题",
      "title": "Alert 标题。",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "type": {
      "label": "类型",
      "title": "Alert 类型。",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "primary",
              "value": "primary"
            },
            {
              "label": "success",
              "value": "success"
            },
            {
              "label": "warning",
              "value": "warning"
            },
            {
              "label": "info",
              "value": "info"
            },
            {
              "label": "error",
              "value": "error"
            }
          ]
        }
      ],
      "default": "info"
    },
    "description": {
      "label": "描述文本",
      "title": "描述性文本",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "closable": {
      "label": "可以关闭",
      "title": "是否可以关闭",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "center": {
      "label": "文字居中",
      "title": "文字是否居中",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "close-text": {
      "label": "关闭按钮文本",
      "title": "自定义关闭按钮文本",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "show-icon": {
      "label": "显示类型图标",
      "title": "是否显示类型图标",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "effect": {
      "label": "主题样式",
      "title": "主题样式",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "light",
              "value": "light"
            },
            {
              "label": "dark",
              "value": "dark"
            }
          ]
        }
      ],
      "default": "light"
    },
    "show-after": {
      "label": "在触发后多久显示内容",
      "title": "在触发后多久显示内容，单位毫秒",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0,
      "version": "2.10.0"
    },
    "hide-after": {
      "label": "延迟关闭",
      "title": "延迟关闭，单位毫秒",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 200,
      "version": "2.10.0"
    },
    "auto-close": {
      "label": "出现后自动隐藏延时",
      "title": "alert 出现后自动隐藏延时，单位毫秒",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0,
      "version": "2.10.0"
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "Alert 内容描述"
    },
    {
      "name": "title",
      "title": "标题的内容"
    },
    {
      "name": "icon",
      "title": "icon 的内容",
      "version": "2.9.7"
    }
  ],
  "events": [
    {
      "name": "open ",
      "version": "2.10.0",
      "title": "开启 Alert 时触发的事件",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "close",
      "title": "关闭 Alert 时触发的事件",
      "type": "function",
      "details": "(event?: Event) => void"
    }
  ]
}