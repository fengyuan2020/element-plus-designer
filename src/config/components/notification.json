{
  "__id__": "notification",
  "name": "",
  "tag": "el-notification",
  "tagIcon": "notification",
  "layouts": [
    "containerItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/notification.html",
  "attributes": {
    "title": {
      "label": "title",
      "title": "title",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "message": {
      "label": "通知栏正文内容",
      "title": "通知栏正文内容",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        },
        {
          "name": "VNode"
        },
        {
          "name": "function",
          "details": "() => VNode"
        }
      ]
    },
    "dangerouslyUseHTMLString": {
      "label": "将 message 属性作为 HTML 片段处理",
      "title": "是否将 message 属性作为 HTML 片段处理",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "type": {
      "label": "通知的类型",
      "title": "通知的类型",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "primary",
              "value": "primary"
            },
            {
              "label": "success",
              "value": "success"
            },
            {
              "label": "warning",
              "value": "warning"
            },
            {
              "label": "info",
              "value": "info"
            },
            {
              "label": "error",
              "value": "error"
            }
          ]
        }
      ]
    },
    "icon": {
      "label": "自定义图标。 若设置了 type",
      "title": "自定义图标。 若设置了 <code>type</code>，则 <code>icon</code> 会被覆盖",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "customClass": {
      "label": "自定义类名",
      "title": "自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "duration": {
      "label": "显示时间",
      "title": "显示时间, 单位为毫秒。 值为 0 则不会自动关闭",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 4500
    },
    "position": {
      "label": "自定义弹出位置",
      "title": "自定义弹出位置",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "top-right",
              "value": "top-right"
            },
            {
              "label": "top-left",
              "value": "top-left"
            },
            {
              "label": "bottom-right",
              "value": "bottom-right"
            },
            {
              "label": "bottom-left",
              "value": "bottom-left"
            }
          ]
        }
      ],
      "default": "top-right"
    },
    "showClose": {
      "label": "显示关闭按钮",
      "title": "是否显示关闭按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "onClose": {
      "label": "关闭时的回调函数",
      "title": "关闭时的回调函数",
      "types": [
        {
          "name": "function",
          "details": "() => void"
        }
      ]
    },
    "onClick": {
      "label": "点击 Notification 时的回调函数",
      "title": "点击 Notification 时的回调函数",
      "types": [
        {
          "name": "function",
          "details": "() => void"
        }
      ]
    },
    "offset": {
      "label": "相对屏幕顶部的偏移量 偏移的距离",
      "title": "相对屏幕顶部的偏移量 偏移的距离，在同一时刻，所有的 Notification 实例应当具有一个相同的偏移量",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    },
    "appendTo": {
      "label": " notification 的根元素",
      "title": "设置 notification 的根元素，默认为 <code>document.body</code>",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "zIndex": {
      "label": "初始 zIndex",
      "title": "初始 zIndex",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    },
    "closeIcon": {
      "label": "自定义关闭图标",
      "title": "自定义关闭图标",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        },
        {
          "name": "Component"
        }
      ],
      "default": "Close",
      "version": "2.9.8"
    }
  },
  "slots": [],
  "exposes": [
    {
      "name": "close",
      "title": "关闭当前的 Notification",
      "type": "function",
      "details": "() => void"
    }
  ]
}