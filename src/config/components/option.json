        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "disabled": {
      "label": "",
      "title": "是否禁用该选项",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "默认插槽内容"
