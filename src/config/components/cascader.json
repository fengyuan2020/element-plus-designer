{
  "__id__": "cascader",
  "name": "级联选择",
  "desc": "当一个数据集合有清晰的层级结构时，可通过级联选择器逐级查看并选择。",
  "tag": "el-cascader",
  "tagIcon": "cascader",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/cascader.html",
  "attributes": {
    "vModel": "modelValue",
    "placeholder": "请选择",
    "options": {
      "label": "数据源",
      "title": "可选项数据源，键名可通过 Props 属性配置",
      "ref": true,
      "types": [
        {
          "name": "array",
          "tag": "tree-editor"
        }
      ],
      "default": [],
      "hide": true
    },
    "props": {
      "label": "配置选项",
      "title": "配置选项",
      "types": [
        {
          "name": "object",
          "tag": "fixed-keys-object-editor",
          "props": {
            "keys": [
              {
                "name": "expandTrigger",
                "type": "string",
                "title": "次级菜单的展开方式",
                "tag": "el-radio-group",
                "config": {
                  "optionType": "button"
                },
                "options": [
                  {
                    "label": "click",
                    "value": "click"
                  },
                  {
                    "label": "hover",
                    "value": "hover"
                  }
                ]
              },
              {
                "name": "multiple",
                "title": "是否多选",
                "tag": "el-switch"
              },
              {
                "name": "checkStrictly",
                "title": "是否严格的遵守父子节点不互相关联",
                "tag": "el-switch"
              },
              {
                "name": "emitPath",
                "title": "在选中节点改变时，是否返回由该节点所在的各级菜单的值所组成的数组，若设置 false，则只返回该节点的值",
                "tag": "el-switch"
              },
              {
                "name": "lazy",
                "title": "是否动态加载子节点，需与 lazyLoad 方法结合使用",
                "tag": "el-switch"
              },
              {
                "name": "label",
                "title": "指定选项标签为选项对象的某个属性值",
                "tag": "el-input"
              },
              {
                "name": "value",
                "title": "指定选项的值为选项对象的某个属性值",
                "tag": "el-input"
              },
              {
                "name": "children",
                "title": "指定选项的子选项为选项对象的某个属性值",
                "tag": "el-input"
              },
              {
                "name": "disabled",
                "title": "指定选项的禁用为选项对象的某个属性值",
                "tag": "el-input"
              },
              {
                "name": "leaf",
                "title": "指定选项的叶子节点的标志位为选项对象的某个属性值",
                "tag": "el-input"
              },
              {
                "name": "hoverThreshold",
                "title": "hover 时展开菜单的灵敏度阈值",
                "tag": "el-input-number"
              }
            ]
          }
        }
      ],
      "default": {
        "expandTrigger": "click",
        "multiple": false,
        "checkStrictly": false,
        "emitPath": true,
        "lazy": false,
        "value": "value",
        "label": "label",
        "children": "children",
        "disabled": "disabled",
        "leaf": "leaf",
        "hoverThreshold": 500
      }
    },
    "size": {
      "label": "尺寸",
      "title": "尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default"
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "clearable": {
      "label": "清空",
      "title": "是否支持清空选项",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "show-all-levels": {
      "label": "显示完整路径",
      "title": "输入框中是否显示选中值的完整路径",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "collapse-tags": {
      "label": "折叠Tag",
      "title": "多选模式下是否折叠Tag",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "collapse-tags-tooltip": {
      "label": "折叠选中的标签",
      "title": "当鼠标悬停于折叠标签的文本时，是否显示所有选中的标签。 要使用此属性，<code>collapse-tags</code>属性必须设定为 true",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "max-collapse-tags-tooltip-height": {
      "label": "collapse tags 的最大高度",
      "title": "collapse tags 的最大高度",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ],
      "version": "2.10.2"
    },
    "separator": {
      "label": "分隔符",
      "title": "用于分隔选项的字符",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "/"
    },
    "filterable": {
      "label": "被搜索",
      "title": "该选项是否可以被搜索",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ]
    },
    "filter-method": {
      "label": "搜索逻辑",
      "title": "自定义搜索逻辑，第一个参数是<code>node</code>，第二个参数是<code>keyword</code>，返回的布尔值表示是否保留该选项",
      "types": [
        {
          "name": "function",
          "details": "(node: CascaderNode, keyword: string) => boolean"
        }
      ]
    },
    "debounce": {
      "label": "去抖延迟",
      "title": "搜索关键词正在输入时的去抖延迟，单位为毫秒",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 300
    },
    "before-filter": {
      "label": "钩子函数",
      "title": "过滤函数调用前，所要调用的钩子函数，该函数接收要过滤的值作为参数。 如果该函数的返回值是 <code>false</code> 或者是一个被拒绝的 <code>Promise</code>，那么接下来的过滤逻辑便不会执行。",
      "types": [
        {
          "name": "function",
          "details": "(value: string) => boolean"
        }
      ]
    },
    "popper-class": {
      "label": "弹出内容类名",
      "title": "弹出内容的自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "teleported": {
      "label": "使用teleport",
      "title": "弹层是否使用 teleport",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "effect": {
      "label": "tooltip 主题",
      "title": "tooltip 主题，内置了 <code>dark</code> / <code>light</code> 两种",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "dark",
              "value": "dark"
            },
            {
              "label": "light",
              "value": "light"
            }
          ]
        },
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": "light",
      "version": "2.10.5"
    },
    "tag-type": {
      "label": "标签类型",
      "title": "标签类型",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "success",
              "value": "success"
            },
            {
              "label": "info",
              "value": "info"
            },
            {
              "label": "warning",
              "value": "warning"
            },
            {
              "label": "danger",
              "value": "danger"
            }
          ]
        }
      ],
      "default": "info"
    },
    "tag-effect": {
      "label": "tag effect",
      "title": "tag effect",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "light",
              "value": "light"
            },
            {
              "label": "dark",
              "value": "dark"
            },
            {
              "label": "plain",
              "value": "plain"
            }
          ]
        }
      ],
      "default": "light",
      "version": "2.7.8"
    },
    "validate-event": {
      "label": "表单校验",
      "title": "输入时是否触发表单的校验",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "max-collapse-tags": {
      "label": "需要显示的 Tag 的最大数量",
      "title": "需要显示的 Tag 的最大数量 只有当 <code>collapse-tags</code> 设置为 true 时才会生效。",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 1,
      "version": "2.3.10"
    },
    "empty-values": {
      "label": "组件的空值配置",
      "title": "组件的空值配置 [参考config-provider](/en-US/component/config-provider#empty-values-configurations)",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "version": "2.7.0"
    },
    "value-on-clear": {
      "label": "清空选项的值",
      "title": "清空选项的值  [参考 config-provider](/en-US/component/config-provider#empty-values-configurations)",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        },
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "boolean",
          "tag": "el-switch"
        },
        {
          "name": "function",
          "detail": ""
        }
      ],
      "version": "2.7.0"
    },
    "persistent": {
      "label": "当下拉框未被激活并且persistent设置为false",
      "title": "当下拉框未被激活并且<code>persistent</code>设置为<code>false</code>，下拉框容器会被删除。",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "version": "2.7.8"
    },
    "fallback-placements": {
      "title": "Tooltip 可用的 positions 请查看[popper.js 文档](https://popper.js.org/docs/v2/modifiers/flip/#fallbackplacements)",
      "types": [
        {
          "name": "arrary",
          "tag": ""
        }
      ],
      "version": "2.8.1"
    },
    "placement": {
      "label": "下拉框出现的位置",
      "title": "下拉框出现的位置",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "top",
              "value": "top"
            },
            {
              "label": "top-start",
              "value": "top-start"
            },
            {
              "label": "top-end",
              "value": "top-end"
            },
            {
              "label": "bottom",
              "value": "bottom"
            },
            {
              "label": "bottom-start",
              "value": "bottom-start"
            },
            {
              "label": "bottom-end",
              "value": "bottom-end"
            },
            {
              "label": "left",
              "value": "left"
            },
            {
              "label": "left-start",
              "value": "left-start"
            },
            {
              "label": "left-end",
              "value": "left-end"
            },
            {
              "label": "right",
              "value": "right"
            },
            {
              "label": "right-start",
              "value": "right-start"
            },
            {
              "label": "right-end",
              "value": "right-end"
            }
          ]
        }
      ],
      "default": "bottom-start",
      "version": "2.8.1"
    },
    "popper-append-to-body": {
      "label": "弹出的内容插入到 body 元素",
      "title": "是否将弹出的内容直接插入到 body 元素。 在弹出内容的边框定位出现问题时，可将该属性设置为 false",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "deprecated": true
    },
    "show-checked-strategy": {
      "label": "多选模式下已选节点的展示策略。 当你想让显示更简洁时",
      "title": "多选模式下已选节点的展示策略。 当你想让显示更简洁时，使用 <code>parent</code>。 当你想显示每个子项时，使用 <code>child</code>。",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "parent",
              "value": "parent"
            },
            {
              "label": "child",
              "value": "child"
            }
          ]
        }
      ],
      "default": "child",
      "version": "2.10.5"
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义备选项的节点内容，分别为当前节点的 Node 对象和数据",
      "type": "object",
      "details": "{ node: any, data: any }"
    },
    {
      "name": "empty",
      "title": "无匹配选项时的内容"
    },
    {
      "name": "prefix",
      "title": "输入框头部内容",
      "version": "2.9.4"
    },
    {
      "name": "suggestion-item",
      "title": "搜索时自定义建议项内容",
      "type": "object",
      "version": "2.9.5",
      "details": "{ item: CascaderNode }"
    },
    {
      "name": "tag",
      "title": "自定义tag样式",
      "type": "object",
      "version": "2.10.3",
      "details": "{ data: Tag[], deleteTag: (tag: Tag) => void }"
    },
    {
      "name": "header",
      "title": "下拉列表顶部的内容",
      "version": "2.10.5"
    },
    {
      "name": "footer",
      "title": "下拉列表底部的内容",
      "version": "2.10.5"
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "当绑定值变化时触发的事件",
      "type": "function",
      "details": "(value: CascaderValue) => void"
    },
    {
      "name": "expand-change",
      "title": "当展开节点发生变化时触发",
      "type": "function",
      "details": "(value: CascaderValue) => void"
    },
    {
      "name": "blur",
      "title": "当失去焦点时触发",
      "type": "function",
      "details": "(event: FocusEvent) => void"
    },
    {
      "name": "focus",
      "title": "当获得焦点时触发",
      "type": "function",
      "details": "(event: FocusEvent) => void"
    },
    {
      "name": "clear",
      "version": "2.7.7",
      "title": "可清空的单选模式下用户点击清空按钮时触发",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "visible-change",
      "title": "下拉框出现/隐藏时触发",
      "type": "function",
      "details": "(value: boolean) => void"
    },
    {
      "name": "remove-tag",
      "title": "在多选模式下，移除Tag时触发",
      "type": "function",
      "details": "(value: CascaderNode['valueByOption']) => void"
    }
  ],
  "exposes": [
    {
      "name": "getCheckedNodes",
      "title": "获取一个当前选中节点的数组。(仅仅是传单) 是否只返回叶选中的节点，默认是 <code>false</code>",
      "type": "function",
      "details": "(leafOnly: boolean) => CascaderNode[] | undefined"
    },
    {
      "name": "cascaderPanelRef",
      "title": "cascader 面板的 ref",
      "type": "object",
      "details": "ComputedRef<any>"
    },
    {
      "name": "togglePopperVisible ",
      "version": "2.2.31",
      "title": "切换 popper 可见状态",
      "type": "function",
      "details": "(visible?: boolean) => void"
    },
    {
      "name": "contentRef",
      "title": "cascader 内容的 ref",
      "type": "object",
      "details": "ComputedRef<any>"
    },
    {
      "name": "presentText",
      "version": "2.8.4",
      "title": "选中的内容文本",
      "type": "object",
      "details": "ComputedRef<string>"
    }
  ],
  "data": {
    "name": "options",
    "label": "选项",
    "source": "static",
    "inProps": true,
    "static": {
      "ref": true,
      "type": {
        "name": "array",
        "tag": "tree-editor"
      },
      "default": [
        {
          "value": "1",
          "label": "选项1",
          "children": [
            {
              "value": "2",
              "label": "选项1-1"
            }
          ]
        }
      ]
    },
    "dynamic": {
      "url": "https://www.fastmock.site/mock/c9e1a1c3cefb84da4306dcf9fadde652/ele-plus-desinger/cascaderData",
      "method": "get",
      "dataKey": ""
    }
  }
}