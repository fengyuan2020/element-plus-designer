{
  "__id__": "image",
  "name": "图片",
  "tag": "el-image",
  "tagIcon": "image",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.gitee.io/zh-CN/component/image.html",
  "attributes": {
    "src": {
      "label": "源地址",
      "title": "图片源地址，同原生属性一致",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "fit": {
      "label": "适应容器框",
      "title": "确定图片如何适应容器框，同原生 [object-fit](https://developer.mozilla.org/en-US/docs/Web/CSS/object-fit)",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "fill",
              "value": "fill"
            },
            {
              "label": "contain",
              "value": "contain"
            },
            {
              "label": "cover",
              "value": "cover"
            },
            {
              "label": "none",
              "value": "none"
            },
            {
              "label": "scale-down",
              "value": "scale-down"
            }
          ]
        }
      ]
    },
    "hide-on-click-modal": {
      "label": "通过点击遮罩层关闭",
      "title": "当开启 preview 功能时，是否可以通过点击遮罩层关闭 preview",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "loading": {
      "label": "加载图像策略",
      "title": "浏览器加载图像的策略，和 [浏览器原生](https://developer.mozilla.org/en-US/docs/Web/HTML/Element/img#attr-loading)能力一致",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "eager",
              "value": "eager"
            },
            {
              "label": "lazy",
              "value": "lazy"
            }
          ]
        }
      ],
      "version": "2.2.3"
    },
    "lazy": {
      "label": "使用懒加载",
      "title": "是否使用懒加载",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "scroll-container": {
      "label": "监听 scroll 事件容器",
      "title": "开启懒加载功能后，监听 scroll 事件的容器 默认情况下，开启懒加载功能后，监听 scroll 事件的容器",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "alt": {
      "label": "alt",
      "title": "原生属性 `alt`",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "referrerpolicy": {
      "label": "referrerPolicy",
      "title": "原生属性 [referrerPolicy](https://developer.mozilla.org/en-US/docs/Web/API/HTMLImageElement/referrerPolicy)。",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "crossorigin": {
      "title": "原生属性 [crossorigin](https://developer.mozilla.org/en-US/docs/Web/HTML/Attributes/crossorigin)",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "anonymous",
              "value": "anonymous"
            },
            {
              "label": "use-credentials",
              "value": "use-credentials"
            }
          ]
        }
      ]
    },
    "preview-src-list": {
      "label": "开启启图片预览功能",
      "title": "开启图片预览功能",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "default": []
    },
    "z-index": {
      "label": "z-index",
      "title": "设置图片预览的 z-index",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "initial-index": {
      "label": "初始预览图像索引",
      "title": "初始预览图像索引，小于 `url-list` 的长度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    },
    "close-on-press-escape": {
      "label": "ESC 关闭 Image Viewer",
      "title": "是否可以通过按下 ESC 关闭 Image Viewer",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "preview-teleported": {
      "label": "插入至 body 元素上",
      "title": "image-viewer 是否插入至 body 元素上。 嵌套的父元素属性会发生修改时应该将此属性设置为 `true`",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "infinite": {
      "label": "无限循环预览",
      "title": "是否可以无限循环预览",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "zoom-rate": {
      "label": "缩放速率",
      "title": "图像查看器缩放事件的缩放速率。",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 1.2
    },
    "min-scale": {
      "title": "图像查看器缩放事件的最小缩放比例",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0.2,
      "version": "2.4.0"
    },
    "max-scale": {
      "title": "图像查看器缩放事件的最大缩放比例",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 7,
      "version": "2.4.0"
    }
  },
  "events": [
    {
      "name": "load",
      "title": "图片加载成功触发",
      "type": "Function",
      "fnDes": "(e: Event) => void"
    },
    {
      "name": "error",
      "title": "图片加载失败触发",
      "type": "Function",
      "fnDes": "(e: Event) => void"
    },
    {
      "name": "switch",
      "title": "切换图像时触发。",
      "type": "Function",
      "fnDes": "(index: number) => void"
    },
    {
      "name": "close",
      "title": "当点击 X 按钮或者在` hide-on-click-modal `为 true 时点击遮罩层时触发",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "show",
      "title": "当 Viewer 显示时触发",
      "type": "Function",
      "fnDes": "() => void"
    }
  ],
  "slots": [
    {
      "name": "placeholder",
      "title": "当图像尚未加载时，自定义的占位符内容"
    },
    {
      "name": "error",
      "title": "自定义图像加载失败的内容"
    },
    {
      "name": "viewer",
      "title": "当图像预览时自定义内容"
    }
  ],
  "desc": "图片容器，在保留所有原生 img 的特性下，支持懒加载，自定义占位、加载失败等"
}