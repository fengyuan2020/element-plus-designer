{
  "__id__": "autocomplete",
  "name": "自动补全",
  "tag": "el-autocomplete",
  "tagIcon": "autocomplete",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.gitee.io/zh-CN/component/autocomplete.html",
  "attributes": {
    "vModel": "modelValue",
    "placeholder": "请选择",
    "clearable": {
      "label": "可清空",
      "title": "是否可清空",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "disabled": {
      "label": "禁用",
      "title": "自动补全组件是否被禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "value-key": {
      "label": "用于显示的键名",
      "title": "输入建议对象中用于显示的键名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "value"
    },
    "debounce": {
      "label": "防抖延时",
      "title": "获取输入建议的防抖延时，单位为毫秒",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 300
    },
    "placement": {
      "label": "菜单弹出位置",
      "title": "菜单弹出位置",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "top",
              "value": "top"
            },
            {
              "label": "top-end",
              "value": "top-end"
            },
            {
              "label": "bottom",
              "value": "bottom"
            },
            {
              "label": "bottom-start",
              "value": "bottom-start"
            },
            {
              "label": "bottom-end",
              "value": "bottom-end"
            }
          ]
        }
      ],
      "default": "bottom-start"
    },
    "fetch-suggestions": {
      "label": "获取输入建议方法名",
      "propsRef": true,
      "title": "请直接输入方法名获取输入建议的方法， 仅当你的输入建议数据 resolve 时，通过调用 `callback(data:[]) ` 来返回它",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "trigger-on-focus": {
      "label": "",
      "title": "whether show suggestions when input focus",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "select-when-unmatched": {
      "label": "无匹配时是否触发select事件",
      "title": "在输入没有任何匹配建议的情况下，按下回车是否触发 `select` 事件",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "name": {
      "title": "等价于原生 input `name` 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "label": {
      "title": "输入框关联的 label 文字",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "hide-loading": {
      "label": "隐藏加载图标",
      "title": "是否隐藏远程加载时的加载图标",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "popper-class": {
      "label": "",
      "title": "下拉列表的类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "popper-append-to-body": {
      "label": "插入至 body 元素",
      "title": "是否将下拉列表插入至 body 元素。 在下拉列表的定位出现问题时，可将该属性设置为 false",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "deprecated": true
    },
    "teleported": {
      "label": "",
      "title": "是否将下拉列表元素插入 append-to 指向的元素下",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "highlight-first-item": {
      "label": "高亮第一项搜索结果",
      "title": "是否默认高亮远程搜索结果的第一项",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "fit-input-width": {
      "label": "",
      "title": "下拉框的宽度是否与输入框相同",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    }
  },
  "events": [
    {
      "name": "select",
      "title": "点击选中建议项时触发",
      "type": "Function",
      "fnDes": "(item: typeof modelValue | any) => void"
    },
    {
      "name": "change",
      "title": "在 Input 值改变时触发",
      "type": "Function",
      "fnDes": "(value: string | number) => void"
    }
  ],
  "slots": [
    {
      "name": "default",
      "title": "自定义输入建议的内容。",
      "type": "^[object]`{ item: Record<string, any> }`"
    },
    {
      "name": "prefix",
      "title": "输入框头部内容"
    },
    {
      "name": "suffix",
      "title": "输入框尾部内容"
    },
    {
      "name": "prepend",
      "title": "输入框前置内容，在 prefix 之前"
    },
    {
      "name": "append",
      "title": "输入框后置内容，在 suffix 之后"
    },
    {
      "name": "loading",
      "title": "修改加载区域内容",
      "version": "2.5.0"
    }
  ],
  "exposes": [
    {
      "name": "activated",
      "title": "自动补全输入框是否被激活",
      "type": "^[object]`Ref<boolean>`"
    },
    {
      "name": "blur",
      "title": "使 input 失去焦点",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "close",
      "title": "折叠建议列表",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "focus",
      "title": "使 input 获取焦点",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "handleSelect",
      "title": "手动触发选中建议事件",
      "type": "Function",
      "fnDes": "(item: any) => promise<void>"
    },
    {
      "name": "handleKeyEnter",
      "title": "手动触发键盘回车事件",
      "type": "Function",
      "fnDes": "() => promise<void>"
    },
    {
      "name": "highlightedIndex",
      "title": "当前高亮显示选项的索引",
      "type": "^[object]`Ref<number>`"
    },
    {
      "name": "highlight",
      "title": "在建议中高亮显示一个项目",
      "type": "Function",
      "fnDes": "(itemIndex: number) => void"
    },
    {
      "name": "inputRef",
      "title": "el-input 组件实例",
      "type": "^[object]`Ref<ElInputInstance>`"
    },
    {
      "name": "loading",
      "title": "远程获取提示内容的加载状态指示器",
      "type": "^[object]`Ref<boolean>`"
    },
    {
      "name": "popperRef",
      "title": "el-tooltip 组件实例",
      "type": "^[object]`Ref<ElTooltipInstance>`"
    },
    {
      "name": "suggestions",
      "title": "获取自动补全结果",
      "type": "^[object]`Ref<record<string, any>>`"
    }
  ],
  "data": {
    "name": "options",
    "label": "选项",
    "source": "static",
    "static": {
      "ref": true,
      "type": {
        "name": "array",
        "tag": "object-array-editor",
        "props": {
          "keys": [
            {
              "key": "value",
              "label": "值",
              "tag": "string-number-input"
            },
            {
              "key": "label",
              "label": "标签",
              "tag": "string-number-input"
            }
          ]
        }
      },
      "default": [
        {
          "label": "选项一",
          "value": "1"
        },
        {
          "label": "选项二",
          "value": "2"
        }
      ]
    }
  },
  "desc": "根据输入内容提供对应的输入建议。"
}