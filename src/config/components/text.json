            {
              "label": "success",
              "value": "success"
            },
            {
              "label": "warning",
              "value": "warning"
            },
            {
              "label": "danger",
              "value": "danger"
            },
            {
              "label": "info",
              "value": "info"
            },
            {
              "label": "default",
              "value": "default"
            }
          ]
        }
      ],
      "default": "default"
