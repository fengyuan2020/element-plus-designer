      "version": "2.4.0"
    },
    "tag": {
      "label": "自定义元素标签",
      "title": "自定义元素标签",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "span"
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "默认内容"
