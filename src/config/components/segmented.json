{
  "attributes": {
    "options": {
      "title": "选项的数据",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "default": []
    },
    "size": {
      "title": "组件大小",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": ""
    },
    "block": {
      "title": "撑满父元素宽度",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ]
    },
    "disabled": {
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "validate-event": {
      "title": "是否触发表单验证",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "name": {
      "title": "原生 name 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "id": {
      "title": "原生 `id` 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "aria-label": {
      "title": "原生 `aria-label` 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义 Option 模板"
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "当所选值更改时触发，参数是当前选中的值",
      "type": "Function",
      "fnDes": "(val: any) => void"
    }
  ],
  "desc": "用于展示多个选项并允许用户选择其中单个选项。"
}