{
  "__id__": "segmented",
  "name": "分段控制器",
  "desc": "用于展示多个选项并允许用户选择其中单个选项。",
  "tag": "el-segmented",
  "tagIcon": "segmented",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/segmented.html",
  "attributes": {
    "vModel": "modelValue",
    "options": {
      "label": "选项的数据",
      "title": "选项的数据",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "default": "[]"
    },
    "props": {
      "label": "配置选项",
      "title": "配置选项，详见下表",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "version": "2.9.8"
    },
    "size": {
      "label": "组件大小",
      "title": "组件大小",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "options": [
            {
              "label": "",
              "value": ""
            },
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ],
          "config": {
            "optionType": "button"
          }
        }
      ],
      "default": "default"
    },
    "block": {
      "label": "撑满父元素宽度",
      "title": "撑满父元素宽度",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ]
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "validate-event": {
      "label": "触发表单验证",
      "title": "是否触发表单验证",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "name": {
      "label": "原生 name 属性",
      "title": "原生 name 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "id": {
      "label": "原生 id 属性",
      "title": "原生 <code>id</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "aria-label": {
      "label": "原生 aria-label 属性",
      "title": "原生 <code>aria-label</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true
    },
    "direction": {
      "label": "展示的方向",
      "title": "展示的方向",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "horizontal",
              "value": "horizontal"
            },
            {
              "label": "vertical",
              "value": "vertical"
            }
          ]
        }
      ],
      "default": "horizontal",
      "version": "2.8.7"
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义 Option 模板",
      "type": "object",
      "details": "{ item: Option }"
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "当所选值更改时触发，参数是当前选中的值",
      "type": "function",
      "details": "(val: any) => void"
    }
  ]
}