{
  "__id__": "cascader-panel",
  "name": "",
  "desc": "当一个数据集合有清晰的层级结构时，可通过级联选择器逐级查看并选择。",
  "tag": "el-cascader-panel",
  "tagIcon": "cascader-panel",
  "layouts": [
    "containerItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/cascader-panel.html",
  "attributes": {
    "options": {
      "label": "选项的数据源",
      "title": "选项的数据源， <code>value</code> 和 <code>label</code> 可以通过 <code>CascaderProps</code> 自定义.",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "props": {
      "label": "配置选项",
      "title": "配置选项, 请参阅下面 <code>CascaderProps</code> 表。",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "下级节点的自定义内容，它们分别是当前节点对象和节点数据。",
      "type": "object",
      "details": "{ node: any, data: any }"
    },
    {
      "name": "empty",
      "title": "没有数据时面板的内容。",
      "version": "2.8.3"
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "当选中节点变化时触发",
      "type": "function",
      "details": "(value: CascaderValue | undefined) => void"
    },
    {
      "name": "update:modelValue",
      "title": "当绑定值变化时触发的事件",
      "type": "function",
      "details": "(value: CascaderValue | undefined) => void"
    },
    {
      "name": "expand-change",
      "title": "当展开节点发生变化时触发",
      "type": "function",
      "details": "(value: CascaderNodePathValue) => void"
    },
    {
      "name": "close",
      "title": "面板的关闭事件，提供给 Cascader 以便做更好的判断。",
      "type": "function",
      "details": "() => void"
    }
  ],
  "exposes": [
    {
      "name": "getCheckedNodes",
      "title": "获取一个当前选中节点的数组。(仅仅是传单) 是否只返回叶选中的节点，默认是 <code>false</code>",
      "type": "function",
      "details": "(leafOnly: boolean) => CascaderNode[] | undefined"
    },
    {
      "name": "clearCheckedNodes",
      "title": "清空选中的节点",
      "type": "function",
      "details": "() => void"
    }
  ]
}