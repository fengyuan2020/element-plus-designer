{
  "__id__": "upload",
  "name": "上传",
  "desc": "通过点击或者拖拽上传文件。",
  "tag": "el-upload",
  "tagIcon": "upload",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/upload.html",
  "attributes": {
    "action": {
      "label": "URL",
      "title": "请求 URL",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "#",
      "required": true
    },
    "headers": {
      "label": "请求头部",
      "title": "设置上传的请求头部",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "method": {
      "label": "方法",
      "title": "设置上传请求方法",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": "post"
    },
    "multiple": {
      "label": "多选文件",
      "title": "是否支持多选文件",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "data": {
      "label": "附带参数",
      "title": "上传时附带的额外参数 从 v2.3.13 支持 <code>Awaitable</code> 数据，和 <code>Function</code>",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        },
        {
          "name": "function",
          "details": "(rawFile: UploadRawFile) => Awaitable<Record<string, any>>"
        }
      ],
      "default": {}
    },
    "name": {
      "label": "字段名",
      "title": "上传的文件字段名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "file"
    },
    "with-credentials": {
      "label": "发送 cookie 凭证信息",
      "title": "支持发送 cookie 凭证信息",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "show-file-list": {
      "label": "显示已上传文件列表",
      "title": "是否显示已上传文件列表",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "drag": {
      "label": "启用拖拽上传",
      "title": "是否启用拖拽上传",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "accept": {
      "label": "文件类型",
      "title": "接受上传的<a href=\"https://developer.mozilla.org/en-US/docs/Web/HTML/Element/input#attr-accept\">文件类型</a>（thumbnail-mode 模式下此参数无效）",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "crossorigin": {
      "label": "原生属性 crossorigin",
      "title": "原生属性 <a href=\"https://developer.mozilla.org/en-US/docs/Web/HTML/Attributes/crossorigin\">crossorigin</a>",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "anonymous",
              "value": "anonymous"
            },
            {
              "label": "use-credentials",
              "value": "use-credentials"
            }
          ]
        }
      ]
    },
    "on-preview": {
      "label": "已上传的文件时的钩子",
      "title": "点击文件列表中已上传的文件时的钩子",
      "types": [
        {
          "name": "function",
          "details": "(uploadFile: UploadFile) => void"
        }
      ],
      "hide": true
    },
    "on-remove": {
      "label": "移除文件",
      "title": "文件列表移除文件时的钩子",
      "types": [
        {
          "name": "function",
          "details": "(uploadFile: UploadFile, uploadFiles: UploadFiles) => void"
        }
      ],
      "hide": true
    },
    "on-success": {
      "label": "文件上传成功时的钩子",
      "title": "文件上传成功时的钩子",
      "types": [
        {
          "name": "function",
          "details": "(response: any, uploadFile: UploadFile, uploadFiles: UploadFiles) => void"
        }
      ],
      "hide": true
    },
    "on-error": {
      "label": "文件上传失败时的钩子",
      "title": "文件上传失败时的钩子",
      "types": [
        {
          "name": "function",
          "details": "(error: Error, uploadFile: UploadFile, uploadFiles: UploadFiles) => void"
        }
      ],
      "hide": true
    },
    "on-progress": {
      "label": "文件上传时的钩子",
      "title": "文件上传时的钩子",
      "types": [
        {
          "name": "function",
          "details": "(evt: UploadProgressEvent, uploadFile: UploadFile, uploadFiles: UploadFiles) => void"
        }
      ],
      "hide": true
    },
    "on-change": {
      "label": "文件状态改变时的钩子",
      "title": "文件状态改变时的钩子，添加文件、上传成功和上传失败时都会被调用",
      "types": [
        {
          "name": "function",
          "details": "(uploadFile: UploadFile, uploadFiles: UploadFiles) => void"
        }
      ],
      "hide": true
    },
    "on-exceed": {
      "label": "当超出限制时",
      "title": "当超出限制时，执行的钩子函数",
      "types": [
        {
          "name": "function",
          "details": "(files: File[], uploadFiles: UploadUserFile[]) => void"
        }
      ],
      "hide": true
    },
    "before-upload": {
      "label": "上传文件之前的钩子",
      "title": "上传文件之前的钩子，参数为上传的文件， 若返回<code>false</code>或者返回<code> Promise</code> 且被 reject，则停止上传。",
      "types": [
        {
          "name": "function",
          "details": "(rawFile: UploadRawFile) => Awaitable<void | undefined | null | boolean | File | Blob>"
        }
      ],
      "hide": true
    },
    "before-remove": {
      "label": "删除文件之前的钩子",
      "title": "删除文件之前的钩子，参数为上传的文件和文件列表， 若返回 <code>false </code>或者返回 <code>Promise </code>且被 reject，则停止删除。",
      "types": [
        {
          "name": "function",
          "details": "(uploadFile: UploadFile, uploadFiles: UploadFiles) => Awaitable<boolean>"
        }
      ],
      "hide": true
    },
    "list-type": {
      "label": "列表类型",
      "title": "文件列表的类型",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "text",
              "value": "text"
            },
            {
              "label": "picture",
              "value": "picture"
            },
            {
              "label": "picture-card",
              "value": "picture-card"
            }
          ]
        }
      ],
      "default": "text"
    },
    "auto-upload": {
      "label": "自动上传文件",
      "title": "是否自动上传文件",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "http-request": {
      "label": "允许自行实现上传文件的请求",
      "title": "覆盖默认的 Xhr 行为，允许自行实现上传文件的请求",
      "types": [
        {
          "name": "function",
          "details": "(options: UploadRequestOptions) => XMLHttpRequest | Promise<unknown>"
        }
      ],
      "default": "请参考ajaxUpload",
      "hide": true
    },
    "disabled": {
      "label": "禁用上传",
      "title": "是否禁用上传",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "limit": {
      "label": "最大数量",
      "title": "允许上传文件的最大数量",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义默认内容"
    },
    {
      "name": "trigger",
      "title": "触发文件选择框的内容"
    },
    {
      "name": "tip",
      "title": "提示说明文字"
    },
    {
      "name": "file",
      "title": "缩略图模板的内容",
      "type": "object",
      "details": "{ file: UploadFile, index: number }"
    }
  ],
  "exposes": [
    {
      "name": "abort",
      "title": "取消上传请求",
      "type": "function",
      "details": "(file: UploadFile) => void"
    },
    {
      "name": "submit",
      "title": "手动上传文件列表",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "clearFiles",
      "title": "清空已上传的文件列表（该方法不支持在 <code>before-upload</code> 中调用）",
      "type": "function",
      "details": "(status?: UploadStatus[]) => void"
    },
    {
      "name": "handleStart",
      "title": "手动选择文件",
      "type": "function",
      "details": "(rawFile: UploadRawFile) => void"
    },
    {
      "name": "handleRemove",
      "title": "手动移除文件。 <code>file</code> 和<code>rawFile</code> 已被合并。 <code>rawFile</code> 将在 <code>v2.2.0</code> 中移除",
      "type": "function",
      "details": "(file: UploadFile | UploadRawFile, rawFile?: UploadRawFile) => void"
    }
  ]
}