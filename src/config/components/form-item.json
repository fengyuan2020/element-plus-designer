{
  "__id__": "form-item",
  "name": "表单项",
  "desc": "表单包含 `输入框`, `单选框`, `下拉选择`, `多选框` 等用户输入的组件。 使用表单，您可以收集、验证和提交数据。",
  "tag": "el-form-item",
  "tagIcon": "form",
  "layouts": [
    "containerItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/form.html",
  "attributes": {
    "prop": {
      "label": "model 的键名",
      "title": "<code>model</code> 的键名。 它可以是一个属性的值(如 <code>a.b.0</code> 或 <code>['a', 'b', '0']</code>)。 在使用了 <code>validate</code>、<code>resetFields</code> 的方法时，该属性是必填的。",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "string[]"
        }
      ]
    },
    "label": {
      "label": "标签文本",
      "title": "标签文本",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "label-position": {
      "label": "表单域标签的位置",
      "title": "表单域标签的位置， 当设置为 <code>left</code> 或 <code>right</code> 时，则也需要设置 <code>label-width</code> 属性 默认会继承 <code>Form</code>的<code>label-position</code>",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "left",
              "value": "left"
            },
            {
              "label": "right",
              "value": "right"
            },
            {
              "label": "top",
              "value": "top"
            }
          ]
        }
      ],
      "version": "2.7.7"
    },
    "label-width": {
      "label": "标签宽度",
      "title": "标签宽度，例如 <code>'50px'</code>。 可以使用 <code>auto</code>。",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "required": {
      "label": "必填项",
      "title": "是否为必填项，如不设置，则会根据校验规则确认",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "rules": {
      "label": "表单验证规则",
      "title": "表单验证规则, 具体配置见下表, 更多内容可以参考async-validator",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "hide": true
    },
    "error": {
      "label": "验证错误提示信息",
      "title": "表单域验证错误时的提示信息。设置该值会导致表单验证状态变为 error，并显示该错误信息。",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "show-message": {
      "label": "显示校验错误信息",
      "title": "是否显示校验错误信息",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "inline-message": {
      "label": "行内显示校验信息",
      "title": "是否在行内显示校验信息",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ]
    },
    "size": {
      "label": "尺寸",
      "title": "用于控制该表单域下组件的默认尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default"
    },
    "for": {
      "label": "和原生标签相同能力",
      "title": "和原生标签相同能力",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "validate-status": {
      "label": "校验的状态",
      "title": "formitem 校验的状态",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "error",
              "value": "error"
            },
            {
              "label": "validating",
              "value": "validating"
            },
            {
              "label": "success",
              "value": "success"
            }
          ]
        }
      ]
    },
    "trigger": {
      "label": "验证逻辑的触发方式",
      "title": "验证逻辑的触发方式",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "blur",
              "value": "blur"
            },
            {
              "label": "change",
              "value": "change"
            }
          ]
        }
      ]
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "表单的内容。"
    },
    {
      "name": "label",
      "title": "标签位置显示的内容",
      "type": "object",
      "details": "{ label: string }"
    },
    {
      "name": "error",
      "title": "验证错误信息的显示内容",
      "type": "object",
      "details": "{ error: string }"
    }
  ],
  "exposes": [
    {
      "name": "size",
      "title": "表单项大小",
      "type": "object",
      "details": "ComputedRef<'' | 'large' | 'default' | 'small'>"
    },
    {
      "name": "validateMessage",
      "title": "校验消息",
      "type": "object",
      "details": "Ref<string>"
    },
    {
      "name": "validateState",
      "title": "校验状态",
      "type": "object",
      "details": "Ref<'' | 'error' | 'validating' | 'success'>"
    },
    {
      "name": "validate",
      "title": "验证表单项",
      "type": "function",
      "details": "(trigger: string, callback?: FormValidateCallback | undefined) => FormValidationResult"
    },
    {
      "name": "resetField",
      "title": "对该表单项进行重置，将其值重置为初始值并移除校验结果",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "clearValidate",
      "title": "移除该表单项的校验结果",
      "type": "function",
      "details": "() => void"
    }
  ]
}