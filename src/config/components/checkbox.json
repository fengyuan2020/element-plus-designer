{
  "__id__": "checkbox",
  "name": "多选框",
  "desc": "在一组备选项中进行多选。",
  "tag": "el-checkbox",
  "tagIcon": "checkbox",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/checkbox.html",
  "attributes": {
    "vModel": "modelValue",
    "value": {
      "label": "选中状态的值",
      "title": "选中状态的值（只有在<code>checkbox-group</code>或者绑定对象类型为<code>array</code>时有效）",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        },
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "boolean",
          "tag": "el-switch"
        },
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "version": "2.6.0"
    },
    "label": {
      "label": "选中状态的值",
      "title": "选中状态的值，只有在绑定对象类型为 <code>array</code> 时有效。 如果没有 value， <code>label</code>则作为<code>value</code>使用",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "boolean",
          "tag": "el-switch"
        },
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "true-value": {
      "label": "选中时的值",
      "title": "选中时的值",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ],
      "version": "2.6.0"
    },
    "false-value": {
      "label": "没有选中时的值",
      "title": "没有选中时的值",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ],
      "version": "2.6.0"
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "border": {
      "label": "显示边框",
      "title": "是否显示边框",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "size": {
      "label": "尺寸",
      "title": "Checkbox 的尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ]
    },
    "name": {
      "label": "原生 name 属性",
      "title": "原生 name 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "checked": {
      "label": "当前勾选",
      "title": "当前是否勾选",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "indeterminate": {
      "label": "不确定状态",
      "title": "设置不确定状态，仅负责样式控制",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "validate-event": {
      "label": "输入时是否触发表单的校验",
      "title": "输入时是否触发表单的校验",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "tabindex": {
      "label": "输入框的 tabindex",
      "title": "输入框的 tabindex",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "id": {
      "label": "input id",
      "title": "input id",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "aria-controls": {
      "title": "与 [aria-control](https://developer.mozilla.org/en-US/docs/Web/Accessibility/ARIA/Attributes/aria-controls)一致, 当 `indeterminate`为 `true`时生效",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true,
      "version": "2.7.2"
    },
    "true-label": {
      "label": "选中时的值",
      "title": "选中时的值",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ],
      "deprecated": true
    },
    "false-label": {
      "label": "没有选中时的值",
      "title": "没有选中时的值",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ],
      "deprecated": true
    },
    "controls": {
      "title": "与 [aria-control](https://developer.mozilla.org/en-US/docs/Web/Accessibility/ARIA/Attributes/aria-controls)一致, 当 `indeterminate`为 `true`时生效",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true,
      "deprecated": true
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义默认内容"
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "当绑定值变化时触发的事件",
      "type": "function",
      "details": "(value: string | number | boolean) => void"
    }
  ]
}