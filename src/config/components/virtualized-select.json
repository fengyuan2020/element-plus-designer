  "events": [
    {
      "name": "change",
      "title": "当所选值更改时触发，参数是当前选中的值",
      "type": "Function",
      "fnDes": "(val: any) => void"
    },
    {
      "name": "visible-change",
      "title": "当下拉菜单出现/消失时触发器, 当它出现时, 参数将是 `true`, 否则将是 `false`",
      "type": "Function",
      "fnDes": "(visible: boolean) => void"
    },
    {
      "name": "remove-tag",
      "title": "当一个标签在多个模式下被移除时触发，参数将被移除标签值",
      "type": "Function",
      "fnDes": "(tagValue: any) => void"
    },
    {
      "name": "clear",
      "title": "可清空的单选模式下用户点击清空按钮时触发",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "blur",
