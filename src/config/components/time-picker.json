{
  "__id__": "time-picker",
  "name": "时间选择器",
  "desc": "用于选择或输入日期",
  "tag": "el-time-picker",
  "tagIcon": "time",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/time-picker.html",
  "attributes": {
    "vModel": "modelValue",
    "placeholder": "请选择",
    "readonly": {
      "label": "只读",
      "title": "完全只读",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "disabled": {
      "label": "禁用",
      "title": "禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "editable": {
      "label": "可输入",
      "title": "文本框可输入",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "clearable": {
      "label": "显示清除按钮",
      "title": "是否显示清除按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "size": {
      "label": "尺寸",
      "title": "输入框尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default"
    },
    "start-placeholder": {
      "label": "范围选择时开始日期的占位内容",
      "title": "范围选择时开始日期的占位内容",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "end-placeholder": {
      "label": "范围选择时结束日期的占位内容",
      "title": "范围选择时结束日期的占位内容",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "is-range": {
      "label": "时间范围选择",
      "title": "是否为时间范围选择",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "arrow-control": {
      "label": "箭头选择时间",
      "title": "是否使用箭头进行时间选择",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "popper-class": {
      "label": "下拉框类名",
      "title": "TimePicker 下拉框的类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "popper-style": {
      "label": "为 TimePicke 的下拉菜单自定义样式",
      "title": "为 TimePicke 的下拉菜单自定义样式",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "object",
          "tag": "style-editor"
        }
      ]
    },
    "range-separator": {
      "label": "分隔符",
      "title": "选择范围时的分隔符",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "-"
    },
    "format": {
      "label": "输入框格式",
      "title": "显示在输入框中的格式",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "HH:mm:ss",
      "keep": true
    },
    "default-value": {
      "label": "默认显示的时间",
      "title": "可选，选择器打开时默认显示的时间",
      "types": [
        {
          "name": "date",
          "tag": "el-time-picker"
        },
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "keep": true
    },
    "value-format": {
      "label": "绑定值的格式",
      "title": "可选，绑定值的格式。 不指定则绑定值为 Date 对象",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "keep": true
    },
    "id": {
      "label": "id 属性",
      "title": "等价于原生 input <code>id</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "hide": true
    },
    "name": {
      "label": "name 属性",
      "title": "等价于原生 input <code>name</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "aria-label": {
      "label": "aria-label 属性",
      "title": "等价于原生 input <code>aria-label</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true,
      "version": "2.7.2"
    },
    "prefix-icon": {
      "label": "前缀图标",
      "title": "自定义前缀图标",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": "Clock"
    },
    "clear-icon": {
      "label": "清除图标",
      "title": "自定义清除图标",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": "CircleClose"
    },
    "disabled-hours": {
      "label": "禁止选择部分小时选项",
      "title": "禁止选择部分小时选项",
      "types": [
        {
          "name": "function",
          "details": "(role: string, comparingDate?: Dayjs) => number[]"
        }
      ]
    },
    "disabled-minutes": {
      "label": "禁止选择部分分钟选项",
      "title": "禁止选择部分分钟选项",
      "types": [
        {
          "name": "function",
          "details": "(hour: number, role: string, comparingDate?: Dayjs) => number[]"
        }
      ]
    },
    "disabled-seconds": {
      "label": "禁止选择部分秒选项",
      "title": "禁止选择部分秒选项",
      "types": [
        {
          "name": "function",
          "details": "(hour: number, minute: number, role: string, comparingDate?: Dayjs) => number[]"
        }
      ]
    },
    "teleported": {
      "label": "下拉列表镜像 body 元素",
      "title": "是否将 popover 的下拉列表镜像至 body 元素",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "tabindex": {
      "label": "输入框的 tabindex",
      "title": "输入框的 tabindex",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ],
      "default": 0
    },
    "empty-values": {
      "label": "组件的空值配置",
      "title": "组件的空值配置 <a href=\"/en-US/component/config-provider#empty-values-configurations\">参考config-provider</a>",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "version": "2.7.0"
    },
    "value-on-clear": {
      "label": "清空选项的值",
      "title": "清空选项的值 <a href=\"/en-US/component/config-provider#empty-values-configurations\">参考 config-provider</a>",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        },
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "boolean",
          "tag": "el-switch"
        },
        {
          "name": "function",
          "details": ""
        }
      ],
      "version": "2.7.0"
    },
    "label": {
      "label": "aria-label 属性",
      "title": "等价于原生 input <code>aria-label</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "a11y": true,
      "deprecated": true
    }
  },
  "slots": [],
  "events": [
    {
      "name": "change",
      "title": "用户确认选定的值时触发",
      "type": "function",
      "details": "(val: number | string | Date | [number, number] | [string, string] | [Date, Date]) => void"
    },
    {
      "name": "blur",
      "title": "在组件 Input 失去焦点时触发",
      "type": "function",
      "details": "(e: FocusEvent) => void"
    },
    {
      "name": "focus",
      "title": "在组件 Input 获得焦点时触发",
      "type": "function",
      "details": "(e: FocusEvent) => void"
    },
    {
      "name": "clear",
      "title": "可清空的模式下用户点击清空按钮时触发",
      "type": "function",
      "details": "() => void",
      "version": "2.7.7"
    },
    {
      "name": "visible-change",
      "title": "当 TimePicker 的下拉列表出现/消失时触发",
      "type": "function",
      "details": "(visibility: boolean) => void"
    }
  ],
  "exposes": [
    {
      "name": "focus",
      "title": "使组件获取焦点",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "blur",
      "title": "使组件失去焦点",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "handleOpen",
      "title": "打开时间选择器弹窗",
      "type": "function",
      "details": "() => void",
      "version": "2.2.16"
    },
    {
      "name": "handleClose",
      "title": "关闭时间选择器弹窗",
      "type": "function",
      "details": "() => void",
      "version": "2.2.16"
    }
  ]
}