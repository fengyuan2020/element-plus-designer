{
  "__id__": "select",
  "name": "下拉选择",
  "desc": "当选项过多时，使用下拉菜单展示并选择内容。",
  "tag": "el-select",
  "tagIcon": "select",
  "layouts": [
    "rawItem"
  ],
  "childTag": [
    "option-group",
    "option"
  ],
  "document": "https://element-plus.org/zh-CN/component/select.html",
  "attributes": {
    "vModel": "modelValue",
    "placeholder": "请选择",
    "multiple": {
      "label": "多选",
      "title": "是否多选",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "options": {
      "label": "选项的数据源",
      "title": "选项的数据源， <code>value</code> 的 key 和 <code>label</code> 和 <code> disabled</code>可以通过 <code>props</code>自定义.",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "version": "2.10.5"
    },
    "props": {
      "label": "options 的配置",
      "title": "options 的配置",
      "types": [
        {
          "name": "object",
          "tag": "fixed-keys-object-editor",
          "props": {
            "keys": [
              {
                "name": "label",
                "title": "标签",
                "tag": "el-input"
              },
              {
                "name": "value",
                "title": "值",
                "tag": "el-input"
              },
              {
                "name": "disabled",
                "title": "禁用",
                "tag": "el-input"
              }
            ]
          }
        }
      ],
      "default": {
        "value": "value",
        "label": "label",
        "disabled": "disabled"
      },
      "version": "2.10.5",
      "keep": true
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "value-key": {
      "label": "作为 value 唯一标识的键名",
      "title": "作为 value 唯一标识的键名，绑定值为对象类型时必填",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "value"
    },
    "size": {
      "label": "尺寸",
      "title": "输入框尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default"
    },
    "clearable": {
      "label": "清空",
      "title": "是否可以清空选项",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "collapse-tags": {
      "label": "折叠多选",
      "title": "多选时是否将选中值按文字的形式展示",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "switch": [
        "multiple",
        true
      ]
    },
    "collapse-tags-tooltip": {
      "label": "显示多选tooltip",
      "title": "当鼠标悬停于折叠标签的文本时，是否显示所有选中的标签。 要使用此属性，<code>collapse-tags</code>属性必须设定为 true",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "version": "2.3.0",
      "switch": [
        "collapse-tags",
        true
      ]
    },
    "multiple-limit": {
      "label": "多选最大数目",
      "title": "<code>multiple</code> 属性设置为 <code>true</code> 时，代表多选场景下用户最多可以选择的项目数， 为 0 则不限制",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0,
      "switch": [
        "multiple",
        true
      ]
    },
    "id": {
      "label": "原生 input 的 id",
      "title": "原生 input 的 id",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "name": {
      "label": "Select 输入框的原生 name 属性",
      "title": "Select 输入框的原生 name 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "effect": {
      "label": "文字提示主题",
      "title": "tooltip 主题，内置了 <code>dark</code> / <code>light</code> 两种",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "dark",
              "value": "dark"
            },
            {
              "label": "light",
              "value": "light"
            }
          ]
        },
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": "light"
    },
    "autocomplete": {
      "label": "Select 输入框的原生 autocomplete 属性",
      "title": "Select 输入框的原生 autocomplete 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "off",
      "hide": true
    },
    "filterable": {
      "label": "可筛选",
      "title": "Select 组件是否可筛选",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "allow-create": {
      "label": "创建新条目",
      "title": "是否允许用户创建新条目， 只有当 <code>filterable</code> 设置为 true 时才会生效。",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "filter-method": {
      "label": "筛选方法",
      "title": "自定义筛选方法的第一个参数是当前输入的值。 当<code>filterable</code>设置为 true 时才会生效",
      "types": [
        {
          "name": "function",
          "details": "(query: string) => void"
        }
      ]
    },
    "remote": {
      "label": "服务器搜索数据",
      "title": "其中的选项是否从服务器远程加载",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "remote-method": {
      "label": "当输入值发生变化时触发的函数。 它的参数就是当前的输入值。 当filterable设置为 true 时才会生效",
      "title": "当输入值发生变化时触发的函数。 它的参数就是当前的输入值。 当<code>filterable</code>设置为 true 时才会生效",
      "types": [
        {
          "name": "function",
          "details": "(query: string) => void"
        }
      ]
    },
    "remote-show-suffix": {
      "label": "远程搜索后缀图标",
      "title": "远程搜索方法显示后缀图标",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "loading": {
      "label": "正在获取数据",
      "title": "是否正在从远程获取数据",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "loading-text": {
      "label": "加载内容显示文本",
      "title": "从服务器加载数据时显示的文本，默认为“Loading”",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "no-match-text": {
      "label": "无匹配时显示文字",
      "title": "搜索条件无匹配时显示的文字，也可以使用 <code>empty</code> 插槽设置，默认是 “No matching data'”",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "no-data-text": {
      "label": "无数据显示文字",
      "title": "无选项时显示的文字，也可以使用 <code>empty</code> 插槽设置自定义内容，默认是 “No data”",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "popper-class": {
      "label": "下拉菜单类名",
      "title": "选择器下拉菜单的自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "popper-style": {
      "label": "为 Select 的下拉菜单自定义样式",
      "title": "为 Select 的下拉菜单自定义样式",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "object",
          "tag": "style-editor"
        }
      ],
      "version": "2.11.0"
    },
    "reserve-keyword": {
      "label": "搜索关键词",
      "title": "当 <code>multiple</code> 和 <code>filterable</code>被设置为 true 时，是否在选中一个选项后保留当前的搜索关键词",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "default-first-option": {
      "label": "回车选择第一个匹配项",
      "title": "是否在输入框按下回车时，选择第一个匹配项。 需配合 <code>filterable</code> 或 <code>remote</code> 使用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "teleported": {
      "label": "使用teleport插入body元素",
      "title": "是否使用 teleport。设置成 <code>true</code>则会被追加到 <code>append-to</code> 的位置",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "append-to": {
      "label": "下拉框挂载到哪个 DOM 元素",
      "title": "下拉框挂载到哪个 DOM 元素",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "version": "2.8.4"
    },
    "persistent": {
      "label": "persistent",
      "title": "当下拉选择器未被激活并且<code>persistent</code>设置为<code>false</code>，选择器会被删除。",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "automatic-dropdown": {
      "label": "自动弹出菜单",
      "title": "对于不可搜索的 Select，是否在输入框获得焦点后自动弹出选项菜单",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "clear-icon": {
      "label": "清除图标",
      "title": "自定义清除图标",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": "CircleClose"
    },
    "fit-input-width": {
      "label": "下拉框的宽度是否与输入框相同",
      "title": "下拉框的宽度是否与输入框相同",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "suffix-icon": {
      "label": "后缀图标",
      "title": "自定义后缀图标组件",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": "ArrowDown"
    },
    "tag-type": {
      "label": "标签类型",
      "title": "标签类型",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "success",
              "value": "success"
            },
            {
              "label": "info",
              "value": "info"
            },
            {
              "label": "warning",
              "value": "warning"
            },
            {
              "label": "danger",
              "value": "danger"
            }
          ]
        }
      ],
      "default": "info",
      "switch": [
        "multiple",
        true
      ]
    },
    "tag-effect": {
      "label": "标签效果",
      "title": "标签效果",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "light",
              "value": "light"
            },
            {
              "label": "dark",
              "value": "dark"
            },
            {
              "label": "plain",
              "value": "plain"
            }
          ]
        }
      ],
      "default": "light",
      "version": "2.7.7"
    },
    "validate-event": {
      "label": "触发表单校验",
      "title": "是否触发表单验证",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "offset": {
      "label": "下拉面板偏移量",
      "title": "下拉面板偏移量",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 12,
      "version": "2.8.8"
    },
    "show-arrow": {
      "label": "下拉菜单的内容是否有箭头",
      "title": "下拉菜单的内容是否有箭头",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "version": "2.8.8"
    },
    "placement": {
      "label": "下拉框出现位置",
      "title": "下拉框出现的位置",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "top",
              "value": "top"
            },
            {
              "label": "top-start",
              "value": "top-start"
            },
            {
              "label": "top-end",
              "value": "top-end"
            },
            {
              "label": "bottom",
              "value": "bottom"
            },
            {
              "label": "bottom-start",
              "value": "bottom-start"
            },
            {
              "label": "bottom-end",
              "value": "bottom-end"
            },
            {
              "label": "left",
              "value": "left"
            },
            {
              "label": "left-start",
              "value": "left-start"
            },
            {
              "label": "left-end",
              "value": "left-end"
            },
            {
              "label": "right",
              "value": "right"
            },
            {
              "label": "right-start",
              "value": "right-start"
            },
            {
              "label": "right-end",
              "value": "right-end"
            }
          ]
        }
      ],
      "default": "bottom-start",
      "version": "2.2.17"
    },
    "fallback-placements": {
      "title": "dropdown 可用的 positions 请查看[popper.js 文档](https://popper.js.org/docs/v2/modifiers/flip/#fallbackplacements)",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "default": [
        "bottom-start",
        "top-start",
        "right",
        "left"
      ],
      "version": "2.5.6",
      "keep": true
    },
    "max-collapse-tags": {
      "label": "标签最大数量",
      "title": "需要显示的 Tag 的最大数量 只有当 <code>collapse-tags</code> 设置为 true 时才会生效。",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 1,
      "version": "2.3.0",
      "switch": [
        "collapse-tags",
        true
      ]
    },
    "popper-options": {
      "label": "popper.js 参数",
      "title": "<a href=\"https://popper.js.org/docs/v2/\">popper.js</a> 参数",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {},
      "keep": true
    },
    "aria-label": {
      "label": "aria-label 属性",
      "title": "等价于原生 input <code>aria-label</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true
    },
    "empty-values": {
      "label": "组件的空值配置",
      "title": "组件的空值配置 <a href=\"/en-US/component/config-provider#empty-values-configurations\">参考config-provider</a>",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "version": "2.7.0"
    },
    "value-on-clear": {
      "label": "清空选项的值",
      "title": "清空选项的值 <a href=\"/en-US/component/config-provider#empty-values-configurations\">参考 config-provider</a>",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        },
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "boolean",
          "tag": "el-switch"
        },
        {
          "name": "function",
          "details": ""
        }
      ],
      "version": "2.7.0"
    },
    "suffix-transition": {
      "label": "下拉菜单显示/消失时后缀图标的动画",
      "title": "下拉菜单显示/消失时后缀图标的动画",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true,
      "deprecated": true
    },
    "tabindex": {
      "label": "input 的 tabindex",
      "title": "input 的 tabindex",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ],
      "version": "2.9.0"
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "option 组件列表",
      "subtags": [
        "option-group",
        "option"
      ]
    },
    {
      "name": "header",
      "title": "下拉列表顶部的内容",
      "version": "2.4.3"
    },
    {
      "name": "footer",
      "title": "下拉列表底部的内容",
      "version": "2.4.3"
    },
    {
      "name": "prefix",
      "title": "Select 组件头部内容"
    },
    {
      "name": "empty",
      "title": "无选项时的列表"
    },
    {
      "name": "tag",
      "title": "作为 Select 组件的内容时，子标签 <code>data</code>、<code>selectDisabled</code> 和 <code>deleteTag</code> 是在版本 [object Object] 中新增的。",
      "subtags": "[object Object]",
      "version": "2.5.0"
    },
    {
      "name": "loading",
      "title": "select 组件自定义 loading内容",
      "version": "2.5.2"
    },
    {
      "name": "label",
      "title": "select 组件自定义标签内容 <code>index</code> 引入于[object Object]",
      "subtags": "[object Object]",
      "version": "2.7.4"
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "选中值发生变化时触发",
      "type": "function",
      "details": "(value: any) => void"
    },
    {
      "name": "visible-change",
      "title": "下拉框出现/隐藏时触发",
      "type": "function",
      "details": "(visible: boolean) => void"
    },
    {
      "name": "remove-tag",
      "title": "多选模式下移除tag时触发",
      "type": "function",
      "details": "(tagValue: any) => void"
    },
    {
      "name": "clear",
      "title": "可清空的单选模式下用户点击清空按钮时触发",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "blur",
      "title": "当 input 失去焦点时触发",
      "type": "function",
      "details": "(event: FocusEvent) => void"
    },
    {
      "name": "focus",
      "title": "当 input 获得焦点时触发",
      "type": "function",
      "details": "(event: FocusEvent) => void"
    },
    {
      "name": "popup-scroll",
      "title": "下拉滚动时触发",
      "type": "function",
      "details": "(data:{scrollTop: number, scrollLeft: number}) => void",
      "version": "2.9.4"
    }
  ],
  "exposes": [
    {
      "name": "focus",
      "title": "使选择器的输入框获取焦点",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "blur",
