{
  "__id__": "transfer",
  "name": "穿梭框",
  "desc": "",
  "tag": "el-transfer",
  "tagIcon": "transfer",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/transfer.html",
  "attributes": {
    "vModel": "modelValue",
    "data": {
      "label": "数据源",
      "title": "Transfer 的数据源",
      "types": [
        {
          "name": "array",
          "tag": "object-array-editor",
          "props": {
            "keys": [
              {
                "key": "label",
                "label": "名称"
              },
              {
                "key": "key",
                "label": "键值"
              },
              {
                "key": "disabled",
                "label": "禁用",
                "tag": "ElSwitch"
              }
            ]
          }
        }
      ],
      "default": [],
      "keep": true
    },
    "filterable": {
      "label": "可搜索",
      "title": "是否可搜索",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "filter-placeholder": {
      "label": "搜索框占位符",
      "title": "搜索框占位符",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "switch": [
        "filterable",
        true
      ],
      "default": "Enter keyword"
    },
    "filter-method": {
      "label": "搜索方法",
      "title": "自定义搜索方法",
      "types": [
        {
          "name": "function",
          "details": "(query: string, item: Record<string, any>) => boolean"
        }
      ],
      "hide": true
    },
    "target-order": {
      "label": "右侧元素排序策略",
      "title": "右侧列表元素的排序策略： 若为 <code>original</code>，则保持与数据源相同的顺序； 若为 <code>push</code>，则新加入的元素排在最后； 若为 <code>unshift</code>，则新加入的元素排在最前",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "相同",
              "value": "original"
            },
            {
              "label": "最后",
              "value": "push"
            },
            {
              "label": "最前",
              "value": "unshift"
            }
          ]
        }
      ],
      "default": "original",
      "keep": true
    },
    "titles": {
      "label": "列表标题",
      "title": "自定义列表标题",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "default": [
        "List 1",
        "List 2"
      ],
      "keep": true
    },
    "button-texts": {
      "label": "按钮文字",
      "title": "自定义按钮文案",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "default": [],
      "keep": true
    },
    "render-content": {
      "label": "数据项渲染函数",
      "title": "自定义数据项渲染函数",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "format": {
      "label": "列表顶部勾选状态文案",
      "title": "列表顶部勾选状态文案",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {}
    },
    "props": {
      "label": "数据源的字段别名",
      "title": "数据源的字段别名",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "left-default-checked": {
      "label": "初始状态下左侧列表的已勾选项的 key 数组",
      "title": "初始状态下左侧列表的已勾选项的 key 数组",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "right-default-checked": {
      "label": "初始状态下右侧列表的已勾选项的 key 数组",
      "title": "初始状态下右侧列表的已勾选项的 key 数组",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "validate-event": {
      "label": "触发表单验证",
      "title": "是否触发表单验证",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义数据项的内容， 参数为 <code>{ option }</code>"
    },
    {
      "name": "left-footer",
      "title": "左侧列表底部的内容"
    },
    {
      "name": "right-footer",
      "title": "右侧列表底部的内容"
    },
    {
      "name": "left-empty",
      "title": "左侧面板为空或没有数据符合筛选条件时的内容",
      "version": "2.9.0"
    },
    {
      "name": "right-empty",
      "title": "右侧面板为空或没有数据符合筛选条件时的内容",
      "version": "2.9.0"
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "右侧列表元素变化时触发",
      "type": "function",
      "details": "(value: TransferKey[], direction: TransferDirection, movedKeys: TransferKey[]) => void"
    },
    {
      "name": "left-check-change",
      "title": "左侧列表元素被用户选中 / 取消选中时触发",
      "type": "function",
      "details": "(value: TransferKey[], movedKeys?: TransferKey[]) => void"
    },
    {
      "name": "right-check-change",
      "title": "右侧列表元素被用户选中 / 取消选中时触发",
      "type": "function",
      "details": "(value: TransferKey[], movedKeys?: TransferKey[]) => void"
    }
  ],
  "exposes": [
    {
      "name": "clearQuery",
      "title": "清空某个面板的搜索关键词",
      "type": "function",
      "details": "(which: TransferDirection) => void"
    },
    {
      "name": "leftPanel",
      "title": "左侧面板 ref",
      "type": "object",
      "details": "Ref<TransferPanelInstance>"
    },
    {
      "name": "rightPanel",
      "title": "右侧面板 ref",
      "type": "object",
      "details": "Ref<TransferPanelInstance>"
    }
  ]
}