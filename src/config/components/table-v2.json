        {
          "type": "Function",
          "details": "(scrollTop: number) => void"
        }
      ]
    },
    {
      "name": "scrollToRow",
      "title": "使用给定的滚动策略滚动至指定行",
      "params": [
        {
          "type": "Function",
          "details": "(row: number, strategy?: 'center' | 'end' | 'start' | 'smart') => void"
        }
      ]
    }
  ]
