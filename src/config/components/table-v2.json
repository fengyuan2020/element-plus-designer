      "name": "footer",
      "title": ""
    },
    {
      "name": "empty",
      "title": ""
    },
    {
      "name": "overlay",
      "title": ""
    }
  ],
  "events": [
    {
      "name": "column-sort",
      "title": "列排序时调用",
      "params": [
        "<code>object</code>",
        "<",
        "<a href=\"#typings\">ColumnSortParam</a>",
        ">"
      ]
    },
    {
      "name": "expanded-rows-change",
      "title": "行展开状态改变时触发",
      "params": [
        "<a href=\"#typings\">KeyType[]</a>"
      ]
    },
    {
      "name": "end-reached",
      "title": "到达表格末尾时触发。 回调包含剩余距离，它通常是滚动条高度。",
      "params": [
        {
          "type": "Function",
          "details": "(remainDistance: number) => void"
        }
      ]
    },
    {
      "name": "scroll",
      "title": "表格被用户滚动后触发",
      "params": [
        "<code>object</code>",
        "<",
        "<a href=\"#typings\">ScrollParams</a>",
        ">"
      ]
    },
    {
      "name": "rows-rendered",
      "title": "当行被渲染后触发",
      "params": [
        "<code>object</code>",
        "<",
        "<a href=\"#typings\">RowsRenderedParams</a>",
        ">"
      ]
    },
    {
      "name": "row-expand",
      "title": "点击箭头图标展开/折叠树节点时触发",
      "params": [
        "<code>object</code>",
        "<",
        "<a href=\"#typings\">RowExpandParams</a>",
        ">"
      ]
    }
  ],
