{
  "__id__": "table-v2",
  "name": "虚拟表格",
  "desc": "在前端开发领域，表格一直都是一个高频出现的组件，尤其是在中后台和数据分析场景。 但是，对于 [Table V1](./table.md)来说，当一屏里超过 1000 条数据记录时，就会出现卡顿等性能问题，体验不是很好。",
  "tag": "el-table-v2",
  "tagIcon": "table",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/table-v2.html",
  "attributes": {
    "cache": {
      "label": "预加载的行数",
      "title": "为了更好的渲染效果预先多加载的行数",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 2
    },
    "estimated-row-height": {
      "label": "预估高度",
      "title": "渲染动态的单元格的预估高度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "header-class": {
      "label": "header 类名",
      "title": "header 部分的自定义 class 名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "keep": true
    },
    "header-props": {
      "label": "header props 名",
      "title": "header 部分的自定义 props 名",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "keep": true
    },
    "header-cell-props": {
      "label": "header cell 部分的自定义 props 名",
      "title": "header cell 部分的自定义 props 名",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "keep": true
    },
    "header-height": {
      "label": "Header 的高度由height设置。 如果传入数组",
      "title": "Header 的高度由<code>height</code>设置。 如果传入数组，它会使 header row 等于数组长度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "default": 50,
      "keep": true
    },
    "footer-height": {
      "label": "Footer 部分的高度",
      "title": "Footer 部分的高度，当传入值时，这部分将被计算入 table 的高度里",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    },
    "row-class": {
      "label": "row wrapper 部分的自定义 class 名",
      "title": "row wrapper 部分的自定义 class 名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "keep": true
    },
    "row-key": {
      "label": "每行的 key 值",
      "title": "每行的 key 值，如果不提供，将使用索引 index 代替",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "id",
      "keep": true
    },
    "row-props": {
      "label": "row component 部分的自定义 class 名",
      "title": "row component 部分的自定义 class 名",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "keep": true
    },
    "row-height": {
      "label": "每行的高度",
      "title": "每行的高度, 用于计算表的总高度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 50
    },
    "row-event-handlers": {
      "label": "当每行添加了一系列事件处理器时触发",
      "title": "当每行添加了一系列事件处理器时触发",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "keep": true
    },
    "cell-props": {
      "label": "每个单元格 cell 的自定义 props ",
      "title": "每个单元格 cell 的自定义 props (除了 header cell 以外)",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "keep": true
    },
    "columns": {
      "label": "列 column 的配置数组",
      "title": "列 column 的配置数组",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "keep": true
    },
    "data": {
      "label": "要在表中渲染的数据数组",
      "title": "要在表中渲染的数据数组",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "default": [],
      "keep": true
    },
    "data-getter": {
      "label": "一个自定义方法从数据源获取数据",
      "title": "一个自定义方法从数据源获取数据",
      "types": [
        {
          "name": "Function<"
        },
        {
          "name": "DataGetter"
        },
        {
          "name": ">"
        }
      ]
    },
    "fixed-data": {
      "label": "渲染行在表格主内容上方和 header 下方区域的数据",
      "title": "渲染行在表格主内容上方和 header 下方区域的数据",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        },
        {
          "name": "<"
        },
        {
          "name": "Data"
        },
        {
          "name": ">"
        }
      ]
    },
    "expand-column-key": {
      "label": "列的 key 来标记哪个行可以被展开",
      "title": "列的 key 来标记哪个行可以被展开",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "expanded-row-keys": {
      "label": "存放行展开状态的 key 的数组",
      "title": "存放行展开状态的 key 的数组，可以和 <code>v-model</code> 搭配使用",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "keep": true
    },
    "default-expanded-row-keys": {
      "label": "默认展开的行的 key 的数组",
      "title": "默认展开的行的 key 的数组, [object Object]",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "keep": true
    },
    "class": {
      "label": "表格的类名称",
      "title": "表格的类名称，将应用于表格的全部的三个部分 (左、右、主)",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "array",
          "tag": "array-editor"
        },
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "keep": true
    },
    "fixed": {
      "label": "自适应还是固定",
      "title": "单元格宽度是自适应还是固定",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "width": {
      "label": "宽度",
      "title": "表格的宽度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "required": true
    },
    "height": {
      "label": "高度",
      "title": "表格的高度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "required": true
    },
    "max-height": {
      "label": "最大高度",
      "title": "表格的最大高度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "indent-size": {
      "label": "树形表水平缩进",
      "title": "树形表的水平缩进",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 12
    },
    "h-scrollbar-size": {
      "label": "水平滚动条大小",
      "title": "配置表格的水平滚动条大小，防止水平和垂直滚动条重叠。",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 6
    },
    "v-scrollbar-size": {
      "label": "垂直滚动条大小",
      "title": "配置表格的垂直滚动条大小，防止水平和垂直滚动条重叠。",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 6
    },
    "scrollbar-always-on": {
      "label": "滚动条一直显示",
      "title": "如果开启，滚动条将一直显示，反之只会在鼠标经过时显示。",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "sort-by": {
      "label": "排序方式",
      "title": "排序方式",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": {},
      "keep": true
    },
    "sort-state": {
      "label": "多个排序",
      "title": "多个排序",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "keep": true
    }
  },
  "slots": [
    {
      "name": "cell",
      "title": "",
      "params": "<code>object</code><<a href=\"#typings\">CellSlotProps</a>>"
    },
    {
      "name": "header",
      "title": "",
      "params": "<code>object</code><<a href=\"#typings\">HeaderSlotProps</a>>"
    },
    {
      "name": "header-cell",
      "title": "",
      "params": "<code>object</code><<a href=\"#typings\">HeaderCellSlotProps</a>>"
    },
    {
      "name": "row",
      "title": "",
      "params": "<code>object</code><<a href=\"#typings\">RowSlotProps</a>>"
