{
  "__id__": "input-number",
  "name": "计数器",
  "desc": "仅允许输入标准的数字值，可定义范围",
  "tag": "el-input-number",
  "tagIcon": "number",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/input-number.html",
  "attributes": {
    "vModel": "modelValue",
    "placeholder": "请输入",
    "min": {
      "label": "最小值",
      "title": "设置计数器允许的最小值",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": "Number.MIN_SAFE_INTEGER"
    },
    "max": {
      "label": "最大值",
      "title": "设置计数器允许的最大值",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": "Number.MAX_SAFE_INTEGER"
    },
    "step": {
      "label": "步长",
      "title": "计数器步长",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 1
    },
    "step-strictly": {
      "label": "严格步数",
      "title": "是否只能输入 step 的倍数",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "precision": {
      "label": "精度",
      "title": "数值精度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number",
          "props": {
            "min": 0
          }
        }
      ]
    },
    "size": {
      "label": "精度",
      "title": "计数器尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default"
    },
    "readonly": {
      "label": "只读",
      "title": "原生 <code>readonly</code> 属性，是否只读",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "version": "2.2.16"
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用状态",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "controls": {
      "label": "使用控制按钮",
      "title": "是否使用控制按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "controls-position": {
      "label": "按钮位置",
      "title": "控制按钮位置",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "默认",
              "value": ""
            },
            {
              "label": "右边",
              "value": "right"
            }
          ]
        }
      ]
    },
    "name": {
      "label": "",
      "title": "等价于原生 input name 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "hide": true
    },
    "label": {
      "label": "",
      "title": "输入框关联的 label 文字",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "hide": true,
      "deprecated": true
    },
    "aria-label": {
      "label": "等价于原生 input aria-label 属性",
      "title": "等价于原生 input <code>aria-label</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "a11y": true,
      "version": "2.7.2"
    },
    "id": {
      "label": "等价于原生 input id 属性",
      "title": "等价于原生 input <code>id</code> 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "value-on-clear": {
      "label": "清空时显示的值",
      "title": "当输入框被清空时显示的值 (> 2.2.0)",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "version": "2.2.0"
    },
    "validate-event": {
      "label": "触发表单校验",
      "title": "是否触发表单验证",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "inputmode": {
      "label": "等价于原生 input inputmode 属性",
      "title": "等价于原生 input <code>inputmode</code> 属性",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "未设置",
              "value": null
            },
            {
              "label": "最小值",
              "value": "min"
            },
            {
              "label": "最大值",
              "value": "max"
            }
          ]
        },
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "version": "2.10.3"
    },
    "align": {
      "label": "内部输入文本对齐",
      "title": "内部输入文本对齐",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "left",
              "value": "left"
            },
            {
              "label": "center",
              "value": "center"
            },
            {
              "label": "right",
              "value": "right"
            }
          ]
        }
      ],
      "default": "center",
      "version": "2.10.5"
    },
    "disabled-scientific": {
      "label": "禁用科学计数法的输入",
      "title": "禁用科学计数法的输入（例如输入 'e'）",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "version": "2.10.5"
    }
  },
  "slots": [
    {
      "name": "decrease-icon",
      "title": "自定义输入框按钮减少图标",
      "version": "2.6.3"
    },
    {
      "name": "increase-icon",
      "title": "自定义输入框按钮增加图标",
      "version": "2.6.3"
    },
    {
      "name": "prefix",
      "title": "输入框头部内容",
      "version": "2.8.4"
    },
    {
      "name": "suffix",
      "title": "输入框尾部内容",
      "version": "2.8.4"
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "绑定值被改变时触发",
      "type": "function",
      "details": "(currentValue: number | undefined, oldValue: number | undefined) => void"
    },
    {
      "name": "blur",
      "title": "在组件 Input 失去焦点时触发",
      "type": "function",
      "details": "(event: FocusEvent) => void"
    },
    {
      "name": "focus",
      "title": "在组件 Input 获得焦点时触发",
      "type": "function",
      "details": "(event: FocusEvent) => void"
    }
  ],
  "exposes": [
    {
      "name": "focus",
      "title": "使 input 组件获得焦点",
      "type": "function",
      "details": "() => void"
    },
    {
      "name": "blur",
      "title": "使 input 组件失去焦点",
      "type": "function",
      "details": "() => void"
    }
  ]
}