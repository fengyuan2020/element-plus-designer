{
  "__id__": "dropdown-item",
  "name": "下拉折叠",
  "desc": "将动作或菜单折叠到下拉菜单中。",
  "tag": "el-dropdown-item",
  "tagIcon": "dropdown",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/dropdown.html",
  "attributes": {
    "command": {
      "label": "派发到command回调函数的指令参数",
      "title": "派发到<code>command</code>回调函数的指令参数",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "divided": {
      "label": "显示分隔符",
      "title": "是否显示分隔符",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "icon": {
      "label": "自定义图标",
      "title": "自定义图标",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义Dropdown-Item内容"
    }
  ]
}