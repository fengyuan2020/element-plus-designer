{
  "__id__": "splitter-panel",
  "name": "",
  "tag": "el-splitter-panel",
  "tagIcon": "splitter-panel",
  "layouts": [
    "containerItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/splitter.html",
  "attributes": {
    "size": {
      "label": "面板大小",
      "title": "面板大小(像素或百分比)",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "min": {
      "label": "最小尺寸",
      "title": "面板最小尺寸(像素或百分比)",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "max": {
      "label": "最大尺寸",
      "title": "面板的最大尺寸(像素或百分比)",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "resizable": {
      "label": "可调整",
      "title": "是否可以调整面板大小",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "collapsible": {
      "label": "可折叠",
      "title": "面板是否可折叠",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "面板的默认内容"
    },
    {
      "name": "start-collapsible",
      "title": "自定义起始折叠按钮的内容"
    },
    {
      "name": "end-collapsible",
      "title": "结束可折叠按钮的自定义内容"
    }
  ],
  "events": [
    {
      "name": "update:size",
      "title": "当面板大小改变时触发",
      "type": "function",
      "details": "(size: number) => void"
    }
  ]
}