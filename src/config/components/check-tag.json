{
  "__id__": "check-tag",
  "name": "可选中标签",
  "desc": "用于标记和选择。",
  "tag": "el-check-tag",
  "tagIcon": "check-tag",
  "layouts": [
    "rawItem"
  ],
  "document": "https://element-plus.org/zh-CN/component/tag.html",
  "attributes": {
    "checked": {
      "label": "",
      "title": "是否选中",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "disabled": {
      "label": "禁用",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false,
      "version": "2.8.2"
    },
    "type": {
      "label": "CheckTag 类型",
      "title": "CheckTag 类型",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "primary",
              "value": "primary"
            },
            {
              "label": "success",
              "value": "success"
            },
            {
              "label": "info",
              "value": "info"
            },
            {
              "label": "warning",
              "value": "warning"
            },
            {
              "label": "danger",
              "value": "danger"
            }
          ]
        }
      ],
      "default": "primary",
      "version": "2.5.4"
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "自定义默认内容"
    }
  ],
  "events": [
    {
      "name": "change",
      "title": "点击 Check Tag 时触发的事件",
      "type": "function",
      "details": "(value: boolean) => void"
    }
  ]
}