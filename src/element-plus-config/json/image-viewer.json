{
  "__id__": "image-viewer",
  "name": "图片",
  "tag": "el-image-viewer",
  "tagIcon": "image",
  "layout": "rawItem",
  "document": "https://element-plus.gitee.io/zh-CN/component/image.html",
  "attributes": {
    "url-list": {
      "label": "",
      "title": "用于预览的图片链接列表",
      "types": [
        {
          "name": "array",
          "tag": "array-editor"
        }
      ],
      "default": []
    },
    "z-index": {
      "label": "",
      "title": "预览时遮罩层的 z-index",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "initial-index": {
      "label": "",
      "title": "初始预览图像索引，小于 `url-list` 的长度",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    },
    "infinite": {
      "label": "",
      "title": "是否可以无限循环预览",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "hide-on-click-modal": {
      "label": "",
      "title": "是否可以通过点击遮罩层关闭预览",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "teleported": {
      "label": "",
      "title": "image 自身是否插入至 body 元素上。 嵌套的父元素属性会发生修改时应该将此属性设置为 `true`",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "zoom-rate": {
      "label": "",
      "title": "图像查看器缩放事件的缩放速率。",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 1.2
    },
    "close-on-press-escape": {
      "label": "",
      "title": "是否可以通过按下 ESC 关闭 Image Viewer",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    }
  },
  "events": [
    {
      "name": "close",
      "title": "当点击 X 按钮或者在` hide-on-click-modal `为 true 时点击遮罩层时触发",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "switch",
      "title": "切换图像时触发。",
      "type": "Function",
      "fnDes": "(index: number) => void"
    }
  ],
  "exposes": [
    {
      "name": "setActiveItem",
      "title": "手动切换图片",
      "type": "Function",
      "fnDes": "(index: number) => void"
    }
  ],
  "slots": []
}