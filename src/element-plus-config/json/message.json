{
  "__id__": "message",
  "attributes": {
    "message": {
      "label": "",
      "title": "消息文字",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "VNode",
          "tag": ""
        },
        {
          "name": "function",
          "tag": "",
          "desc": "() => VNode"
        }
      ],
      "default": ""
    },
    "type": {
      "label": "",
      "title": "消息类型",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "success",
              "value": "success"
            },
            {
              "label": "warning",
              "value": "warning"
            },
            {
              "label": "info",
              "value": "info"
            },
            {
              "label": "error",
              "value": "error"
            }
          ]
        }
      ],
      "default": "info"
    },
    "icon": {
      "label": "",
      "title": "自定义图标，该属性会覆盖 `type` 的图标。",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "dangerouslyUseHTMLString": {
      "label": "",
      "title": "是否将 message 属性作为 HTML 片段处理",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "customClass": {
      "label": "",
      "title": "自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": ""
    },
    "duration": {
      "label": "",
      "title": "显示时间，单位为毫秒。 设为 0 则不会自动关闭",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 3000
    },
    "showClose": {
      "label": "",
      "title": "是否显示关闭按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "center": {
      "label": "",
      "title": "文字是否居中",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "onClose": {
      "label": "",
      "title": "关闭时的回调函数, 参数为被关闭的 message 实例",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "() => void"
        }
      ]
    },
    "offset": {
      "label": "",
      "title": "Message 距离窗口顶部的偏移量",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 16
    },
    "appendTo": {
      "label": "",
      "title": "设置 message 的根元素，默认为 `document.body`",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "HTMLElement",
          "tag": ""
        }
      ]
    },
    "grouping": {
      "label": "",
      "title": "合并内容相同的消息，不支持 VNode 类型的消息",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "repeatNum": {
      "label": "",
      "title": "重复次数，类似于 Badge 。当和 `grouping` 属性一起使用时作为初始数量使用",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 1
    }
  },
  "exposes": [],
  "slots": []
}