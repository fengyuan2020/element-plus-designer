{
  "__id__": "checkbox",
  "name": "多选框",
  "tag": "el-checkbox",
  "tagIcon": "checkbox",
  "layout": "formItem",
  "document": "https://element-plus.gitee.io/zh-CN/component/checkbox.html",
  "attributes": {
    "vModel": "modelValue",
    "label": {
      "label": "选中状态的值",
      "title": "选中状态的值（只有在`checkbox-group`或者绑定对象类型为`array`时有效）",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "number",
          "tag": "el-input-number"
        },
        {
          "name": "boolean",
          "tag": "el-switch"
        },
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "true-label": {
      "label": "",
      "title": "选中时的值",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "false-label": {
      "label": "",
      "title": "没有选中时的值",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "disabled": {
      "label": "",
      "title": "是否禁用",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "border": {
      "label": "",
      "title": "是否显示边框",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "size": {
      "label": "",
      "title": "Checkbox 的尺寸",
      "types": [
        {
          "name": "enum",
          "tag": "el-radio-group",
          "config": {
            "optionType": "button"
          },
          "options": [
            {
              "label": "large",
              "value": "large"
            },
            {
              "label": "default",
              "value": "default"
            },
            {
              "label": "small",
              "value": "small"
            }
          ]
        }
      ],
      "default": "default"
    },
    "name": {
      "label": "",
      "title": "原生 name 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "checked": {
      "label": "",
      "title": "当前是否勾选",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "indeterminate": {
      "label": "",
      "title": "设置不确定状态，仅负责样式控制",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "validate-event": {
      "label": "",
      "title": "输入时是否触发表单的校验",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "tabindex": {
      "label": "",
      "title": "输入框的 tabindex",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ]
    },
    "id": {
      "label": "",
      "title": "input id",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "controls": {
      "label": "",
      "title": "与 [aria-control](https://developer.mozilla.org/en-US/docs/Web/Accessibility/ARIA/Attributes/aria-controls)一致, 当 `indeterminate`为 `true`时生效",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ]
    }
  },
  "events": [
    {
      "name": "change",
      "title": "当绑定值变化时触发的事件",
      "type": "Function",
      "fnDes": "(value: string | number | boolean) => void"
    }
  ],
  "slots": [
    {
      "name": "default",
      "title": "自定义默认内容"
    }
  ]
}