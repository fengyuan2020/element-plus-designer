{
  "__id__": "dialog",
  "name": "对话框",
  "tag": "el-dialog",
  "tagIcon": "dialog",
  "layouts": [
    "containerItem"
  ],
  "document": "https://element-plus.gitee.io/zh-CN/component/dialog.html",
  "attributes": {
    "vModel": "modelValue",
    "title": {
      "label": "标题",
      "title": "Dialog 对话框 Dialog 的标题， 也可通过具名 slot （见下表）传入",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": ""
    },
    "width": {
      "label": "宽度",
      "title": "对话框的宽度，默认值为 50%",
      "types": [
        {
          "name": "stringNumber",
          "tag": "string-number-input"
        }
      ],
      "default": ""
    },
    "fullscreen": {
      "label": "全屏",
      "title": "是否为全屏 Dialog",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "top": {
      "label": "margin-top",
      "title": "dialog CSS 中的 margin-top 值，默认为 15vh",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "15vh"
    },
    "modal": {
      "label": "遮罩层",
      "title": "是否需要遮罩层",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "modal-class": {
      "label": "遮罩类名",
      "title": "遮罩的自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ]
    },
    "append-to-body": {
      "label": "插入至 body 元素上",
      "title": "Dialog 自身是否插入至 body 元素上。 嵌套的 Dialog 必须指定该属性并赋值为 `true`",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "append-to": {
      "title": "Dialog 挂载到哪个 DOM 元素 将覆盖 `append-to-body`",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": "body",
      "version": "2.4.3"
    },
    "lock-scroll": {
      "label": "Dialog 出现 body 滚动锁定",
      "title": "是否在 Dialog 出现时将 body 滚动锁定",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "custom-class": {
      "label": "自定义类名",
      "title": "Dialog 的自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "deprecated": true,
      "default": ""
    },
    "open-delay": {
      "label": "打开延时",
      "title": "dialog 打开的延时时间，单位毫秒",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    },
    "close-delay": {
      "label": "关闭延时",
      "title": "drawer 关闭的延时时间，单位毫秒",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    },
    "close-on-click-modal": {
      "label": "点击modal关闭Dialog",
      "title": "是否可以通过点击 modal 关闭 Dialog",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "close-on-press-escape": {
      "label": "ESC 关闭 Dialog",
      "title": "是否可以通过按下 ESC 关闭 Dialog",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "show-close": {
      "label": "显示关闭按钮",
      "title": "是否显示关闭按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "before-close": {
      "label": "关闭前回调",
      "title": "关闭前的回调，会暂停 Dialog 的关闭. 回调函数内执行 done 参数方法的时候才是真正关闭对话框的时候.",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "(done: DoneFn) => void"
        }
      ]
    },
    "draggable": {
      "label": "可拖拽",
      "title": "为 Dialog 启用可拖拽功能",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "center": {
      "label": "header 和 footer 居中",
      "title": "是否让 Dialog 的 header 和 footer 部分居中排列",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "align-center": {
      "label": "水平垂直对齐",
      "title": "是否水平垂直对齐对话框",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "destroy-on-close": {
      "label": "关闭销毁元素",
      "title": "当关闭 Dialog 时，销毁其中的元素",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "close-icon": {
      "label": "关闭图标",
      "title": "自定义关闭图标，默认 Close",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": "Close"
    },
    "z-index": {
      "label": "z-index",
      "title": "和原生的 CSS 的 z-index 相同，改变 z 轴的顺序",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ]
    },
    "header-aria-level": {
      "title": "header 的 `aria-level` 属性",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": 2
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "Dialog 的内容"
    },
    {
      "name": "header",
      "title": "对话框标题的内容；会替换标题部分，但不会移除关闭按钮。"
    },
    {
      "name": "title",
      "title": "与 header 作用相同 请使用 header",
      "deprecated": true
    },
    {
      "name": "footer",
      "title": "Dialog 按钮操作区的内容"
    }
  ],
  "events": [
    {
      "name": "open",
      "title": "Dialog 打开的回调",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "opened",
      "title": "Dialog 打开动画结束时的回调",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "close",
      "title": "Dialog 关闭的回调",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "closed",
      "title": "Dialog 关闭动画结束时的回调",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "open-auto-focus",
      "title": "输入焦点聚焦在 Dialog 内容时的回调",
      "type": "Function",
      "fnDes": "() => void"
    },
    {
      "name": "close-auto-focus",
      "title": "输入焦点从 Dialog 内容失焦时的回调",
      "type": "Function",
      "fnDes": "() => void"
    }
  ]
}