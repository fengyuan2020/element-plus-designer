{
  "__id__": "skeleton",
  "name": "骨架",
  "tag": "el-skeleton",
  "tagIcon": "skeleton",
  "layout": "rawItem",
  "document": "https://element-plus.gitee.io/zh-CN/component/skeleton.html",
  "attributes": {
    "animated": {
      "label": "使用动画",
      "title": "是否使用动画",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "count": {
      "label": "渲染多少个 template",
      "title": "渲染多少个 template, 建议使用尽可能小的数字",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 1
    },
    "loading": {
      "label": "DOM 结构",
      "title": "是否显示加载结束后的 DOM 结构",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "rows": {
      "label": "段落数量",
      "title": "骨架屏段落数量",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 3
    },
    "throttle": {
      "label": "渲染延迟",
      "title": "渲染延迟（以毫秒为单位）",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "真正渲染的DOM",
      "params": "^[object]`$attrs`"
    },
    {
      "name": "template",
      "title": "渲染 skeleton 模板的内容",
      "params": "^[object]`{ key: number }`"
    }
  ]
}