{
  "attributes": {
    "target": {
      "title": "获取引导卡片指向的元素， 为空时居中于屏幕。 自^(2.5.2)以来支持字符串和函数类型。 字符串类型是文档.querySelector的选择器。",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "() => HTMLElemen"
        }
      ]
    },
    "show-arrow": {
      "title": "是否显示箭头",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "title": {
      "title": "标题",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": "*"
    },
    "description": {
      "title": "主要描述部分",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ]
    },
    "placement": {
      "title": "引导卡片相对于目标元素的位置",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "top",
              "value": "top"
            },
            {
              "label": "top-start",
              "value": "top-start"
            },
            {
              "label": "top-end",
              "value": "top-end"
            },
            {
              "label": "bottom",
              "value": "bottom"
            },
            {
              "label": "bottom-start",
              "value": "bottom-start"
            },
            {
              "label": "bottom-end",
              "value": "bottom-end"
            },
            {
              "label": "left",
              "value": "left"
            },
            {
              "label": "left-start",
              "value": "left-start"
            },
            {
              "label": "left-end",
              "value": "left-end"
            },
            {
              "label": "right",
              "value": "right"
            },
            {
              "label": "right-start",
              "value": "right-start"
            },
            {
              "label": "right-end",
              "value": "right-end"
            }
          ]
        }
      ],
      "default": "bottom"
    },
    "content-style": {
      "title": "为content自定义样式",
      "types": [
        {
          "name": "CSSProperties",
          "tag": "style-editor"
        }
      ],
      "default": "*"
    },
    "mask": {
      "title": "是否启用蒙层，也可传入配置改变蒙层样式和填充色",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": true
    },
    "type": {
      "title": "类型，影响底色与文字颜色",
      "types": [
        {
          "name": "default`",
          "tag": ""
        },
        {
          "name": "`primary",
          "tag": ""
        }
      ],
      "default": "default"
    },
    "next-button-props": {
      "title": "“下一步”按钮的属性",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ]
    },
    "prev-button-props": {
      "title": "“上一步”按钮的属性",
      "types": [
        {
          "name": "object",
          "tag": "object-editor"
        }
      ],
      "default": "*"
    },
    "scroll-into-view-options": {
      "title": "是否支持当前元素滚动到视窗内，也可传入配置指定滚动视窗的相关参数，默认跟随 Tour 的 `scrollIntoViewOptions` 属性",
      "types": [
        {
          "name": "boolean`",
          "tag": ""
        },
        {
          "name": "`ScrollIntoViewOptions",
          "tag": ""
        }
      ]
    },
    "show-close": {
      "title": "是否显示关闭按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "close-icon": {
      "title": "自定义关闭图标，默认Close",
      "types": [
        {
          "name": "string`",
          "tag": ""
        },
        {
          "name": "`Component",
          "tag": ""
        }
      ],
      "default": "*"
    }
  },
  "slots": [
    {
      "name": "default",
      "title": "主要描述部分"
    },
    {
      "name": "header",
      "title": "头部"
    }
  ],
  "events": [
    {
      "name": "close",
      "title": "关闭引导时的回调函数",
      "params": "^[Function]`() => void`"
    }
  ]
}