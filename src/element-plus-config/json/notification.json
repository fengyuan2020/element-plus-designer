{
  "__id__": "notification",
  "attributes": {
    "title": {
      "label": "",
      "title": "标题",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": ""
    },
    "message": {
      "label": "",
      "title": "通知栏正文内容",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "VNode",
          "tag": ""
        }
      ],
      "default": ""
    },
    "dangerouslyUseHTMLString": {
      "label": "",
      "title": "是否将 message 属性作为 HTML 片段处理",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "type": {
      "label": "",
      "title": "通知的类型",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "success",
              "value": "success"
            },
            {
              "label": "warning",
              "value": "warning"
            },
            {
              "label": "info",
              "value": "info"
            },
            {
              "label": "error",
              "value": "error"
            }
          ]
        }
      ],
      "default": ""
    },
    "icon": {
      "label": "",
      "title": "自定义图标。 若设置了 `type`，则 `icon` 会被覆盖",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ]
    },
    "customClass": {
      "title": "自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input"
        }
      ],
      "default": ""
    },
    "custom-class": {
      "label": "",
      "title": "自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": ""
    },
    "duration": {
      "label": "",
      "title": "显示时间, 单位为毫秒。 值为 0 则不会自动关闭",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 4500
    },
    "position": {
      "label": "",
      "title": "自定义弹出位置",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "top-right",
              "value": "top-right"
            },
            {
              "label": "top-left",
              "value": "top-left"
            },
            {
              "label": "bottom-right",
              "value": "bottom-right"
            },
            {
              "label": "bottom-left",
              "value": "bottom-left"
            }
          ]
        }
      ],
      "default": "top-right"
    },
    "showClose": {
      "title": "是否显示关闭按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "onClose": {
      "title": "关闭时的回调函数",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "() => void"
        }
      ]
    },
    "onClick": {
      "title": "点击 Notification 时的回调函数",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "() => void"
        }
      ]
    },
    "show-close": {
      "label": "",
      "title": "是否显示关闭按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "on-close": {
      "label": "",
      "title": "关闭时的回调函数",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "() => void"
        }
      ]
    },
    "on-click": {
      "label": "",
      "title": "点击 Notification 时的回调函数",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "() => void"
        }
      ]
    },
    "offset": {
      "label": "",
      "title": "相对屏幕顶部的偏移量 偏移的距离，在同一时刻，所有的 Notification 实例应当具有一个相同的偏移量",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    },
    "appendTo": {
      "label": "",
      "title": "设置 notification 的根元素，默认为 `document.body`",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "HTMLElement",
          "tag": ""
        }
      ]
    },
    "zIndex": {
      "label": "",
      "title": "初始 zIndex",
      "types": [
        {
          "name": "number",
          "tag": "el-input-number"
        }
      ],
      "default": 0
    }
  },
  "slots": [],
  "exposes": []
}