{
  "__id__": "message-box",
  "attributes": {
    "autofocus": {
      "label": "",
      "title": "打开 MessageBox 时是否自动获得焦点",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "title": {
      "label": "",
      "title": "MessageBox 的标题",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": ""
    },
    "message": {
      "label": "",
      "title": "MessageBox 的正文内容",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "VNode",
          "tag": ""
        },
        {
          "name": "function",
          "tag": "",
          "desc": "() => VNode"
        }
      ]
    },
    "dangerouslyUseHTMLString": {
      "label": "",
      "title": "是否将 `message` 作为 HTML 片段处理",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "type": {
      "label": "",
      "title": "消息类型，用于图标显示",
      "types": [
        {
          "name": "enum",
          "tag": "el-select",
          "options": [
            {
              "label": "success",
              "value": "success"
            },
            {
              "label": "info",
              "value": "info"
            },
            {
              "label": "warning",
              "value": "warning"
            },
            {
              "label": "error",
              "value": "error"
            }
          ]
        }
      ],
      "default": ""
    },
    "icon": {
      "label": "",
      "title": "自定义图标组件，会覆盖 `type` 的类型",
      "types": [
        {
          "name": "string",
          "tag": "icon-chooser"
        }
      ],
      "default": ""
    },
    "custom-class": {
      "label": "",
      "title": "MessageBox 的自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": ""
    },
    "custom-style": {
      "label": "",
      "title": "MessageBox 的自定义内联样式",
      "types": [
        {
          "name": "object",
          "tag": "style-editor"
        }
      ],
      "default": {}
    },
    "callback": {
      "label": "",
      "title": "若不使用 Promise，可以使用此参数指定 MessageBox 关闭后的回调",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "(value: string, action: Action) => any | (action: Action) => any"
        }
      ],
      "default": null
    },
    "show-close": {
      "label": "",
      "title": "MessageBox 是否显示右上角关闭按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "before-close": {
      "label": "",
      "title": "messageBox 关闭前的回调，会暂停消息弹出框的关闭过程。",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "(action: Action, instance: MessageBoxState, done: () => void) => void"
        }
      ],
      "default": null
    },
    "distinguish-cancel-and-close": {
      "label": "",
      "title": "是否将取消（点击取消按钮）与关闭（点击关闭按钮或遮罩层、按下 Esc 键）进行区分",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "lock-scroll": {
      "label": "",
      "title": "是否在 MessageBox 出现时将 body 滚动锁定",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "show-cancel-button": {
      "label": "",
      "title": "是否显示取消按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": "false（以 confirm 和 prompt 方式调用时为 true）"
    },
    "show-confirm-button": {
      "label": "",
      "title": "是否显示确定按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "cancel-button-text": {
      "label": "",
      "title": "取消按钮的文本内容",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "取消"
    },
    "confirm-button-text": {
      "label": "",
      "title": "确定按钮的文本内容",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "确定"
    },
    "cancel-button-class": {
      "label": "",
      "title": "取消按钮的自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": ""
    },
    "confirm-button-class": {
      "label": "",
      "title": "确定按钮的自定义类名",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": ""
    },
    "close-on-click-modal": {
      "label": "",
      "title": "是否可通过点击遮罩层关闭 MessageBox",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": "true（以 alert 方式调用时为 false）"
    },
    "close-on-press-escape": {
      "label": "",
      "title": "是否可通过按下 ESC 键关闭 MessageBox",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": "true（以 alert 方式调用时为 false）"
    },
    "close-on-hash-change": {
      "label": "",
      "title": "是否在 hash 改变时关闭 MessageBox",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": true
    },
    "show-input": {
      "label": "",
      "title": "是否显示输入框",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": "false（以 prompt 方式调用时为 true）"
    },
    "input-placeholder": {
      "label": "",
      "title": "输入框占位文本",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": ""
    },
    "input-type": {
      "label": "",
      "title": "输入框的类型",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "text"
    },
    "input-value": {
      "label": "",
      "title": "输入框的初始文本",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": null
    },
    "input-pattern": {
      "label": "",
      "title": "输入框的校验表达式",
      "types": [
        {
          "name": "regexp",
          "tag": ""
        }
      ],
      "default": null
    },
    "input-validator": {
      "label": "",
      "title": "输入框的校验函数。 应该返回一个 boolean 或者 string， 如果返回的是一个 string 类型，那么该返回值会被赋值给 inputErrorMessage 用于向用户展示错误消息。",
      "types": [
        {
          "name": "function",
          "tag": "",
          "desc": "(value: string) => boolea | string"
        }
      ],
      "default": null
    },
    "input-error-message": {
      "label": "",
      "title": "校验未通过时的提示文本",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "输入的数据不合法!"
    },
    "center": {
      "label": "",
      "title": "是否居中布局",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "draggable": {
      "label": "",
      "title": "MessageBox 是否可拖放",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "round-button": {
      "label": "",
      "title": "是否使用圆角按钮",
      "types": [
        {
          "name": "boolean",
          "tag": "el-switch"
        }
      ],
      "default": false
    },
    "button-size": {
      "label": "",
      "title": "自定义确认按钮及取消按钮的大小",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        }
      ],
      "default": "default"
    },
    "append-to": {
      "label": "",
      "title": "设置组件的根元素",
      "types": [
        {
          "name": "string",
          "tag": "el-input",
          "props": {
            "clearable": true
          }
        },
        {
          "name": "HTMLElement",
          "tag": ""
        }
      ]
    }
  },
  "slots": []
}